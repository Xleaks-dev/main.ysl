--//  COMPLETE AUTO-FARM SYSTEM
--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local AutoFarm = {}
AutoFarm.FarmRoute = {}
AutoFarm.CurrentTarget = nil

function AutoFarm:FindBrainrots()
    local brainrots = {}
    local folder = Workspace:FindFirstChild("Brainrots") or Workspace
    
    for _, item in pairs(folder:GetChildren()) do
        if item:IsA("Model") or item:IsA("Part") then
            local name = item.Name:lower()
            
            -- Rarity filter
            if _G.YslPremium.TargetRarity ~= "All" then
                if not name:find(_G.YslPremium.TargetRarity:lower()) then
                    continue
                end
            end
            
            -- Mutation filter
            if _G.YslPremium.OnlyMutations then
                local hasMutation = item:FindFirstChildOfClass("ParticleEmitter") or 
                                   item:FindFirstChildOfClass("PointLight")
                if not hasMutation then continue end
            end
            
            local part = item:IsA("Part") and item or item.PrimaryPart or item:FindFirstChildWhichIsA("BasePart")
            
            if part then
                local dist = (part.Position - rootPart.Position).Magnitude
                if dist < _G.YslPremium.MinDistance then
                    table.insert(brainrots, {item = item, part = part, distance = dist})
                end
            end
        end
    end
    
    -- Sort by distance
    table.sort(brainrots, function(a, b) return a.distance < b.distance end)
    
    return brainrots
end

function AutoFarm:CollectBrainrot(brainrotData)
    SafeExecute(function()
        local part = brainrotData.part
        
        -- Teleport to player
        part.CFrame = rootPart.CFrame
        part.CanCollide = false
        part.Anchored = false
        
        -- Force collection
        if part.Touched then
            firetouchinterest(rootPart, part, 0)
            task.wait(0.01)
            firetouchinterest(rootPart, part, 1)
        end
        
        _G.YslPremium.Stats.Brainrots = _G.YslPremium.Stats.Brainrots + 1
    end)
end

function AutoFarm:StartBrainrotFarm()
    if Connections.BrainrotFarm then
        Connections.BrainrotFarm:Disconnect()
    end
    
    if not _G.YslPremium.AutoCollectBrainrots then return end
    
    Connections.BrainrotFarm = RunService.Heartbeat:Connect(function()
        SafeExecute(function()
            local brainrots = self:FindBrainrots()
            
            for _, brainrotData in ipairs(brainrots) do
                self:CollectBrainrot(brainrotData)
            end
        end)
        task.wait(0.1)
    end)
end

function AutoFarm:StartMoneyFarm()
    if Connections.MoneyFarm then
        Connections.MoneyFarm:Disconnect()
    end
    
    if not _G.YslPremium.AutoCollectMoney then return end
    
    Connections.MoneyFarm = RunService.Heartbeat:Connect(function()
        SafeExecute(function()
            for _, item in pairs(Workspace:GetDescendants()) do
                if item:IsA("BasePart") and item.Name:lower():find("money") then
                    item.CFrame = rootPart.CFrame
                    _G.YslPremium.Stats.Money = _G.YslPremium.Stats.Money + 1
                end
            end
        end)
        task.wait(0.1)
    end)
end

function AutoFarm:StartUpgrades()
    -- Speed Upgrade
    if _G.YslPremium.AutoUpgradeSpeed then
        task.spawn(function()
            while _G.YslPremium.AutoUpgradeSpeed do
                SafeExecute(function()
                    local speedShop = Workspace:FindFirstChild("SpeedShop") or 
                                     Workspace:FindFirstChild("Upgrades") and Workspace.Upgrades:FindFirstChild("Speed")
                    
                    if speedShop then
                        for _, v in pairs(speedShop:GetDescendants()) do
                            if v:IsA("ClickDetector") then
                                fireclickdetector(v)
                            end
                        end
                    end
                end)
                task.wait(1)
            end
        end)
    end
    
    -- Carry Upgrade
    if _G.YslPremium.AutoUpgradeCarry then
        task.spawn(function()
            while _G.YslPremium.AutoUpgradeCarry do
                SafeExecute(function()
                    local carryShop = Workspace:FindFirstChild("CarryShop") or 
                                     Workspace:FindFirstChild("Upgrades") and Workspace.Upgrades:FindFirstChild("Carry")
                    
                    if carryShop then
                        for _, v in pairs(carryShop:GetDescendants()) do
                            if v:IsA("ClickDetector") then
                                fireclickdetector(v)
                            end
                        end
                    end
                end)
                task.wait(1)
            end
        end)
    end
end

function AutoFarm:StartAutoRebirth()
    if not _G.YslPremium.AutoRebirth then return end
    
    task.spawn(function()
        while _G.YslPremium.AutoRebirth do
            SafeExecute(function()
                local speedStat = lplayer:FindFirstChild("leaderstats") and 
                                 lplayer.leaderstats:FindFirstChild("Speed")
                
                if speedStat and speedStat.Value >= _G.YslPremium.RebirthAt then
                    -- Find rebirth button
                    local rebirthRemote = ReplicatedStorage:FindFirstChild("RebirthEvent") or
                                         ReplicatedStorage:FindFirstChild("Rebirth")
                    
                    if rebirthRemote and rebirthRemote:IsA("RemoteEvent") then
                        rebirthRemote:FireServer()
                        _G.YslPremium.Stats.Rebirths = _G.YslPremium.Stats.Rebirths + 1
                        Notify("ğŸ”„ Auto Rebirth", "Rebirthed at " .. speedStat.Value .. " speed!", 3)
                        task.wait(10)
                    end
                end
            end)
            task.wait(5)
        end
    end)
end

function AutoFarm:StartRadioactiveEvent()
    if not _G.YslPremium.AutoRadioactive then return end
    
    -- Auto Radioactive Coins
    if _G.YslPremium.AutoRadCoins then
        task.spawn(function()
            while _G.YslPremium.AutoRadCoins do
                SafeExecute(function()
                    for _, item in pairs(Workspace:GetDescendants()) do
                        if item:IsA("BasePart") then
                            local light = item:FindFirstChild("PointLight")
                            if light and light.Color == Color3.fromRGB(0, 255, 0) then
                                item.CFrame = rootPart.CFrame
                                _G.YslPremium.Stats.RadCoins = _G.YslPremium.Stats.RadCoins + 1
                            end
                        end
                    end
                end)
                task.wait(0.05)
            end
        end)
    end
    
    -- Auto Bombs
    if _G.YslPremium.AutoBombs then
        task.spawn(function()
            while _G.YslPremium.AutoBombs do
                SafeExecute(function()
                    for _, bomb in pairs(Workspace:GetDescendants()) do
                        if bomb:IsA("Model") and bomb.Name:lower():find("bomb") then
                            local part = bomb.PrimaryPart or bomb:FindFirstChildWhichIsA("BasePart")
                            if part then
                                part.CFrame = rootPart.CFrame + Vector3.new(0, 5, 0)
                            end
                        end
                    end
                end)
                task.wait(0.1)
            end
        end)
    end
    
    -- Auto Spin
    if _G.YslPremium.AutoSpin then
        task.spawn(function()
            while _G.YslPremium.AutoSpin do
                SafeExecute(function()
                    local wheel = Workspace:FindFirstChild("SpinWheel") or 
                                 Workspace:FindFirstChild("RadioactiveSpin")
                    
                    if wheel then
                        for _, v in pairs(wheel:GetDescendants()) do
                            if v:IsA("ClickDetector") then
                                fireclickdetector(v)
                            end
                        end
                    end
                end)
                task.wait(2)
            end
        end)
    end
end

function AutoFarm:StartMasterFarm()
    if not _G.YslPremium.AutoFarm then return end
    
    -- Enable all farming features
    _G.YslPremium.AutoCollectBrainrots = true
    _G.YslPremium.AutoCollectMoney = true
    _G.YslPremium.AutoUpgradeSpeed = true
    _G.YslPremium.AutoUpgradeCarry = true
    _G.YslPremium.AutoRebirth = true
    _G.YslPremium.AutoRadioactive = true
    _G.YslPremium.AutoRadCoins = true
    _G.YslPremium.AutoBombs = true
    
    self:StartBrainrotFarm()
    self:StartMoneyFarm()
    self:StartUpgrades()
    self:StartAutoRebirth()
    self:StartRadioactiveEvent()
    
    Notify("ğŸ¤– Master Farm", "All auto-farm systems activated! ğŸ’°", 4)
end

