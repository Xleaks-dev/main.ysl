    return loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
end)

if not success then
    game.Players.LocalPlayer:Kick("\nâš¡ YslMenu V112\n\nUI Library failed to load!\nRejoin and try again.")
    return
end

