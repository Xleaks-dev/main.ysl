    CreateButton(MiscTab, "TP to Spawn", function() TeleportTo("Spawn") end)
    CreateButton(MiscTab, "TP to Sell Point", function() TeleportTo("SellPoint") end)
    
    CreateSection(MiscTab, "Quick Actions")
    CreateButton(MiscTab, "Claim Free Epic", function()
        pcall(function()
            TakeFreeEpic_RF:InvokeServer()
            Notify("Free Epic", "Claimed!", 2)
        end)
    end)
    CreateButton(MiscTab, "Rejoin Server", function()
        game:GetService("TeleportService"):Teleport(game.PlaceId, lplayer)
    end)
    
    -- Select first tab
    Tabs[1].Button.MouseButton1Click:Fire()
    
    -- Entrance Animation
    MainWindow.Size = UDim2.new(0, 0, 0, 0)
    MainWindow.Position = UDim2.new(0.5, 0, 0.5, 0)
    
    CreateTween(MainWindow, {
        Size = UDim2.new(0, 700, 0, 550),
        Position = UDim2.new(0.5, -350, 0.5, -275)
    }, 1, Enum.EasingStyle.Elastic)
    
