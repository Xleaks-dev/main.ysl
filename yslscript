task.spawn(function()
    while task.wait(5) do
        if _G.Ysl.AutoUpgrade.Speed and _G.Ysl.Enabled then
            pcall(function()
                local currentSpeed = lplayer:GetAttribute("CurrentSpeed") or 16
                if currentSpeed <= _G.Ysl.UpgradeSpeedAt then
                    UpgradeSpeed_RF:InvokeServer()
                end
            end)
        end
    end
end)

