    -- TAB 4: MOVEMENT
    local MovementTab = CreateTab("Movement", "ğŸƒ")
    
    CreateSection(MovementTab, "Speed")
    CreateToggle(MovementTab, "Speed Boost", function(v) _G.Ysl.Movement.Speed = v end)
    CreateSlider(MovementTab, "Speed Value", 16, 500, 100, function(v) _G.Ysl.Movement.SpeedValue = v end)
    
