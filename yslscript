            pcall(function()
                UpgradeCarry_RF:InvokeServer()
            end)
        end
    end
end)

-- Auto Upgrade All Brainrots
task.spawn(function()
    while task.wait(5) do
        if _G.Ysl.AutoUpgrade.Brainrots and _G.Ysl.Enabled then
            pcall(function()
                local bases = Workspace:FindFirstChild("Bases")
                if bases then
                    for _, base in pairs(bases:GetChildren()) do
                        for _, slot in pairs(base:GetDescendants()) do
                            if slot.Name:match("slot") and slot:IsA("Model") then
                                -- Upgrade the brainrot in this slot
                                UpgradeBrainrot_RF:InvokeServer(slot)
                            end
                        end
                    end
                end
            end)
        end
    end
end)

--[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    AUTO REBIRTH (SMART MODE)
â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]

task.spawn(function()
    while task.wait(5) do
        if _G.Ysl.AutoRebirth.Enabled and _G.Ysl.Enabled then
            pcall(function()
                local currentTime = os.time()
                local timeSinceLastRebirth = currentTime - _G.Ysl.AutoRebirth.LastRebirth
                
                -- 10 second cooldown
                if timeSinceLastRebirth >= 10 then
                    local currentSpeed = lplayer:GetAttribute("CurrentSpeed") or 16
                    
                    if currentSpeed >= _G.Ysl.RebirthAt then
                        local success = Rebirth_RF:InvokeServer()
                        if success then
                            _G.Ysl.AutoRebirth.LastRebirth = currentTime
                            Notify("Auto Rebirth", "Rebirthed at Speed: " .. currentSpeed, 3)
                        end
                    end
                end
            end)
        end
    end
end)

--[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    MOVEMENT FUNCTIONS
â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]

-- Speed
local speedConnection
task.spawn(function()
    while task.wait() do
        if _G.Ysl.Movement.Speed and _G.Ysl.Enabled then
            pcall(function()
                if humanoid then
                    humanoid.WalkSpeed = _G.Ysl.Movement.SpeedValue
                end
            end)
        else
            if humanoid then
                humanoid.WalkSpeed = 16
            end
        end
    end
end)

-- Fly
local flying = false
local flyConnection

local function startFly()
    flying = true
    local bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
    bodyVelocity.Velocity = Vector3.new(0, 0, 0)
    bodyVelocity.Parent = hrp
    
    local bodyGyro = Instance.new("BodyGyro")
    bodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
    bodyGyro.P = 9e4
    bodyGyro.Parent = hrp
    
    flyConnection = RunService.RenderStepped:Connect(function()
        if not _G.Ysl.Movement.Fly or not _G.Ysl.Enabled then
            stopFly()
            return
        end
        
        local camera = workspace.CurrentCamera
        local moveDirection = Vector3.new()
        
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then
            moveDirection = moveDirection + camera.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then
            moveDirection = moveDirection - camera.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then
            moveDirection = moveDirection - camera.CFrame.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then
            moveDirection = moveDirection + camera.CFrame.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
            moveDirection = moveDirection + Vector3.new(0, 1, 0)
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
            moveDirection = moveDirection - Vector3.new(0, 1, 0)
        end
        
        bodyVelocity.Velocity = moveDirection.Unit * _G.Ysl.Movement.FlySpeed
        bodyGyro.CFrame = camera.CFrame
    end)
end

function stopFly()
    flying = false
    if flyConnection then
        flyConnection:Disconnect()
        flyConnection = nil
    end
    if hrp then
        for _, v in pairs(hrp:GetChildren()) do
            if v:IsA("BodyVelocity") or v:IsA("BodyGyro") then
                v:Destroy()
            end
        end
    end
end

-- NoClip
local noclipConnection
task.spawn(function()
    while task.wait(0.1) do
        if _G.Ysl.Movement.NoClip and _G.Ysl.Enabled then
            pcall(function()
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end)
        end
    end
end)

-- Infinite Jump
local infJumpConnection
UserInputService.JumpRequest:Connect(function()
    if _G.Ysl.Movement.InfJump and _G.Ysl.Enabled and humanoid then
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

--[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    PROTECTION FUNCTIONS
â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]

-- GodMode
task.spawn(function()
    while task.wait() do
        if _G.Ysl.Protection.GodMode and _G.Ysl.Enabled then
            pcall(function()
                if humanoid and humanoid.Health < humanoid.MaxHealth then
                    humanoid.Health = humanoid.MaxHealth
                end
            end)
        end
    end
end)

-- Anti-Void (Teleport back if falling)
task.spawn(function()
    while task.wait(0.5) do
        if _G.Ysl.Protection.AntiVoid and _G.Ysl.Enabled then
            pcall(function()
                if hrp.Position.Y < -50 then
                    -- Find a safe spawn
                    local bases = Workspace:FindFirstChild("Bases")
                    if bases then
                        for _, base in pairs(bases:GetChildren()) do
                            local spawn = base:FindFirstChild("Spawn")
                            if spawn and spawn:IsA("BasePart") then
                                hrp.CFrame = spawn.CFrame + Vector3.new(0, 5, 0)
                                Notify("Anti-Void", "Teleported to safety!", 2)
                                break
                            end
                        end
                    end
                end
            end)
        end
    end
end)

-- Wave Immunity (Make waves non-collidable)
local waveImmunityConnection
task.spawn(function()
    while task.wait(0.1) do
        if _G.Ysl.Protection.WaveImmunity and _G.Ysl.Enabled then
            pcall(function()
                -- Heal if damaged
                if humanoid and humanoid.Health < humanoid.MaxHealth then
                    humanoid.Health = humanoid.MaxHealth
                    _G.Ysl.Stats.WavesBlocked = _G.Ysl.Stats.WavesBlocked + 1
                end
                
                -- Disable harmful states
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming, false)
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
                
                -- Make waves non-collidable
                local activeTsunamis = Workspace:FindFirstChild("ActiveTsunamis")
                if activeTsunamis then
                    for _, tsunami in pairs(activeTsunamis:GetChildren()) do
                        if tsunami:IsA("Model") then
                            for _, part in pairs(tsunami:GetDescendants()) do
                                if part:IsA("BasePart") then
                                    local dist = (part.Position - hrp.Position).Magnitude
                                    if dist <= 50 then
                                        part.CanCollide = false
                                        part.CanTouch = false
                                    end
                                end
                            end
                        end
                    end
                end
                
                -- Also handle Lava
                local lava = Workspace:FindFirstChild("Lava")
                if lava then
                    for _, part in pairs(lava:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                            part.CanTouch = false
                        end
                    end
                end
            end)
        end
    end
end)

-- Remove Waves Function
local function RemoveWaves()
    pcall(function()
        local activeTsunamis = Workspace:FindFirstChild("ActiveTsunamis")
        if activeTsunamis then
            activeTsunamis:ClearAllChildren()
            Notify("Wave Delete", "All waves removed!", 2)
        end
    end)
end

--[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    VIP BYPASS FUNCTIONS
â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]

-- Remove VIP Walls
local function RemoveVIPWalls()
    pcall(function()
        local vipWalls = Workspace:FindFirstChild("VIPWalls")
        if vipWalls then
            for _, wall in pairs(vipWalls:GetChildren()) do
                if wall:IsA("BasePart") then
                    wall:Destroy()
                end
            end
            Notify("VIP Bypass", "All VIP walls removed!", 3)
        end
    end)
end

-- Teleport Past VIP Walls
local function TeleportPastVIP()
    pcall(function()
        local vipWalls = Workspace:FindFirstChild("VIPWalls")
        if vipWalls then
            local furthestWall = nil
            local furthestDist = 0
            
            for _, wall in pairs(vipWalls:GetChildren()) do
                if wall:IsA("BasePart") then
                    local dist = (wall.Position - Vector3.new(0, 0, 0)).Magnitude
                    if dist > furthestDist then
                        furthestDist = dist
                        furthestWall = wall
                    end
                end
            end
            
            if furthestWall then
                hrp.CFrame = furthestWall.CFrame + furthestWall.CFrame.LookVector * 10 + Vector3.new(0, 5, 0)
                Notify("VIP Bypass", "Teleported past VIP area!", 2)
            end
        end
    end)
end

--[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    ESP FUNCTIONS
â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]

local ESPObjects = {}

local function CreateESP(obj, color, text)
    if ESPObjects[obj] then return end
    
    local billboard = Instance.new("BillboardGui")
    billboard.Name = "ESP"
    billboard.AlwaysOnTop = true
    billboard.Size = UDim2.new(0, 100, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.Parent = obj
    
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, 0, 1, 0)
    frame.BackgroundTransparency = 0.7
    frame.BackgroundColor3 = color
    frame.Parent = billboard
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = frame
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = Color3.new(1, 1, 1)
    label.TextScaled = true
    label.Font = Enum.Font.GothamBold
    label.Parent = frame
    
    ESPObjects[obj] = billboard
end

local function RemoveESP(obj)
    if ESPObjects[obj] then
        ESPObjects[obj]:Destroy()
        ESPObjects[obj] = nil
    end
end

-- Brainrot ESP
task.spawn(function()
    while task.wait(2) do
        if _G.Ysl.ESP.Brainrots and _G.Ysl.Enabled then
            pcall(function()
                local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
                if activeBrainrots then
                    for _, rarityFolder in pairs(activeBrainrots:GetChildren()) do
                        local color = Color3.fromRGB(255, 255, 255)
                        if rarityFolder.Name == "Legendary" then
                            color = Color3.fromRGB(255, 170, 0)
                        elseif rarityFolder.Name == "Epic" then
                            color = Color3.fromRGB(163, 53, 238)
                        elseif rarityFolder.Name == "Rare" then
                            color = Color3.fromRGB(0, 170, 255)
                        elseif rarityFolder.Name == "Cosmic" then
                            color = Color3.fromRGB(255, 0, 255)
                        elseif rarityFolder.Name == "Secret" then
                            color = Color3.fromRGB(255, 255, 0)
                        end
                        
                        for _, brainrotModel in pairs(rarityFolder:GetChildren()) do
                            if brainrotModel.Name == "RenderedBrainrot" then
                                CreateESP(brainrotModel, color, rarityFolder.Name)
                            end
                        end
                    end
                end
            end)
        else
            -- Remove all brainrot ESP
            for obj, _ in pairs(ESPObjects) do
                if obj.Parent and obj.Parent.Parent == Workspace:FindFirstChild("ActiveBrainrots") then
                    RemoveESP(obj)
                end
            end
        end
    end
end)

-- Lucky Block ESP
task.spawn(function()
    while task.wait(2) do
        if _G.Ysl.ESP.LuckyBlocks and _G.Ysl.Enabled then
            pcall(function()
                local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
                if luckyBlocks then
                    for _, block in pairs(luckyBlocks:GetChildren()) do
                        CreateESP(block, Color3.fromRGB(255, 215, 0), "Lucky Block")
                    end
                end
            end)
        else
            -- Remove lucky block ESP
            for obj, _ in pairs(ESPObjects) do
                if obj.Parent == Workspace:FindFirstChild("ActiveLuckyBlocks") then
                    RemoveESP(obj)
                end
            end
        end
    end
end)

-- VIP Wall ESP
task.spawn(function()
    while task.wait(2) do
        if _G.Ysl.ESP.VIPWalls and _G.Ysl.Enabled then
            pcall(function()
                local vipWalls = Workspace:FindFirstChild("VIPWalls")
                if vipWalls then
                    for _, wall in pairs(vipWalls:GetChildren()) do
                        if wall:IsA("BasePart") then
                            CreateESP(wall, Color3.fromRGB(255, 0, 0), "VIP Wall")
                        end
                    end
                end
            end)
        else
            -- Remove VIP ESP
            for obj, _ in pairs(ESPObjects) do
                if obj.Parent == Workspace:FindFirstChild("VIPWalls") then
                    RemoveESP(obj)
                end
            end
        end
    end
end)

-- Player ESP
task.spawn(function()
    while task.wait(1) do
        if _G.Ysl.ESP.Players and _G.Ysl.Enabled then
            pcall(function()
                for _, player in pairs(Players:GetPlayers()) do
                    if player ~= lplayer and player.Character then
                        local char = player.Character
                        local head = char:FindFirstChild("Head")
                        if head then
                            CreateESP(head, Color3.fromRGB(0, 255, 0), player.Name)
                        end
                    end
                end
            end)
        else
            -- Remove player ESP
            for obj, _ in pairs(ESPObjects) do
                if obj.Parent and obj.Parent.Parent == Workspace then
                    for _, player in pairs(Players:GetPlayers()) do
                        if player.Character == obj.Parent then
                            RemoveESP(obj)
                        end
                    end
                end
            end
        end
    end
end)

--[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    TELEPORT FUNCTIONS
â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]

local teleportLocations = {
    ["Spawn"] = function()
        local bases = Workspace:FindFirstChild("Bases")
        if bases then
            for _, base in pairs(bases:GetChildren()) do
                local spawn = base:FindFirstChild("Spawn")
                if spawn then
                    return spawn.CFrame + Vector3.new(0, 5, 0)
                end
            end
        end
    end,
    
    ["Sell Point"] = function()
        local sellPoint = Workspace:FindFirstChild("SellPoint")
        if sellPoint and sellPoint:FindFirstChild("Seller") then
            return sellPoint.Seller:GetPivot() + Vector3.new(0, 5, 0)
        end
    end,
    
    ["Upgrade Shop"] = function()
        local upgradeShop = Workspace:FindFirstChild("UpgradeShop")
        if upgradeShop then
            return upgradeShop:GetPivot() + Vector3.new(0, 5, 0)
        end
    end,
    
    ["Money Wheel"] = function()
        local wheel = Workspace:FindFirstChild("MoneySpinWheel")
        if wheel then
            return wheel:GetPivot() + Vector3.new(0, 5, 0)
        end
    end,
    
    ["Nearest Brainrot"] = function()
        local nearest = nil
        local nearestDist = math.huge
        
        local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
        if activeBrainrots then
            for _, rarityFolder in pairs(activeBrainrots:GetChildren()) do
                for _, brainrot in pairs(rarityFolder:GetDescendants()) do
                    if brainrot.Name == "RenderedBrainrot" then
                        local dist = (brainrot:GetPivot().Position - hrp.Position).Magnitude
                        if dist < nearestDist then
                            nearestDist = dist
                            nearest = brainrot
                        end
                    end
                end
            end
        end
        
        if nearest then
            return nearest:GetPivot() + Vector3.new(0, 3, 0)
        end
    end,
    
    ["Nearest Lucky Block"] = function()
        local nearest = nil
        local nearestDist = math.huge
        
        local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
        if luckyBlocks then
            for _, block in pairs(luckyBlocks:GetChildren()) do
                local dist = (block:GetPivot().Position - hrp.Position).Magnitude
                if dist < nearestDist then
                    nearestDist = dist
                    nearest = block
                end
            end
        end
        
        if nearest then
            return nearest:GetPivot() + Vector3.new(0, 3, 0)
        end
    end
}

local function TeleportTo(locationName)
    pcall(function()
        local location = teleportLocations[locationName]
        if location then
            local cframe = location()
            if cframe then
                hrp.CFrame = cframe
                Notify("Teleport", "Teleported to " .. locationName, 2)
            end
        end
    end)
end

