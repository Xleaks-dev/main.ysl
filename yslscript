-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local CoreGui = game:GetService("CoreGui")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")

-- Player
local lplayer = Players.LocalPlayer
local mouse = lplayer:GetMouse()

-- Premium Key
local PREMIUM_KEY = "ysl-key-escapetsunami"
local KEY_VERIFIED = false

--[[ UTILITY FUNCTIONS ]]

local function CreateTween(object, properties, duration, style, direction)
    duration = duration or 0.3
    style = style or Enum.EasingStyle.Quad
    direction = direction or Enum.EasingDirection.Out
    
    local tween = TweenService:Create(object, TweenInfo.new(duration, style, direction), properties)
    tween:Play()
    return tween
end

local function MakeDraggable(frame, dragHandle)
    dragHandle = dragHandle or frame
    
    local dragging = false
    local dragInput
    local dragStart
    local startPos
    
    dragHandle.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = frame.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    dragHandle.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            local delta = input.Position - dragStart
            CreateTween(frame, {
                Position = UDim2.new(
                    startPos.X.Scale,
                    startPos.X.Offset + delta.X,
                    startPos.Y.Scale,
                    startPos.Y.Offset + delta.Y
                )
            }, 0.1, Enum.EasingStyle.Linear)
        end
    end)
end

--[[ KEY SYSTEM ]]

local function CreateKeySystem()
    local KeySystemUI = Instance.new("ScreenGui")
    KeySystemUI.Name = "YslKeySystem"
    KeySystemUI.Parent = CoreGui
    KeySystemUI.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    KeySystemUI.ResetOnSpawn = false
    
    local Blur = Instance.new("BlurEffect")
    Blur.Size = 0
    Blur.Parent = game.Lighting
    CreateTween(Blur, {Size = 30}, 0.5)
    
    local Overlay = Instance.new("Frame")
    Overlay.Size = UDim2.new(1, 0, 1, 0)
    Overlay.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    Overlay.BackgroundTransparency = 1
    Overlay.BorderSizePixel = 0
    Overlay.Parent = KeySystemUI
    CreateTween(Overlay, {BackgroundTransparency = 0.6}, 0.5)
    
    local MainContainer = Instance.new("Frame")
    MainContainer.Size = UDim2.new(0, 600, 0, 500)
    MainContainer.Position = UDim2.new(0.5, -300, 0.5, -250)
    MainContainer.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
    MainContainer.BackgroundTransparency = 0.05
    MainContainer.BorderSizePixel = 0
    MainContainer.ClipsDescendants = true
    MainContainer.Parent = KeySystemUI
    
    local MainCorner = Instance.new("UICorner")
    MainCorner.CornerRadius = UDim.new(0, 25)
    MainCorner.Parent = MainContainer
    
    local MainStroke = Instance.new("UIStroke")
    MainStroke.Color = Color3.fromRGB(255, 0, 127)
    MainStroke.Thickness = 3
    MainStroke.Parent = MainContainer
    
    task.spawn(function()
        local hue = 0
        while KeySystemUI.Parent do
            hue = (hue + 1) % 360
            MainStroke.Color = Color3.fromHSV(hue / 360, 1, 1)
            task.wait(0.03)
        end
    end)
    
    local TopBar = Instance.new("Frame")
    TopBar.Size = UDim2.new(1, 0, 0, 120)
    TopBar.BackgroundColor3 = Color3.fromRGB(255, 0, 127)
    TopBar.BorderSizePixel = 0
    TopBar.Parent = MainContainer
    
    local TopBarCorner = Instance.new("UICorner")
    TopBarCorner.CornerRadius = UDim.new(0, 25)
    TopBarCorner.Parent = TopBar
    
    local YSLLogo = Instance.new("TextLabel")
    YSLLogo.Size = UDim2.new(0, 300, 0, 60)
    YSLLogo.Position = UDim2.new(0.5, -150, 0, 30)
    YSLLogo.BackgroundTransparency = 1
    YSLLogo.Text = "yslscripts"
    YSLLogo.TextSize = 50
    YSLLogo.Font = Enum.Font.GothamBold
    YSLLogo.TextColor3 = Color3.fromRGB(255, 255, 255)
    YSLLogo.Parent = TopBar
    
    task.spawn(function()
        local hue = 0
        while KeySystemUI.Parent do
            hue = (hue + 2) % 360
            YSLLogo.TextColor3 = Color3.fromHSV(hue / 360, 1, 1)
            task.wait(0.03)
        end
    end)
    
    local ContentArea = Instance.new("Frame")
    ContentArea.Size = UDim2.new(1, -40, 1, -160)
    ContentArea.Position = UDim2.new(0, 20, 0, 140)
    ContentArea.BackgroundTransparency = 1
    ContentArea.Parent = MainContainer
    
    local InputContainer = Instance.new("Frame")
    InputContainer.Size = UDim2.new(1, 0, 0, 70)
    InputContainer.Position = UDim2.new(0, 0, 0, 90)
    InputContainer.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
    InputContainer.BorderSizePixel = 0
    InputContainer.Parent = ContentArea
    
    local InputCorner = Instance.new("UICorner")
    InputCorner.CornerRadius = UDim.new(0, 15)
    InputCorner.Parent = InputContainer
    
    local KeyBox = Instance.new("TextBox")
    KeyBox.Size = UDim2.new(1, -20, 1, -20)
    KeyBox.Position = UDim2.new(0, 10, 0, 10)
    KeyBox.BackgroundTransparency = 1
    KeyBox.PlaceholderText = "Enter Key..."
    KeyBox.PlaceholderColor3 = Color3.fromRGB(150, 150, 150)
    KeyBox.Text = ""
    KeyBox.TextSize = 22
    KeyBox.Font = Enum.Font.GothamBold
    KeyBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    KeyBox.Parent = InputContainer
    
    local SubmitButton = Instance.new("TextButton")
    SubmitButton.Size = UDim2.new(0, 250, 0, 65)
    SubmitButton.Position = UDim2.new(0.5, -125, 0, 180)
    SubmitButton.BackgroundColor3 = Color3.fromRGB(255, 0, 127)
    SubmitButton.BorderSizePixel = 0
    SubmitButton.Text = "VERIFY"
    SubmitButton.TextSize = 22
    SubmitButton.Font = Enum.Font.GothamBold
    SubmitButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    SubmitButton.Parent = ContentArea
    
    local SubmitCorner = Instance.new("UICorner")
    SubmitCorner.CornerRadius = UDim.new(0, 15)
    SubmitCorner.Parent = SubmitButton
    
    local StatusLabel = Instance.new("TextLabel")
    StatusLabel.Size = UDim2.new(1, 0, 0, 40)
    StatusLabel.Position = UDim2.new(0, 0, 0, 265)
    StatusLabel.BackgroundTransparency = 1
    StatusLabel.Text = ""
    StatusLabel.TextSize = 16
    StatusLabel.Font = Enum.Font.GothamBold
    StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    StatusLabel.Parent = ContentArea
    
    MainContainer.Size = UDim2.new(0, 0, 0, 0)
    MainContainer.Position = UDim2.new(0.5, 0, 0.5, 0)
    CreateTween(MainContainer, {Size = UDim2.new(0, 600, 0, 500), Position = UDim2.new(0.5, -300, 0.5, -250)}, 1, Enum.EasingStyle.Elastic)
    
    MakeDraggable(MainContainer, TopBar)
    
    local function VerifyKey()
        if KeyBox.Text == PREMIUM_KEY then
            StatusLabel.Text = "âœ… VERIFIED!"
            StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 127)
            task.wait(1)
            CreateTween(MainContainer, {Size = UDim2.new(0, 0, 0, 0), Position = UDim2.new(0.5, 0, 0.5, 0)}, 0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In)
            CreateTween(Overlay, {BackgroundTransparency = 1}, 0.5)
            CreateTween(Blur, {Size = 0}, 0.5)
            task.wait(0.6)
            KeySystemUI:Destroy()
            Blur:Destroy()
            KEY_VERIFIED = true
            loadMainScript()
        else
            StatusLabel.Text = "âŒ INVALID KEY!"
            StatusLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
        end
    end
    
    SubmitButton.MouseButton1Click:Connect(VerifyKey)
    KeyBox.FocusLost:Connect(function(enterPressed)
        if enterPressed then VerifyKey() end
    end)
end

CreateKeySystem()

--[[ MAIN SCRIPT ]]

function loadMainScript()
    
    local character = lplayer.Character or lplayer.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")
    local hrp = character:WaitForChild("HumanoidRootPart")
    
    -- Store player's base on spawn
    local MyBase = nil
    task.spawn(function()
        task.wait(2)
        local bases = Workspace:FindFirstChild("Bases")
        if bases then
            local closestBase = nil
            local closestDist = math.huge
            for _, base in pairs(bases:GetChildren()) do
                local spawn = base:FindFirstChild("Spawn")
                if spawn then
                    local dist = (spawn.Position - hrp.Position).Magnitude
                    if dist < closestDist then
                        closestDist = dist
                        closestBase = base
                    end
                end
            end
            MyBase = closestBase
            print("Found my base:", MyBase and MyBase.Name or "nil")
        end
    end)
    
    -- Remotes
    local RemoteFunctions = ReplicatedStorage:WaitForChild("RemoteFunctions")
    local RemoteEvents = ReplicatedStorage:WaitForChild("RemoteEvents")
    
    local Rebirth_RF = RemoteFunctions:WaitForChild("Rebirth")
    local UpgradeSpeed_RF = RemoteFunctions:WaitForChild("UpgradeSpeed")
    local UpgradeBrainrot_RF = RemoteFunctions:WaitForChild("UpgradeBrainrot")
    local UpgradeBase_RF = RemoteFunctions:WaitForChild("UpgradeBase")
    local SellAll_RF = RemoteFunctions:WaitForChild("SellAll")
    local UpgradeCarry_RF = RemoteFunctions:WaitForChild("UpgradeCarry")
    
    local CollectMoney_RE = RemoteEvents:WaitForChild("CollectMoney")
    
    -- Settings
    _G.Ysl = {
        Enabled = true,
        WalkSpeed = 16,
        WalkSpeedEnabled = false,
        RebirthAt = 500,
        AutoFarm = {
            Brainrot = false,
            Money = false,
            LuckyBlocks = false,
            SellAll = false
        },
        AutoUpgrade = {
            Base = false,
            Brainrots = false,
            Carry = false
        },
        AutoRebirth = false,
        Movement = {
            Fly = false,
            FlySpeed = 50,
            NoClip = false,
            InfJump = false
        },
        VIPBypass = false
    }
    
    local function Notify(text)
        game.StarterGui:SetCore("SendNotification", {
            Title = "yslscripts",
            Text = text,
            Duration = 3
        })
    end
    
    -- Anti-AFK
    for i,v in pairs(getconnections(lplayer.Idled)) do
        v:Disable()
    end
    
    -- Respawn handler
    lplayer.CharacterAdded:Connect(function(char)
        task.wait(0.5)
        character = char
        humanoid = char:WaitForChild("Humanoid")
        hrp = char:WaitForChild("HumanoidRootPart")
        
        -- Find base again
        task.wait(2)
        local bases = Workspace:FindFirstChild("Bases")
        if bases then
            local closestBase = nil
            local closestDist = math.huge
            for _, base in pairs(bases:GetChildren()) do
                local spawn = base:FindFirstChild("Spawn")
                if spawn then
                    local dist = (spawn.Position - hrp.Position).Magnitude
                    if dist < closestDist then
                        closestDist = dist
                        closestBase = base
                    end
                end
            end
            MyBase = closestBase
        end
        
        Notify("Respawned - Features Active")
    end)
    
    --[[ WALKSPEED CONTROL ]]
    
    local speedConnection = nil
    local function EnableWalkSpeed()
        if speedConnection then speedConnection:Disconnect() end
        
        speedConnection = RunService.Heartbeat:Connect(function()
            if _G.Ysl.WalkSpeedEnabled and humanoid and humanoid.Parent then
                humanoid.WalkSpeed = _G.Ysl.WalkSpeed
            end
        end)
    end
    
    local function DisableWalkSpeed()
        if speedConnection then
            speedConnection:Disconnect()
            speedConnection = nil
        end
        if humanoid then
            humanoid.WalkSpeed = 16
        end
    end
    
    --[[ AUTO FARM - FIXED ]]
    
    -- Auto Farm Brainrots
    task.spawn(function()
        while task.wait(0.15) do
            if _G.Ysl.AutoFarm.Brainrot and _G.Ysl.Enabled then
                pcall(function()
                    local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
                    if not activeBrainrots then return end
                    
                    local nearest = nil
                    local nearestDist = 500
                    
                    for _, folder in pairs(activeBrainrots:GetChildren()) do
                        for _, rendered in pairs(folder:GetChildren()) do
                            if rendered.Name == "RenderedBrainrot" then
                                for _, model in pairs(rendered:GetChildren()) do
                                    if model:IsA("Model") then
                                        for _, desc in pairs(model:GetDescendants()) do
                                            if desc.Name == "TakePrompt" and desc:IsA("ProximityPrompt") then
                                                local pos = model:GetPivot().Position
                                                local dist = (pos - hrp.Position).Magnitude
                                                if dist < nearestDist then
                                                    nearestDist = dist
                                                    nearest = {pos = pos, prompt = desc}
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                    
                    if nearest then
                        hrp.CFrame = CFrame.new(nearest.pos + Vector3.new(0, 2, 0))
                        task.wait(0.1)
                        fireproximityprompt(nearest.prompt)
                        task.wait(0.15)
                    end
                end)
            end
        end
    end)
    
    -- Auto Collect Money - ACTUALLY FIXED
    task.spawn(function()
        while task.wait(0.2) do
            if _G.Ysl.AutoFarm.Money and _G.Ysl.Enabled and MyBase then
                pcall(function()
                    local slots = MyBase:FindFirstChild("Slots")
                    if not slots then return end
                    
                    for _, slot in pairs(slots:GetChildren()) do
                        if not _G.Ysl.AutoFarm.Money then break end
                        
                        local collect = slot:FindFirstChild("Collect")
                        if collect and collect:IsA("BasePart") then
                            -- Teleport directly on top
                            hrp.CFrame = collect.CFrame * CFrame.new(0, 3, 0)
                            task.wait(0.1)
                            
                            -- Fire event
                            CollectMoney_RE:FireServer()
                            
                            -- Click detector
                            local detector = collect:FindFirstChildOfClass("ClickDetector")
                            if detector then
                                fireclickdetector(detector)
                            end
                            
                            task.wait(0.15)
                        end
                    end
                end)
            end
        end
    end)
    
    -- Auto Lucky Blocks
    task.spawn(function()
        while task.wait(0.2) do
            if _G.Ysl.AutoFarm.LuckyBlocks and _G.Ysl.Enabled then
                pcall(function()
                    local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
                    if not luckyBlocks then return end
                    
                    local nearest = nil
                    local nearestDist = 500
                    
                    for _, block in pairs(luckyBlocks:GetChildren()) do
                        if block:IsA("Model") then
                            local prompt = block:FindFirstChildWhichIsA("ProximityPrompt", true)
                            if prompt then
                                local pos = block:GetPivot().Position
                                local dist = (pos - hrp.Position).Magnitude
                                if dist < nearestDist then
                                    nearestDist = dist
                                    nearest = {pos = pos, prompt = prompt}
                                end
                            end
                        end
                    end
                    
                    if nearest then
                        hrp.CFrame = CFrame.new(nearest.pos + Vector3.new(0, 2, 0))
                        task.wait(0.1)
                        for i = 1, 5 do
                            fireproximityprompt(nearest.prompt)
                            task.wait(0.02)
                        end
                        task.wait(0.15)
                    end
                end)
            end
        end
    end)
    
    -- Auto Sell
    task.spawn(function()
        while task.wait(60) do
            if _G.Ysl.AutoFarm.SellAll and _G.Ysl.Enabled then
                pcall(function()
                    SellAll_RF:InvokeServer()
                    Notify("Auto Sold All!")
                end)
            end
        end
    end)
    
    -- Speed Upgrades
    local function BuySpeed(times)
        task.spawn(function()
            for i = 1, times do
                pcall(function() UpgradeSpeed_RF:InvokeServer() end)
                task.wait(0.1)
            end
            Notify("Upgraded Speed " .. times .. " times!")
        end)
    end
    
    -- Auto Upgrades
    task.spawn(function()
        while task.wait(15) do
            if _G.Ysl.AutoUpgrade.Base then
                pcall(function() UpgradeBase_RF:InvokeServer() end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(15) do
            if _G.Ysl.AutoUpgrade.Carry then
                pcall(function() UpgradeCarry_RF:InvokeServer() end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(12) do
            if _G.Ysl.AutoUpgrade.Brainrots and MyBase then
                pcall(function()
                    for _, desc in pairs(MyBase:GetDescendants()) do
                        if desc.Name:match("slot %d+ brainrot") and desc:FindFirstChild("Root") then
                            pcall(function() UpgradeBrainrot_RF:InvokeServer(desc) end)
                            task.wait(0.1)
                        end
                    end
                end)
            end
        end
    end)
    
    -- Auto Rebirth
    task.spawn(function()
        while task.wait(5) do
            if _G.Ysl.AutoRebirth and _G.Ysl.Enabled then
                pcall(function()
                    local speed = lplayer:GetAttribute("CurrentSpeed") or 16
                    if speed >= _G.Ysl.RebirthAt then
                        Rebirth_RF:InvokeServer()
                        Notify("Auto Rebirthed!")
                    end
                end)
            end
        end
    end)
    
    -- Fly
    local flying = false
    local flyBV, flyBG
    
    local function StartFly()
        flying = true
        
        flyBV = Instance.new("BodyVelocity")
        flyBV.Velocity = Vector3.zero
        flyBV.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        flyBV.Parent = hrp
        
        flyBG = Instance.new("BodyGyro")
        flyBG.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        flyBG.P = 9e4
        flyBG.Parent = hrp
        
        RunService.Heartbeat:Connect(function()
            if not _G.Ysl.Movement.Fly or not flying then
                if flyBV then flyBV:Destroy() end
                if flyBG then flyBG:Destroy() end
                flying = false
                return
            end
            
            if not hrp.Parent then return end
            
            local camera = workspace.CurrentCamera
            local dir = Vector3.zero
            
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then dir = dir + camera.CFrame.LookVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then dir = dir - camera.CFrame.LookVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then dir = dir - camera.CFrame.RightVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then dir = dir + camera.CFrame.RightVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then dir = dir + Vector3.new(0, 1, 0) end
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then dir = dir - Vector3.new(0, 1, 0) end
            
            flyBV.Velocity = dir.Magnitude > 0 and dir.Unit * _G.Ysl.Movement.FlySpeed or Vector3.zero
            flyBG.CFrame = camera.CFrame
        end)
    end
    
    -- NoClip
    RunService.Stepped:Connect(function()
        if _G.Ysl.Movement.NoClip and character then
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end)
    
    -- Inf Jump
    UserInputService.JumpRequest:Connect(function()
        if _G.Ysl.Movement.InfJump and humanoid then
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end)
    
    -- VIP Bypass
    task.spawn(function()
        while task.wait(1) do
            if _G.Ysl.VIPBypass then
                pcall(function()
                    local vipWalls = Workspace:FindFirstChild("VIPWalls")
                    if vipWalls then
                        for _, wall in pairs(vipWalls:GetChildren()) do
                            if wall:IsA("BasePart") then
                                wall.CanCollide = false
                                wall.Transparency = 0.8
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    --[[ UI ]]
    
    local YslUI = Instance.new("ScreenGui")
    YslUI.Name = "YslUI"
    YslUI.Parent = CoreGui
    YslUI.ResetOnSpawn = false
    
    local Main = Instance.new("Frame")
    Main.Size = UDim2.new(0, 550, 0, 650)
    Main.Position = UDim2.new(0.5, -275, 0.5, -325)
    Main.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
    Main.BorderSizePixel = 0
    Main.Parent = YslUI
    
    local MainCorner = Instance.new("UICorner")
    MainCorner.CornerRadius = UDim.new(0, 15)
    MainCorner.Parent = Main
    
    local MainStroke = Instance.new("UIStroke")
    MainStroke.Color = Color3.fromRGB(255, 0, 127)
    MainStroke.Thickness = 2
    MainStroke.Parent = Main
    
    task.spawn(function()
        local h = 0
        while YslUI.Parent do
            h = (h + 1) % 360
            MainStroke.Color = Color3.fromHSV(h / 360, 1, 1)
            task.wait(0.03)
        end
    end)
    
    -- Top
    local Top = Instance.new("Frame")
    Top.Size = UDim2.new(1, 0, 0, 60)
    Top.BackgroundColor3 = Color3.fromRGB(20, 20, 25)
    Top.BorderSizePixel = 0
    Top.Parent = Main
    
    local TopCorner = Instance.new("UICorner")
    TopCorner.CornerRadius = UDim.new(0, 15)
    TopCorner.Parent = Top
    
    local Title = Instance.new("TextLabel")
    Title.Size = UDim2.new(1, 0, 1, 0)
    Title.BackgroundTransparency = 1
    Title.Text = "yslscripts"
    Title.TextSize = 28
    Title.Font = Enum.Font.GothamBold
    Title.TextColor3 = Color3.fromRGB(255, 0, 127)
    Title.Parent = Top
    
    task.spawn(function()
        local h = 0
        while YslUI.Parent do
            h = (h + 2) % 360
            Title.TextColor3 = Color3.fromHSV(h / 360, 1, 1)
            task.wait(0.03)
        end
    end)
    
    MakeDraggable(Main, Top)
    
    -- Content
    local Content = Instance.new("ScrollingFrame")
    Content.Size = UDim2.new(1, -20, 1, -80)
    Content.Position = UDim2.new(0, 10, 0, 70)
    Content.BackgroundTransparency = 1
    Content.BorderSizePixel = 0
    Content.ScrollBarThickness = 6
    Content.ScrollBarImageColor3 = Color3.fromRGB(255, 0, 127)
    Content.CanvasSize = UDim2.new(0, 0, 0, 0)
    Content.AutomaticCanvasSize = Enum.AutomaticSize.Y
    Content.Parent = Main
    
    local Layout = Instance.new("UIListLayout")
    Layout.Padding = UDim.new(0, 8)
    Layout.SortOrder = Enum.SortOrder.LayoutOrder
    Layout.Parent = Content
    
    local function Section(name)
        local sec = Instance.new("TextLabel")
        sec.Size = UDim2.new(1, 0, 0, 30)
        sec.BackgroundTransparency = 1
        sec.Text = name
        sec.TextSize = 18
        sec.Font = Enum.Font.GothamBold
        sec.TextColor3 = Color3.fromRGB(255, 0, 127)
        sec.TextXAlignment = Enum.TextXAlignment.Left
        sec.Parent = Content
        return sec
    end
    
    local function Toggle(name, callback)
        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(1, 0, 0, 40)
        frame.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
        frame.BorderSizePixel = 0
        frame.Parent = Content
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 8)
        corner.Parent = frame
        
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, -60, 1, 0)
        label.Position = UDim2.new(0, 10, 0, 0)
        label.BackgroundTransparency = 1
        label.Text = name
        label.TextSize = 14
        label.Font = Enum.Font.Gotham
        label.TextColor3 = Color3.fromRGB(255, 255, 255)
        label.TextXAlignment = Enum.TextXAlignment.Left
        label.Parent = frame
        
        local button = Instance.new("TextButton")
        button.Size = UDim2.new(0, 45, 0, 25)
        button.Position = UDim2.new(1, -50, 0.5, -12.5)
        button.BackgroundColor3 = Color3.fromRGB(50, 50, 55)
        button.Text = ""
        button.Parent = frame
        
        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(1, 0)
        btnCorner.Parent = button
        
        local circle = Instance.new("Frame")
        circle.Size = UDim2.new(0, 21, 0, 21)
        circle.Position = UDim2.new(0, 2, 0, 2)
        circle.BackgroundColor3 = Color3.fromRGB(200, 200, 200)
        circle.BorderSizePixel = 0
        circle.Parent = button
        
        local circleCorner = Instance.new("UICorner")
        circleCorner.CornerRadius = UDim.new(1, 0)
        circleCorner.Parent = circle
        
        local on = false
        
        button.MouseButton1Click:Connect(function()
            on = not on
            CreateTween(button, {BackgroundColor3 = on and Color3.fromRGB(255, 0, 127) or Color3.fromRGB(50, 50, 55)}, 0.2)
            CreateTween(circle, {Position = on and UDim2.new(1, -23, 0, 2) or UDim2.new(0, 2, 0, 2)}, 0.2)
            callback(on)
        end)
        
        return frame
    end
    
    local function Button(name, callback)
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(1, 0, 0, 40)
        btn.BackgroundColor3 = Color3.fromRGB(255, 0, 127)
        btn.BorderSizePixel = 0
        btn.Text = name
        btn.TextSize = 15
        btn.Font = Enum.Font.GothamBold
        btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        btn.Parent = Content
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 8)
        corner.Parent = btn
        
        btn.MouseButton1Click:Connect(callback)
        return btn
    end
    
    local function Slider(name, min, max, default, callback)
        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(1, 0, 0, 60)
        frame.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
        frame.BorderSizePixel = 0
        frame.Parent = Content
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 8)
        corner.Parent = frame
        
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, -60, 0, 25)
        label.Position = UDim2.new(0, 10, 0, 5)
        label.BackgroundTransparency = 1
        label.Text = name
        label.TextSize = 14
        label.Font = Enum.Font.Gotham
        label.TextColor3 = Color3.fromRGB(255, 255, 255)
        label.TextXAlignment = Enum.TextXAlignment.Left
        label.Parent = frame
        
        local value = Instance.new("TextLabel")
        value.Size = UDim2.new(0, 50, 0, 25)
        value.Position = UDim2.new(1, -55, 0, 5)
        value.BackgroundTransparency = 1
        value.Text = tostring(default)
        value.TextSize = 14
        value.Font = Enum.Font.GothamBold
        value.TextColor3 = Color3.fromRGB(255, 0, 127)
        value.TextXAlignment = Enum.TextXAlignment.Right
        value.Parent = frame
        
        local bar = Instance.new("Frame")
        bar.Size = UDim2.new(1, -20, 0, 8)
        bar.Position = UDim2.new(0, 10, 0, 40)
        bar.BackgroundColor3 = Color3.fromRGB(40, 40, 45)
        bar.BorderSizePixel = 0
        bar.Parent = frame
        
        local barCorner = Instance.new("UICorner")
        barCorner.CornerRadius = UDim.new(1, 0)
        barCorner.Parent = bar
        
        local fill = Instance.new("Frame")
        fill.Size = UDim2.new((default - min) / (max - min), 0, 1, 0)
        fill.BackgroundColor3 = Color3.fromRGB(255, 0, 127)
        fill.BorderSizePixel = 0
        fill.Parent = bar
        
        local fillCorner = Instance.new("UICorner")
        fillCorner.CornerRadius = UDim.new(1, 0)
        fillCorner.Parent = fill
        
        local dot = Instance.new("Frame")
        dot.Size = UDim2.new(0, 16, 0, 16)
        dot.Position = UDim2.new((default - min) / (max - min), -8, 0.5, -8)
        dot.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        dot.BorderSizePixel = 0
        dot.Parent = bar
        
        local dotCorner = Instance.new("UICorner")
        dotCorner.CornerRadius = UDim.new(1, 0)
        dotCorner.Parent = dot
        
        local dragging = false
        
        bar.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                dragging = true
                
                local function update()
                    local mouse = UserInputService:GetMouseLocation()
                    local barPos = bar.AbsolutePosition
                    local barSize = bar.AbsoluteSize
                    local rel = math.clamp((mouse.X - barPos.X) / barSize.X, 0, 1)
                    local val = math.floor(min + (max - min) * rel)
                    
                    value.Text = tostring(val)
                    fill.Size = UDim2.new(rel, 0, 1, 0)
                    dot.Position = UDim2.new(rel, -8, 0.5, -8)
                    callback(val)
                end
                
                update()
                
                local conn1 = UserInputService.InputChanged:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
                        update()
                    end
                end)
                
                local conn2 = UserInputService.InputEnded:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 then
                        dragging = false
                        conn1:Disconnect()
                        conn2:Disconnect()
                    end
                end)
            end
        end)
        
        return frame
    end
    
    -- BUILD UI
    
    Section("ğŸš€ WALKSPEED CONTROL")
    
    Slider("WalkSpeed", 16, 500, 16, function(v)
        _G.Ysl.WalkSpeed = v
        if _G.Ysl.WalkSpeedEnabled then
            humanoid.WalkSpeed = v
        end
    end)
    
    Toggle("Enable WalkSpeed", function(v)
        _G.Ysl.WalkSpeedEnabled = v
        if v then
            EnableWalkSpeed()
            Notify("WalkSpeed Enabled: " .. _G.Ysl.WalkSpeed)
        else
            DisableWalkSpeed()
            Notify("WalkSpeed Disabled")
        end
    end)
    
    Section("ğŸŒ¾ AUTO FARM")
    
    Toggle("Auto Farm Brainrots", function(v)
        _G.Ysl.AutoFarm.Brainrot = v
        Notify(v and "Auto Farm Brainrots: ON" or "Auto Farm Brainrots: OFF")
    end)
    
    Toggle("Auto Collect Money (Your Base)", function(v)
        _G.Ysl.AutoFarm.Money = v
        Notify(v and "Auto Money: ON" or "Auto Money: OFF")
    end)
    
    Toggle("Auto Farm Lucky Blocks", function(v)
        _G.Ysl.AutoFarm.LuckyBlocks = v
        Notify(v and "Auto Lucky Blocks: ON" or "Auto Lucky Blocks: OFF")
    end)
    
    Toggle("Auto Sell (Every 60s)", function(v)
        _G.Ysl.AutoFarm.SellAll = v
    end)
    
    Button("Sell All Now", function()
        pcall(function()
            SellAll_RF:InvokeServer()
            Notify("Sold All!")
        end)
    end)
    
    Button("ğŸ”¥ ENABLE ALL FARM", function()
        _G.Ysl.AutoFarm.Brainrot = true
        _G.Ysl.AutoFarm.Money = true
        _G.Ysl.AutoFarm.LuckyBlocks = true
        _G.Ysl.AutoFarm.SellAll = true
        _G.Ysl.AutoUpgrade.Base = true
        _G.Ysl.AutoUpgrade.Brainrots = true
        _G.Ysl.AutoUpgrade.Carry = true
        Notify("ALL FARM ENABLED!")
    end)
    
    Section("â¬†ï¸ UPGRADES")
    
    Button("Buy Speed +1", function() BuySpeed(1) end)
    Button("Buy Speed +5", function() BuySpeed(5) end)
    Button("Buy Speed +10", function() BuySpeed(10) end)
    
    Toggle("Auto Upgrade Base", function(v) _G.Ysl.AutoUpgrade.Base = v end)
    Toggle("Auto Upgrade Brainrots", function(v) _G.Ysl.AutoUpgrade.Brainrots = v end)
    Toggle("Auto Upgrade Carry", function(v) _G.Ysl.AutoUpgrade.Carry = v end)
    
    Section("ğŸ”„ REBIRTH")
    
    Slider("Rebirth At Speed", 100, 5000, 500, function(v)
        _G.Ysl.RebirthAt = v
    end)
    
    Toggle("Auto Rebirth", function(v)
        _G.Ysl.AutoRebirth = v
        Notify(v and "Auto Rebirth: ON" or "Auto Rebirth: OFF")
    end)
    
    Button("Rebirth Now", function()
        pcall(function()
            Rebirth_RF:InvokeServer()
            Notify("Rebirthed!")
        end)
    end)
    
    Section("âœˆï¸ MOVEMENT")
    
    Slider("Fly Speed", 20, 200, 50, function(v)
        _G.Ysl.Movement.FlySpeed = v
    end)
    
    Toggle("Fly", function(v)
        _G.Ysl.Movement.Fly = v
        if v then StartFly() end
    end)
    
    Toggle("NoClip", function(v) _G.Ysl.Movement.NoClip = v end)
    Toggle("Infinite Jump", function(v) _G.Ysl.Movement.InfJump = v end)
    
    Section("ğŸš« VIP BYPASS")
    
    Toggle("VIP & VIP+ Bypass", function(v)
        _G.Ysl.VIPBypass = v
        Notify(v and "VIP Bypass: ON" or "VIP Bypass: OFF")
    end)
    
    Section("ğŸ“ TELEPORTS")
    
    Button("TP to My Base", function()
        if MyBase then
            local spawn = MyBase:FindFirstChild("Spawn")
            if spawn then
                hrp.CFrame = spawn.CFrame + Vector3.new(0, 5, 0)
                Notify("Teleported to Base")
            end
        end
    end)
    
    Button("TP to Sell Point", function()
        local sellPoint = Workspace:FindFirstChild("SellPoint")
        if sellPoint and sellPoint:FindFirstChild("Seller") then
            hrp.CFrame = sellPoint.Seller:GetPivot() + Vector3.new(0, 5, 0)
            Notify("Teleported to Sell")
        end
    end)
    
    -- Toggle with RightShift
    UserInputService.InputBegan:Connect(function(input)
        if input.KeyCode == Enum.KeyCode.RightShift then
            Main.Visible = not Main.Visible
        end
    end)
    
    -- Entrance
    Main.Size = UDim2.new(0, 0, 0, 0)
    Main.Position = UDim2.new(0.5, 0, 0.5, 0)
    CreateTween(Main, {Size = UDim2.new(0, 550, 0, 650), Position = UDim2.new(0.5, -275, 0.5, -325)}, 1, Enum.EasingStyle.Elastic)
    
    Notify("yslscripts Loaded! Press RightShift to toggle")
    
