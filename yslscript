--// Helper Functions
local function Notify(title, text, duration)
    Rayfield:Notify({
        Title = title,
        Content = text,
        Duration = duration or 3,
        Image = 4483362458
    })
end

local function SafeExecute(func)
    local success, err = pcall(func)
    if not success then
        warn("YslMenu Error:", err)
    end
    return success
end

--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--//  SUPER ADVANCED VIP BYPASS SYSTEM
--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local VIPBypass = {}
VIPBypass.ProcessedDoors = {}
VIPBypass.LastCheck = 0

function VIPBypass:FindVIPDoors()
    local doors = {}
    
    for _, obj in pairs(Workspace:GetDescendants()) do
        if obj:IsA("BasePart") then
            local name = obj.Name:lower()
            local parent = obj.Parent and obj.Parent.Name:lower() or ""
            
            -- Advanced door detection
            if name:find("vip") or name:find("glass") or name:find("premium") or 
               parent:find("vip") or parent:find("door") or parent:find("premium") then
                
                -- Check if it's actually a door (has transparency, specific size, etc.)
                if obj.Size.Y > 5 and obj.Transparency < 1 then
                    table.insert(doors, obj)
                end
            end
        end
    end
    
    return doors
end

function VIPBypass:BypassDoor(door)
    if self.ProcessedDoors[door] then return end
    
    SafeExecute(function()
        -- Method 1: Remove collision
        door.CanCollide = false
        door.CanTouch = false
        
        -- Method 2: Make transparent
        door.Transparency = 0.9
        
        -- Method 3: Remove scripts
        for _, script in pairs(door:GetDescendants()) do
            if script:IsA("Script") or script:IsA("LocalScript") then
                script:Destroy()
            end
        end
        
        -- Method 4: Disable proximity prompts
        for _, prompt in pairs(door:GetDescendants()) do
            if prompt:IsA("ProximityPrompt") then
                prompt.Enabled = false
                prompt.MaxActivationDistance = 0
            end
        end
        
        -- Method 5: Mark as processed
        self.ProcessedDoors[door] = true
        door:SetAttribute("YslBypassed", true)
    end)
end

function VIPBypass:Start()
    if Connections.VIPBypass then
        Connections.VIPBypass:Disconnect()
    end
    
    if not _G.YslPremium.VIPBypass then
        -- Restore doors
        for door, _ in pairs(self.ProcessedDoors) do
            if door and door.Parent then
                door.CanCollide = true
                door.Transparency = door:GetAttribute("OriginalTrans") or 0
            end
        end
        self.ProcessedDoors = {}
        return
    end
    
    Connections.VIPBypass = RunService.Heartbeat:Connect(function()
        local now = tick()
        if now - self.LastCheck < _G.YslPremium.VIPCheckInterval then return end
        self.LastCheck = now
        
        SafeExecute(function()
            local doors = self:FindVIPDoors()
            
            for _, door in pairs(doors) do
                local distance = (door.Position - rootPart.Position).Magnitude
                
                if distance < _G.YslPremium.VIPRange then
                    -- Store original transparency
                    if not door:GetAttribute("OriginalTrans") then
                        door:SetAttribute("OriginalTrans", door.Transparency)
                    end
                    
                    self:BypassDoor(door)
                end
            end
        end)
    end)
    
    Notify("ğŸ”“ VIP Bypass", "Super advanced VIP bypass activated! ğŸ‘‘", 3)
end

function VIPBypass:TeleportThroughDoor()
    SafeExecute(function()
        local doors = self:FindVIPDoors()
        local nearestDoor = nil
        local nearestDist = math.huge
        
        for _, door in pairs(doors) do
            local dist = (door.Position - rootPart.Position).Magnitude
            if dist < nearestDist and dist < 50 then
                nearestDist = dist
                nearestDoor = door
            end
        end
        
        if nearestDoor then
            -- Teleport to the other side
            local doorCFrame = nearestDoor.CFrame
            local lookVector = (rootPart.Position - nearestDoor.Position).Unit
            rootPart.CFrame = doorCFrame + (lookVector * -10) + Vector3.new(0, 5, 0)
            
            Notify("ğŸšª Teleport", "Teleported through VIP door!", 2)
        else
            Notify("âŒ Error", "No VIP door nearby!", 2)
        end
    end)
end

function VIPBypass:RemoveAllVIPWalls()
    local count = 0
    
    SafeExecute(function()
        for _, door in pairs(self:FindVIPDoors()) do
            door:Destroy()
            count = count + 1
        end
    end)
    
    Notify("ğŸ—‘ï¸ VIP Walls", "Removed " .. count .. " VIP walls permanently!", 3)
end

