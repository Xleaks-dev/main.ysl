    -- Submit Key
    local function ValidateKey()
        local inputKey = KeyBox.Text
        
        if VALID_KEYS[inputKey] then
            local keyData = VALID_KEYS[inputKey]
            
            StatusLabel.Text = "âœ… " .. keyData.tier .. " Key Validated!"
            StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 127)
            
            -- Success animation
            for i = 1, 3 do
                MainFrame.BackgroundTransparency = 0.3
                wait(0.1)
                MainFrame.BackgroundTransparency = 0
                wait(0.1)
            end
            
            -- Fade out
            for i = 1, 20 do
                MainFrame.BackgroundTransparency = i / 20
                TopBar.BackgroundTransparency = i / 20
                wait(0.02)
            end
            
            ScreenGui:Destroy()
            return true
        else
            StatusLabel.Text = "âŒ Invalid key! Try again."
            StatusLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
            KeyBox.Text = ""
            
            -- Shake animation
            for i = 1, 3 do
                MainFrame.Position = MainFrame.Position + UDim2.new(0, 15, 0, 0)
                wait(0.05)
                MainFrame.Position = MainFrame.Position - UDim2.new(0, 30, 0, 0)
                wait(0.05)
                MainFrame.Position = MainFrame.Position + UDim2.new(0, 15, 0, 0)
                wait(0.05)
            end
            
            wait(1)
            StatusLabel.Text = "Status: Waiting for key..."
            StatusLabel.TextColor3 = Color3.fromRGB(180, 180, 180)
            
            return false
        end
    end
    
    SubmitButton.MouseButton1Click:Connect(ValidateKey)
    
    KeyBox.FocusLost:Connect(function(enterPressed)
        if enterPressed then
            ValidateKey()
        end
    end)
    
    -- Entrance animation
    MainFrame.BackgroundTransparency = 1
    TopBar.BackgroundTransparency = 1
    
    for i = 20, 0, -1 do
        MainFrame.BackgroundTransparency = i / 20
        TopBar.BackgroundTransparency = i / 20
        wait(0.02)
    end
    
    -- Wait for validation
    repeat wait() until not ScreenGui.Parent
end

