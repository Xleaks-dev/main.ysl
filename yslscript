    local function ClaimFreeEpic()
        pcall(function()
            local success = TakeFreeEpic_RF:InvokeServer()
            if success then
                Notify("Free Epic", "Claimed free epic brainrot!", 3)
            else
                Notify("Free Epic", "Already claimed or unavailable!", 3)
            end
        end)
    end
    
    local function RemoveAllWaves()
        pcall(function()
            local tsunamis = Workspace:FindFirstChild("ActiveTsunamis")
            if tsunamis then
                local count = #tsunamis:GetChildren()
                tsunamis:ClearAllChildren()
                Notify("Waves", count .. " waves removed!", 2)
            end
        end)
    end
    
