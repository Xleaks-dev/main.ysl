--// Connections
local Connections = {}

--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--//  HELPER FUNCTIONS
--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function Notify(title, text, duration)
    Rayfield:Notify({
        Title = title,
        Content = text,
        Duration = duration or 3,
        Image = 4483362458
    })
end

local function SafeCall(func)
    local success, err = pcall(func)
    if not success then
        warn("âŒ YslMenu Error:", err)
    end
    return success
end

--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--//  MOVEMENT SYSTEMS
--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local Movement = {}

-- Speed Hack (Multi-method)
function Movement:SetupSpeed()
    if Connections.Speed then Connections.Speed:Disconnect() end
    
    if not _G.Ysl.Speed then
        if humanoid then humanoid.WalkSpeed = 16 end
        return
    end
    
    Connections.Speed = RunService.Heartbeat:Connect(function()
        SafeCall(function()
            if not humanoid or not rootPart then return end
            
            -- Method 1: WalkSpeed
            humanoid.WalkSpeed = _G.Ysl.SpeedValue
            
            -- Method 2: CFrame boost
            if humanoid.MoveDirection.Magnitude > 0 then
                local boost = (_G.Ysl.SpeedValue - 16) / 16
                rootPart.CFrame = rootPart.CFrame + (humanoid.MoveDirection * boost * 0.1)
            end
        end)
    end)
end

-- Fly System
function Movement:SetupFly()
    if Connections.Fly then Connections.Fly:Disconnect() end
    
    -- Remove old fly parts
    if rootPart:FindFirstChild("YslFlyGyro") then rootPart.YslFlyGyro:Destroy() end
    if rootPart:FindFirstChild("YslFlyVelocity") then rootPart.YslFlyVelocity:Destroy() end
    
    if not _G.Ysl.Fly then return end
    
    local bg = Instance.new("BodyGyro")
    bg.Name = "YslFlyGyro"
    bg.Parent = rootPart
    bg.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
    
    local bv = Instance.new("BodyVelocity")
    bv.Name = "YslFlyVelocity"
    bv.Parent = rootPart
    bv.MaxForce = Vector3.new(9e9, 9e9, 9e9)
    
    Connections.Fly = RunService.Heartbeat:Connect(function()
        SafeCall(function()
            if not _G.Ysl.Fly then return end
            
            local camera = Workspace.CurrentCamera
            local moveDir = Vector3.new(0, 0, 0)
            
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                moveDir = moveDir + (camera.CFrame.LookVector * _G.Ysl.FlySpeed)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                moveDir = moveDir - (camera.CFrame.LookVector * _G.Ysl.FlySpeed)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                moveDir = moveDir - (camera.CFrame.RightVector * _G.Ysl.FlySpeed)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                moveDir = moveDir + (camera.CFrame.RightVector * _G.Ysl.FlySpeed)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                moveDir = moveDir + Vector3.new(0, _G.Ysl.FlySpeed, 0)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                moveDir = moveDir - Vector3.new(0, _G.Ysl.FlySpeed, 0)
            end
            
            bv.Velocity = moveDir
            bg.CFrame = camera.CFrame
        end)
    end)
end

-- NoClip
function Movement:SetupNoClip()
    if Connections.NoClip then Connections.NoClip:Disconnect() end
    
    if not _G.Ysl.NoClip then
        for _, v in pairs(character:GetDescendants()) do
            if v:IsA("BasePart") then v.CanCollide = true end
        end
        return
    end
    
    Connections.NoClip = RunService.Stepped:Connect(function()
        for _, v in pairs(character:GetDescendants()) do
            if v:IsA("BasePart") then v.CanCollide = false end
        end
    end)
end

-- Infinite Jump
UserInputService.JumpRequest:Connect(function()
    if _G.Ysl.InfJump and humanoid then
        SafeCall(function()
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end)
    end
end)

--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--//  ADVANCED VIP BYPASS SYSTEM
--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local VIPBypass = {}
VIPBypass.ProcessedDoors = {}
VIPBypass.DeathWalls = {}

function VIPBypass:FindVIPDoors()
    local doors = {}
    
    for _, obj in pairs(Workspace:GetDescendants()) do
        if obj:IsA("BasePart") then
            local name = obj.Name:lower()
            local parent = obj.Parent and obj.Parent.Name:lower() or ""
            
            -- VIP door detection
            if name:find("vip") or name:find("glass") or name:find("premium") or 
               name:find("door") or parent:find("vip") or parent:find("premium") then
                
                if obj.Size.Y > 3 then
                    table.insert(doors, obj)
                end
            end
        end
    end
    
    return doors
end

function VIPBypass:FindDeathWalls()
    local deathWalls = {}
    
    for _, obj in pairs(Workspace:GetDescendants()) do
        if obj:IsA("BasePart") then
            local name = obj.Name:lower()
            
            -- Death wall detection
            if name:find("kill") or name:find("death") or name:find("void") or 
               name:find("damage") or obj:FindFirstChild("TouchInterest") then
                
                -- Check for kill scripts
                for _, script in pairs(obj:GetDescendants()) do
                    if script:IsA("Script") or script:IsA("LocalScript") then
                        local scriptContent = script.Name:lower()
                        if scriptContent:find("kill") or scriptContent:find("death") or scriptContent:find("damage") then
                            table.insert(deathWalls, obj)
                            break
                        end
                    end
                end
            end
        end
    end
    
    return deathWalls
end

function VIPBypass:BypassDoor(door)
    if self.ProcessedDoors[door] then return end
    
    SafeCall(function()
        -- Store original properties
        if not door:GetAttribute("YslOriginalTrans") then
            door:SetAttribute("YslOriginalTrans", door.Transparency)
            door:SetAttribute("YslOriginalCollide", door.CanCollide)
        end
        
        -- Method 1: Remove collision
        door.CanCollide = false
        door.CanTouch = false
        
        -- Method 2: Transparency
        door.Transparency = 0.8
        
        -- Method 3: Remove scripts
        for _, script in pairs(door:GetDescendants()) do
            if script:IsA("Script") or script:IsA("LocalScript") then
                script:Destroy()
            end
        end
        
        -- Method 4: Disable prompts
        for _, prompt in pairs(door:GetDescendants()) do
            if prompt:IsA("ProximityPrompt") then
                prompt.Enabled = false
                prompt.MaxActivationDistance = 0
            end
        end
        
        -- Method 5: Remove welds/constraints
        for _, constraint in pairs(door:GetDescendants()) do
            if constraint:IsA("Weld") or constraint:IsA("WeldConstraint") or constraint:IsA("Motor6D") then
                constraint:Destroy()
            end
        end
        
        self.ProcessedDoors[door] = true
    end)
end

function VIPBypass:RemoveDeathWall(wall)
    if self.DeathWalls[wall] then return end
    
    SafeCall(function()
        -- Remove all kill scripts
        for _, script in pairs(wall:GetDescendants()) do
            if script:IsA("Script") or script:IsA("LocalScript") then
                script:Destroy()
            end
        end
        
        -- Disable touch
        wall.CanTouch = false
        wall.CanCollide = false
        
        -- Make invisible
        wall.Transparency = 1
        
        -- Remove from workspace
        if _G.Ysl.RemoveDeathWalls then
            wall:Destroy()
        end
        
        self.DeathWalls[wall] = true
        _G.Ysl.Stats.DeathsAvoided = _G.Ysl.Stats.DeathsAvoided + 1
    end)
end

function VIPBypass:Start()
    if Connections.VIPBypass then Connections.VIPBypass:Disconnect() end
    
    if not _G.Ysl.VIPBypass then
        -- Restore doors
        for door, _ in pairs(self.ProcessedDoors) do
            if door and door.Parent then
                door.CanCollide = door:GetAttribute("YslOriginalCollide") or true
                door.Transparency = door:GetAttribute("YslOriginalTrans") or 0
            end
        end
        self.ProcessedDoors = {}
        return
    end
    
    Connections.VIPBypass = RunService.Heartbeat:Connect(function()
        SafeCall(function()
            -- Bypass VIP doors
            local doors = self:FindVIPDoors()
            for _, door in pairs(doors) do
                local dist = (door.Position - rootPart.Position).Magnitude
                if dist < _G.Ysl.VIPRange then
                    self:BypassDoor(door)
                end
            end
            
            -- Remove death walls
            if _G.Ysl.RemoveDeathWalls then
                local deathWalls = self:FindDeathWalls()
                for _, wall in pairs(deathWalls) do
                    self:RemoveDeathWall(wall)
                end
            end
        end)
        task.wait(1) -- Check every second
    end)
end

function VIPBypass:RemoveAllVIP()
    local count = 0
    for _, door in pairs(self:FindVIPDoors()) do
        door:Destroy()
        count = count + 1
    end
    Notify("VIP Bypass", "Removed " .. count .. " VIP walls! ğŸ”“", 3)
end

function VIPBypass:RemoveAllDeathWalls()
    local count = 0
    for _, wall in pairs(self:FindDeathWalls()) do
        wall:Destroy()
        count = count + 1
    end
    Notify("Death Walls", "Removed " .. count .. " death walls! ğŸ›¡ï¸", 3)
end

--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--//  PROTECTION SYSTEMS
--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local Protection = {}

-- God Mode
function Protection:SetupGodMode()
    if not _G.Ysl.GodMode then return end
    
    SafeCall(function()
        humanoid.MaxHealth = math.huge
        humanoid.Health = math.huge
        
        -- Remove damage connections
        for _, connection in pairs(getconnections(humanoid.HealthChanged)) do
            connection:Disable()
        end
    end)
end

-- Wave Immunity (Multi-layer)
function Protection:SetupWaveImmunity()
    if Connections.WaveImmunity then Connections.WaveImmunity:Disconnect() end
    
    if not _G.Ysl.WaveImmunity then return end
    
    Connections.WaveImmunity = RunService.Heartbeat:Connect(function()
        SafeCall(function()
            if not humanoid or not rootPart then return end
            
            -- Layer 1: Health protection
            if humanoid.Health < humanoid.MaxHealth and not _G.Ysl.GodMode then
                humanoid.Health = humanoid.MaxHealth
                _G.Ysl.Stats.WavesBlocked = _G.Ysl.Stats.WavesBlocked + 1
            end
            
            -- Layer 2: Remove water around player
            local region = Region3.new(
                rootPart.Position - Vector3.new(50, 50, 50),
                rootPart.Position + Vector3.new(50, 50, 50)
            )
            Workspace.Terrain:FillRegion(region, 4, Enum.Material.Air)
            
            -- Layer 3: Disable swimming/drowning
            humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming, false)
            humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
            humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
        end)
    end)
end

-- Anti-Void
function Protection:SetupAntiVoid()
    if Connections.AntiVoid then Connections.AntiVoid:Disconnect() end
    
    if not _G.Ysl.AntiVoid then return end
    
    Connections.AntiVoid = RunService.Heartbeat:Connect(function()
        SafeCall(function()
            if rootPart.Position.Y < -100 then
                -- Teleport to safe spot
                local spawn = Workspace:FindFirstChild("SpawnLocation")
                if spawn then
                    rootPart.CFrame = spawn.CFrame + Vector3.new(0, 5, 0)
                else
                    rootPart.CFrame = CFrame.new(0, 50, 0)
                end
                _G.Ysl.Stats.DeathsAvoided = _G.Ysl.Stats.DeathsAvoided + 1
            end
        end)
    end)
end

-- Remove All Kill Parts
function Protection:RemoveKillParts()
    local count = 0
    
    for _, obj in pairs(Workspace:GetDescendants()) do
        if obj:IsA("BasePart") then
            local name = obj.Name:lower()
            
            if name:find("kill") or name:find("death") or name:find("damage") or name:find("lava") then
                obj:Destroy()
                count = count + 1
            end
        end
    end
    
    Notify("Kill Parts", "Removed " .. count .. " kill parts! ğŸ›¡ï¸", 3)
end

-- Remove All Waves
function Protection:RemoveWaves()
    local count = 0
    
    for _, obj in pairs(Workspace:GetDescendants()) do
        if obj:IsA("BasePart") or obj:IsA("Model") then
            local name = obj.Name:lower()
            
            if name:find("wave") or name:find("tsunami") or name:find("water") or name:find("flood") then
                -- Remove scripts first
                for _, script in pairs(obj:GetDescendants()) do
                    if script:IsA("Script") or script:IsA("LocalScript") then
                        script:Destroy()
                    end
                end
                obj:Destroy()
                count = count + 1
            end
        end
    end
    
    -- Clear terrain water
    Workspace.Terrain:Clear()
    
    Notify("Waves", "Destroyed " .. count .. " waves! ğŸŒŠ", 3)
end

--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--//  AUTO FARM SYSTEM
--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local AutoFarm = {}

function AutoFarm:FindBrainrots()
    local brainrots = {}
    local folder = Workspace:FindFirstChild("Brainrots") or Workspace
    
    for _, item in pairs(folder:GetChildren()) do
        if item:IsA("Model") or item:IsA("Part") then
            local name = item.Name:lower()
            
            -- Rarity filter
            if _G.Ysl.TargetRarity ~= "All" then
                if not name:find(_G.Ysl.TargetRarity:lower()) then
                    continue
                end
            end
            
            -- Mutation filter
            if _G.Ysl.OnlyMutations then
                local hasMutation = item:FindFirstChildOfClass("ParticleEmitter") or 
                                   item:FindFirstChildOfClass("PointLight") or
                                   item:FindFirstChildOfClass("Fire")
                if not hasMutation then continue end
            end
            
            local part = item:IsA("Part") and item or item.PrimaryPart or item:FindFirstChildWhichIsA("BasePart")
            
            if part then
                local dist = (part.Position - rootPart.Position).Magnitude
                if dist < _G.Ysl.CollectRange then
                    table.insert(brainrots, {item = item, part = part, distance = dist})
                end
            end
        end
    end
    
    -- Sort by distance
    table.sort(brainrots, function(a, b) return a.distance < b.distance end)
    
    return brainrots
end

function AutoFarm:CollectBrainrot(brainrotData)
    SafeCall(function()
        local part = brainrotData.part
        
        -- Teleport to player
        part.CFrame = rootPart.CFrame
        part.CanCollide = false
        part.Anchored = false
        
        -- Force touch
        if firetouchinterest then
            firetouchinterest(rootPart, part, 0)
            task.wait(0.01)
            firetouchinterest(rootPart, part, 1)
        end
        
        _G.Ysl.Stats.Brainrots = _G.Ysl.Stats.Brainrots + 1
    end)
end

function AutoFarm:StartBrainrotFarm()
    if Connections.BrainrotFarm then Connections.BrainrotFarm:Disconnect() end
    
    if not _G.Ysl.AutoBrainrot then return end
    
    Connections.BrainrotFarm = RunService.Heartbeat:Connect(function()
        SafeCall(function()
            local brainrots = self:FindBrainrots()
            
            for _, data in ipairs(brainrots) do
                self:CollectBrainrot(data)
            end
        end)
        task.wait(0.1)
    end)
end

function AutoFarm:StartMoneyFarm()
    if Connections.MoneyFarm then Connections.MoneyFarm:Disconnect() end
    
    if not _G.Ysl.AutoMoney then return end
    
    Connections.MoneyFarm = RunService.Heartbeat:Connect(function()
        SafeCall(function()
            for _, item in pairs(Workspace:GetDescendants()) do
                if item:IsA("BasePart") and item.Name:lower():find("money") then
                    local dist = (item.Position - rootPart.Position).Magnitude
                    if dist < _G.Ysl.CollectRange then
                        item.CFrame = rootPart.CFrame
                        _G.Ysl.Stats.Money = _G.Ysl.Stats.Money + 1
                    end
                end
            end
        end)
        task.wait(0.1)
    end)
end

function AutoFarm:StartRadCoinFarm()
    if Connections.RadCoinFarm then Connections.RadCoinFarm:Disconnect() end
    
    if not _G.Ysl.AutoRadCoins then return end
    
    Connections.RadCoinFarm = RunService.Heartbeat:Connect(function()
        SafeCall(function()
            for _, item in pairs(Workspace:GetDescendants()) do
                if item:IsA("BasePart") then
                    -- Check for radioactive glow
                    local light = item:FindFirstChild("PointLight")
                    if light and light.Color == Color3.fromRGB(0, 255, 0) then
                        item.CFrame = rootPart.CFrame
                        _G.Ysl.Stats.RadCoins = _G.Ysl.Stats.RadCoins + 1
                    end
                end
            end
        end)
        task.wait(0.05)
    end)
end

function AutoFarm:StartBombFarm()
    if Connections.BombFarm then Connections.BombFarm:Disconnect() end
    
    if not _G.Ysl.AutoBombs then return end
    
    Connections.BombFarm = RunService.Heartbeat:Connect(function()
        SafeCall(function()
            for _, bomb in pairs(Workspace:GetDescendants()) do
                if bomb:IsA("Model") and bomb.Name:lower():find("bomb") then
                    local part = bomb.PrimaryPart or bomb:FindFirstChildWhichIsA("BasePart")
                    if part then
                        part.CFrame = rootPart.CFrame + Vector3.new(0, 5, 0)
                    end
                end
            end
        end)
        task.wait(0.1)
    end)
end

function AutoFarm:StartAutoSpin()
    if not _G.Ysl.AutoSpin then return end
    
    task.spawn(function()
        while _G.Ysl.AutoSpin do
            SafeCall(function()
                local wheel = Workspace:FindFirstChild("SpinWheel") or 
                             Workspace:FindFirstChild("RadioactiveSpin")
                
                if wheel then
                    for _, v in pairs(wheel:GetDescendants()) do
                        if v:IsA("ClickDetector") then
                            fireclickdetector(v)
                        end
                    end
                end
            end)
            task.wait(2)
        end
    end)
end

function AutoFarm:StartAutoUpgrades()
    -- Speed upgrades
    if _G.Ysl.AutoUpgradeSpeed then
        task.spawn(function()
            while _G.Ysl.AutoUpgradeSpeed do
                SafeCall(function()
                    for _, shop in pairs(Workspace:GetDescendants()) do
                        if shop.Name:lower():find("speed") and shop.Name:lower():find("shop") then
                            for _, v in pairs(shop:GetDescendants()) do
                                if v:IsA("ClickDetector") then
                                    fireclickdetector(v)
                                end
                            end
                        end
                    end
                end)
                task.wait(1)
            end
        end)
    end
    
    -- Carry upgrades
    if _G.Ysl.AutoUpgradeCarry then
        task.spawn(function()
            while _G.Ysl.AutoUpgradeCarry do
                SafeCall(function()
                    for _, shop in pairs(Workspace:GetDescendants()) do
                        if shop.Name:lower():find("carry") and shop.Name:lower():find("shop") then
                            for _, v in pairs(shop:GetDescendants()) do
                                if v:IsA("ClickDetector") then
                                    fireclickdetector(v)
                                end
                            end
                        end
                    end
                end)
                task.wait(1)
            end
        end)
    end
end

function AutoFarm:StartAutoRebirth()
    if not _G.Ysl.AutoRebirth then return end
    
    task.spawn(function()
        while _G.Ysl.AutoRebirth do
            SafeCall(function()
                local speedStat = lplayer:FindFirstChild("leaderstats") and 
                                 lplayer.leaderstats:FindFirstChild("Speed")
                
                if speedStat and speedStat.Value >= _G.Ysl.RebirthAt then
                    -- Find rebirth remote
                    for _, remote in pairs(ReplicatedStorage:GetDescendants()) do
                        if remote:IsA("RemoteEvent") and remote.Name:lower():find("rebirth") then
                            remote:FireServer()
                            _G.Ysl.Stats.Rebirths = _G.Ysl.Stats.Rebirths + 1
                            Notify("Auto Rebirth", "Rebirthed at " .. speedStat.Value .. " speed! ğŸ”„", 3)
                            task.wait(10)
                            break
                        end
                    end
                end
            end)
            task.wait(5)
        end
    end)
end

function AutoFarm:StartMasterFarm()
    _G.Ysl.AutoBrainrot = true
    _G.Ysl.AutoMoney = true
    _G.Ysl.AutoRadCoins = true
    _G.Ysl.AutoBombs = true
    _G.Ysl.AutoSpin = true
    _G.Ysl.AutoUpgradeSpeed = true
    _G.Ysl.AutoUpgradeCarry = true
    _G.Ysl.AutoRebirth = true
    
    self:StartBrainrotFarm()
    self:StartMoneyFarm()
    self:StartRadCoinFarm()
    self:StartBombFarm()
    self:StartAutoSpin()
    self:StartAutoUpgrades()
    self:StartAutoRebirth()
    
    Notify("Master Farm", "All auto-farm systems activated! ğŸ¤–", 4)
end

--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--//  ESP SYSTEM
--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local ESP = {}

function ESP:Create(obj, color, text)
    if obj:FindFirstChild("YslESP") then return end
    
    -- Highlight
    local highlight = Instance.new("Highlight")
    highlight.Name = "YslESP"
    highlight.Parent = obj
    highlight.FillColor = color
    highlight.OutlineColor = color
    highlight.FillTransparency = 0.5
    highlight.OutlineTransparency = 0
    
    -- Billboard
    local billboard = Instance.new("BillboardGui")
    billboard.Name = "YslESP"
    billboard.Parent = obj
    billboard.AlwaysOnTop = true
    billboard.Size = UDim2.new(0, 100, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 2, 0)
    
    local label = Instance.new("TextLabel")
    label.Parent = billboard
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextColor3 = color
    label.TextScaled = true
    label.Font = Enum.Font.GothamBold
    label.TextStrokeTransparency = 0
end

function ESP:Remove()
    for _, obj in pairs(Workspace:GetDescendants()) do
        if obj.Name == "YslESP" then
            obj:Destroy()
        end
    end
end

function ESP:StartBrainrotESP()
    if not _G.Ysl.BrainrotESP then
        self:Remove()
        return
    end
    
    task.spawn(function()
        while _G.Ysl.BrainrotESP do
            SafeCall(function()
                local folder = Workspace:FindFirstChild("Brainrots") or Workspace
                for _, item in pairs(folder:GetChildren()) do
                    if item:IsA("Model") and not item:FindFirstChild("YslESP") then
                        self:Create(item, Color3.fromRGB(255, 255, 0), item.Name)
                    end
                end
            end)
            task.wait(2)
        end
    end)
end

function ESP:StartPlayerESP()
    if not _G.Ysl.PlayerESP then
        self:Remove()
        return
    end
    
    task.spawn(function()
        while _G.Ysl.PlayerESP do
            for _, player in pairs(Players:GetPlayers()) do
                if player ~= lplayer and player.Character then
                    local hrp = player.Character:FindFirstChild("HumanoidRootPart")
                    if hrp and not hrp:FindFirstChild("YslESP") then
                        self:Create(hrp, Color3.fromRGB(255, 0, 0), player.Name)
                    end
                end
            end
            task.wait(1)
        end
    end)
end

function ESP:StartSafeZoneESP()
    if not _G.Ysl.SafeZoneESP then
        self:Remove()
        return
    end
    
    for _, obj in pairs(Workspace:GetDescendants()) do
        if obj:IsA("BasePart") and (obj.Name:lower():find("trench") or obj.Name:lower():find("safe") or obj.Name:lower():find("pit")) then
            self:Create(obj, Color3.fromRGB(0, 255, 0), "SAFE ZONE")
        end
    end
end

function ESP:StartVIPDoorESP()
    if not _G.Ysl.VIPDoorESP then
        self:Remove()
        return
    end
    
    for _, door in pairs(VIPBypass:FindVIPDoors()) do
        self:Create(door, Color3.fromRGB(255, 0, 255), "VIP DOOR")
    end
end

--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--//  BYPASS SYSTEMS
--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local Bypass = {}

function Bypass:HideFromAdmins()
    if not _G.Ysl.HideFromAdmins then return end
    
    SafeCall(function()
        for _, player in pairs(Players:GetPlayers()) do
            -- Hide from Roblox staff
            if player.UserId < 100000 or player.Name:find("Roblox") then
                if player.Character then
                    for _, part in pairs(player.Character:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.Transparency = 1
                        end
                    end
                end
            end
        end
    end)
end

function Bypass:RemoveAdmins()
    for _, player in pairs(Players:GetPlayers()) do
        if player.UserId < 100000 or player.Name:find("Roblox") then
            if player.Character then
                player.Character:Destroy()
            end
        end
    end
    Notify("Admins", "Removed all Roblox admins! ğŸ‘»", 3)
end

--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--//  CREATE UI
--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local Window = Rayfield:CreateWindow({
    Name = "âš¡ YslMenu V110 Ultimate",
    LoadingTitle = "YslMenu V110",
    LoadingSubtitle = "The Most Advanced Script Ever! ğŸ˜ˆ",
    ConfigurationSaving = {
        Enabled = true,
        FileName = "YslMenu_V110_Ultimate"
    },
    KeySystem = false
})

--// TABS
local MainTab = Window:CreateTab("ğŸ  Main", 4483362458)
local MovementTab = Window:CreateTab("ğŸš€ Movement", 4483362458)
local VIPTab = Window:CreateTab("ğŸ”“ VIP Bypass", 4483362458)
local ProtectTab = Window:CreateTab("ğŸ›¡ï¸ Protection", 4483362458)
local FarmTab = Window:CreateTab("ğŸ¤– Auto Farm", 4483362458)
local UpgradeTab = Window:CreateTab("âš¡ Auto Upgrades", 4483362458)
local ESPTab = Window:CreateTab("ğŸ‘ï¸ ESP", 4483362458)
local BypassTab = Window:CreateTab("ğŸ” Bypass", 4483362458)
local MiscTab = Window:CreateTab("ğŸ”§ Misc", 4483362458)

--// MAIN TAB
MainTab:CreateSection("ğŸ“Š Live Statistics")

local statsLabel = MainTab:CreateLabel("Loading...")

task.spawn(function()
    local startTime = tick()
    while task.wait(2) do
        _G.Ysl.Stats.SessionTime = math.floor(tick() - startTime)
        local mins = math.floor(_G.Ysl.Stats.SessionTime / 60)
        local secs = _G.Ysl.Stats.SessionTime % 60
        
        statsLabel:Set(string.format(
            "ğŸ§  Brainrots: %d | ğŸ’° Money: %d\n" ..
            "â˜¢ï¸ Rad Coins: %d | ğŸ”„ Rebirths: %d\n" ..
            "ğŸŒŠ Waves Blocked: %d | ğŸ›¡ï¸ Deaths Avoided: %d\n" ..
