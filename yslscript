    -- UI Building Functions
    
    local function Section(name, icon)
        local sec = Instance.new("Frame")
        sec.Size = UDim2.new(1, 0, 0, 45)
        sec.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
        sec.BackgroundTransparency = 0.3
        sec.BorderSizePixel = 0
        sec.Parent = Content
        
        local secCorner = Instance.new("UICorner")
        secCorner.CornerRadius = UDim.new(0, 10)
        secCorner.Parent = sec
        
        AnimateGradient(sec)
        
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, -20, 1, 0)
        label.Position = UDim2.new(0, 10, 0, 0)
        label.BackgroundTransparency = 1
        label.Text = (icon or "‚óè") .. " " .. name
        label.TextSize = 18
        label.Font = Enum.Font.GothamBold
        label.TextColor3 = Color3.fromRGB(255, 255, 255)
        label.TextXAlignment = Enum.TextXAlignment.Left
        label.TextStrokeTransparency = 0.8
        label.Parent = sec
        
        return sec
    end
    
    local function Toggle(name, callback, defaultValue)
        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(1, 0, 0, 50)
        frame.BackgroundColor3 = Color3.fromRGB(18, 18, 28)
        frame.BackgroundTransparency = 0.2
        frame.BorderSizePixel = 0
        frame.Parent = Content
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 10)
        corner.Parent = frame
        
        local stroke = Instance.new("UIStroke")
        stroke.Color = Color3.fromRGB(40, 40, 50)
        stroke.Thickness = 1
        stroke.Transparency = 0.5
        stroke.Parent = frame
        
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, -80, 1, 0)
        label.Position = UDim2.new(0, 15, 0, 0)
        label.BackgroundTransparency = 1
        label.Text = name
        label.TextSize = 14
        label.Font = Enum.Font.GothamMedium
        label.TextColor3 = Color3.fromRGB(220, 220, 220)
        label.TextXAlignment = Enum.TextXAlignment.Left
        label.Parent = frame
        
        local button = Instance.new("TextButton")
        button.Size = UDim2.new(0, 55, 0, 30)
        button.Position = UDim2.new(1, -65, 0.5, -15)
        button.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
        button.Text = ""
        button.AutoButtonColor = false
        button.Parent = frame
        
        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(1, 0)
        btnCorner.Parent = button
        
        local circle = Instance.new("Frame")
        circle.Size = UDim2.new(0, 26, 0, 26)
        circle.Position = UDim2.new(0, 2, 0, 2)
        circle.BackgroundColor3 = Color3.fromRGB(180, 180, 180)
        circle.BorderSizePixel = 0
        circle.Parent = button
        
        local circleCorner = Instance.new("UICorner")
        circleCorner.CornerRadius = UDim.new(1, 0)
        circleCorner.Parent = circle
        
        local on = defaultValue or false
        
        local function updateToggle(state)
            on = state
            if on then
                CreateTween(button, {BackgroundColor3 = Color3.fromRGB(147, 51, 234)}, 0.3)
                CreateTween(circle, {
                    Position = UDim2.new(1, -28, 0, 2),
                    BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                }, 0.3, Enum.EasingStyle.Back)
                AnimateGradient(button)
            else
                CreateTween(button, {BackgroundColor3 = Color3.fromRGB(40, 40, 50)}, 0.3)
                CreateTween(circle, {
                    Position = UDim2.new(0, 2, 0, 2),
                    BackgroundColor3 = Color3.fromRGB(180, 180, 180)
                }, 0.3, Enum.EasingStyle.Back)
                -- Remove gradient
                for _, child in pairs(button:GetChildren()) do
                    if child:IsA("UIGradient") then
                        child:Destroy()
                    end
                end
            end
        end
        
        button.MouseButton1Click:Connect(function()
            updateToggle(not on)
            callback(on)
        end)
        
        -- Hover effect
        button.MouseEnter:Connect(function()
            CreateTween(frame, {BackgroundTransparency = 0}, 0.2)
        end)
        
        button.MouseLeave:Connect(function()
            CreateTween(frame, {BackgroundTransparency = 0.2}, 0.2)
        end)
        
        updateToggle(on)
        
        return frame
    end
    
    local function Button(name, callback, color)
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(1, 0, 0, 50)
        btn.BackgroundColor3 = color or Color3.fromRGB(60, 60, 80)
        btn.BackgroundTransparency = 0.1
        btn.BorderSizePixel = 0
        btn.Text = ""
        btn.AutoButtonColor = false
        btn.Parent = Content
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 10)
        corner.Parent = btn
        
        AnimateGradient(btn)
        
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.Text = name
        label.TextSize = 15
        label.Font = Enum.Font.GothamBold
        label.TextColor3 = Color3.fromRGB(255, 255, 255)
        label.Parent = btn
        
        btn.MouseButton1Click:Connect(function()
            CreateTween(btn, {Size = UDim2.new(1, 0, 0, 55)}, 0.1)
            task.wait(0.1)
            CreateTween(btn, {Size = UDim2.new(1, 0, 0, 50)}, 0.1)
            callback()
        end)
        
        btn.MouseEnter:Connect(function()
            CreateTween(btn, {BackgroundTransparency = 0}, 0.2)
        end)
        
        btn.MouseLeave:Connect(function()
            CreateTween(btn, {BackgroundTransparency = 0.1}, 0.2)
        end)
        
        return btn
    end
    
    local function Slider(name, min, max, default, callback)
        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(1, 0, 0, 70)
        frame.BackgroundColor3 = Color3.fromRGB(18, 18, 28)
        frame.BackgroundTransparency = 0.2
        frame.BorderSizePixel = 0
        frame.Parent = Content
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 10)
        corner.Parent = frame
        
        local stroke = Instance.new("UIStroke")
        stroke.Color = Color3.fromRGB(40, 40, 50)
        stroke.Thickness = 1
        stroke.Transparency = 0.5
        stroke.Parent = frame
        
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, -70, 0, 30)
        label.Position = UDim2.new(0, 15, 0, 5)
        label.BackgroundTransparency = 1
        label.Text = name
        label.TextSize = 14
        label.Font = Enum.Font.GothamMedium
        label.TextColor3 = Color3.fromRGB(220, 220, 220)
        label.TextXAlignment = Enum.TextXAlignment.Left
        label.Parent = frame
        
        local value = Instance.new("TextLabel")
        value.Size = UDim2.new(0, 60, 0, 30)
        value.Position = UDim2.new(1, -70, 0, 5)
        value.BackgroundTransparency = 1
        value.Text = tostring(default)
        value.TextSize = 16
        value.Font = Enum.Font.GothamBold
        value.TextColor3 = Color3.fromRGB(147, 51, 234)
        value.TextXAlignment = Enum.TextXAlignment.Right
        value.Parent = frame
        
        local bar = Instance.new("Frame")
        bar.Size = UDim2.new(1, -30, 0, 10)
        bar.Position = UDim2.new(0, 15, 0, 48)
        bar.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
        bar.BorderSizePixel = 0
        bar.Parent = frame
        
        local barCorner = Instance.new("UICorner")
        barCorner.CornerRadius = UDim.new(1, 0)
        barCorner.Parent = bar
        
        local fill = Instance.new("Frame")
        fill.Size = UDim2.new((default - min) / (max - min), 0, 1, 0)
        fill.BackgroundColor3 = Color3.fromRGB(147, 51, 234)
        fill.BorderSizePixel = 0
        fill.Parent = bar
        
        local fillCorner = Instance.new("UICorner")
        fillCorner.CornerRadius = UDim.new(1, 0)
        fillCorner.Parent = fill
        
        AnimateGradient(fill)
        
        local dot = Instance.new("Frame")
        dot.Size = UDim2.new(0, 20, 0, 20)
        dot.Position = UDim2.new((default - min) / (max - min), -10, 0.5, -10)
        dot.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        dot.BorderSizePixel = 0
        dot.Parent = bar
        
        local dotCorner = Instance.new("UICorner")
        dotCorner.CornerRadius = UDim.new(1, 0)
        dotCorner.Parent = dot
        
        CreateGlow(dot, Color3.fromRGB(147, 51, 234))
        
        local dragging = false
        
        bar.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                dragging = true
                
                local function update()
                    local mouse = UserInputService:GetMouseLocation()
                    local barPos = bar.AbsolutePosition
                    local barSize = bar.AbsoluteSize
                    local rel = math.clamp((mouse.X - barPos.X) / barSize.X, 0, 1)
                    local val = math.floor(min + (max - min) * rel)
                    
                    value.Text = tostring(val)
                    fill.Size = UDim2.new(rel, 0, 1, 0)
                    dot.Position = UDim2.new(rel, -10, 0.5, -10)
                    callback(val)
                end
                
                update()
                
                local conn1 = UserInputService.InputChanged:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
                        update()
                    end
                end)
                
                local conn2 = UserInputService.InputEnded:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 then
                        dragging = false
                        conn1:Disconnect()
                        conn2:Disconnect()
                    end
                end)
            end
        end)
        
        return frame
    end
    
    -- BUILD THE UI
    
    Section("üöÄ WALKSPEED CONTROL", "‚ö°")
    
    Slider("WalkSpeed", 16, 500, 16, function(v)
        _G.YslPremium.WalkSpeed = v
        if _G.YslPremium.WalkSpeedEnabled then
            humanoid.WalkSpeed = v
        end
    end)
    
    Toggle("Enable WalkSpeed", function(v)
        _G.YslPremium.WalkSpeedEnabled = v
        if v then
            EnableWalkSpeed()
            Notify("‚ö° WalkSpeed Enabled: " .. _G.YslPremium.WalkSpeed)
        else
            DisableWalkSpeed()
            Notify("‚ö° WalkSpeed Disabled")
        end
    end)
    
