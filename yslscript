    }
    BGGradient.Rotation = 45
    BGGradient.Parent = GradientBG
    
    local BGCorner = Instance.new("UICorner")
    BGCorner.CornerRadius = UDim.new(0, 20)
    BGCorner.Parent = GradientBG
    
    -- Animate gradient
    task.spawn(function()
        while KeySystemUI.Parent do
            CreateTween(BGGradient, {Rotation = BGGradient.Rotation + 360}, 10, Enum.EasingStyle.Linear)
            task.wait(10)
        end
    end)
    
    -- Top Bar
    local TopBar = Instance.new("Frame")
    TopBar.Name = "TopBar"
    TopBar.Size = UDim2.new(1, 0, 0, 100)
    TopBar.BackgroundColor3 = Color3.fromRGB(255, 0, 127)
    TopBar.BackgroundTransparency = 0
    TopBar.BorderSizePixel = 0
    TopBar.Parent = MainContainer
    
    local TopBarGradient = Instance.new("UIGradient")
    TopBarGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 127)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(138, 43, 226))
    }
    TopBarGradient.Rotation = 90
    TopBarGradient.Parent = TopBar
    
    local TopBarCorner = Instance.new("UICorner")
    TopBarCorner.CornerRadius = UDim.new(0, 20)
    TopBarCorner.Parent = TopBar
    
    -- Logo/Icon
    local Logo = Instance.new("TextLabel")
    Logo.Size = UDim2.new(0, 80, 0, 80)
    Logo.Position = UDim2.new(0, 20, 0, 10)
    Logo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Logo.BackgroundTransparency = 0.9
    Logo.Text = "ğŸ”"
    Logo.TextSize = 50
    Logo.Font = Enum.Font.GothamBold
    Logo.TextColor3 = Color3.fromRGB(255, 255, 255)
    Logo.Parent = TopBar
    
    local LogoCorner = Instance.new("UICorner")
    LogoCorner.CornerRadius = UDim.new(0, 15)
    LogoCorner.Parent = Logo
    
    -- Title
    local Title = Instance.new("TextLabel")
    Title.Size = UDim2.new(0, 400, 0, 40)
    Title.Position = UDim2.new(0, 110, 0, 15)
    Title.BackgroundTransparency = 1
    Title.Text = "YSL PREMIUM ACCESS"
    Title.TextSize = 32
    Title.Font = Enum.Font.GothamBold
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextXAlignment = Enum.TextXAlignment.Left
    Title.Parent = TopBar
    
    local TitleStroke = Instance.new("UIStroke")
    TitleStroke.Color = Color3.fromRGB(0, 0, 0)
    TitleStroke.Thickness = 2
    TitleStroke.Transparency = 0.5
    TitleStroke.Parent = Title
    
    -- Subtitle
    local Subtitle = Instance.new("TextLabel")
    Subtitle.Size = UDim2.new(0, 400, 0, 30)
    Subtitle.Position = UDim2.new(0, 110, 0, 55)
    Subtitle.BackgroundTransparency = 1
    Subtitle.Text = "V117 ULTIMATE EDITION"
    Subtitle.TextSize = 16
    Subtitle.Font = Enum.Font.Gotham
    Subtitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    Subtitle.TextTransparency = 0.3
    Subtitle.TextXAlignment = Enum.TextXAlignment.Left
    Subtitle.Parent = TopBar
    
    -- Content Area
    local ContentArea = Instance.new("Frame")
    ContentArea.Name = "ContentArea"
    ContentArea.Size = UDim2.new(1, -40, 1, -140)
    ContentArea.Position = UDim2.new(0, 20, 0, 120)
    ContentArea.BackgroundTransparency = 1
    ContentArea.Parent = MainContainer
    
    -- Info Text
    local InfoText = Instance.new("TextLabel")
    InfoText.Size = UDim2.new(1, 0, 0, 60)
    InfoText.Position = UDim2.new(0, 0, 0, 0)
    InfoText.BackgroundTransparency = 1
    InfoText.Text = "Enter your premium key to unlock the most advanced\nEscape Tsunami script ever created"
    InfoText.TextSize = 16
    InfoText.Font = Enum.Font.Gotham
    InfoText.TextColor3 = Color3.fromRGB(255, 255, 255)
    InfoText.TextTransparency = 0.3
    InfoText.TextWrapped = true
    InfoText.Parent = ContentArea
    
    -- Key Input Container
    local InputContainer = Instance.new("Frame")
    InputContainer.Size = UDim2.new(1, 0, 0, 70)
    InputContainer.Position = UDim2.new(0, 0, 0, 80)
    InputContainer.BackgroundColor3 = Color3.fromRGB(30, 30, 35)
    InputContainer.BackgroundTransparency = 0.3
    InputContainer.BorderSizePixel = 0
    InputContainer.Parent = ContentArea
    
    local InputCorner = Instance.new("UICorner")
    InputCorner.CornerRadius = UDim.new(0, 12)
    InputCorner.Parent = InputContainer
    
    local InputStroke = Instance.new("UIStroke")
    InputStroke.Color = Color3.fromRGB(255, 0, 127)
    InputStroke.Thickness = 2
    InputStroke.Transparency = 0.5
    InputStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    InputStroke.Parent = InputContainer
    
    -- Key Icon
    local KeyIcon = Instance.new("TextLabel")
    KeyIcon.Size = UDim2.new(0, 50, 0, 50)
    KeyIcon.Position = UDim2.new(0, 10, 0, 10)
    KeyIcon.BackgroundTransparency = 1
    KeyIcon.Text = "ğŸ”‘"
    KeyIcon.TextSize = 30
    KeyIcon.Parent = InputContainer
    
    -- Key TextBox
    local KeyBox = Instance.new("TextBox")
    KeyBox.Size = UDim2.new(1, -80, 1, -20)
    KeyBox.Position = UDim2.new(0, 70, 0, 10)
    KeyBox.BackgroundTransparency = 1
    KeyBox.PlaceholderText = "Enter Premium Key..."
    KeyBox.PlaceholderColor3 = Color3.fromRGB(150, 150, 150)
    KeyBox.Text = ""
    KeyBox.TextSize = 20
    KeyBox.Font = Enum.Font.GothamBold
    KeyBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    KeyBox.TextXAlignment = Enum.TextXAlignment.Left
    KeyBox.ClearTextOnFocus = false
    KeyBox.Parent = InputContainer
    
    -- Focus effect
    KeyBox.Focused:Connect(function()
        CreateTween(InputStroke, {Transparency = 0, Thickness = 3}, 0.2)
        CreateTween(InputContainer, {BackgroundTransparency = 0.1}, 0.2)
    end)
    
    KeyBox.FocusLost:Connect(function()
        CreateTween(InputStroke, {Transparency = 0.5, Thickness = 2}, 0.2)
        CreateTween(InputContainer, {BackgroundTransparency = 0.3}, 0.2)
    end)
    
    -- Submit Button
    local SubmitButton = Instance.new("TextButton")
    SubmitButton.Size = UDim2.new(0, 220, 0, 60)
    SubmitButton.Position = UDim2.new(0.5, -110, 0, 170)
    SubmitButton.BackgroundColor3 = Color3.fromRGB(255, 0, 127)
    SubmitButton.BorderSizePixel = 0
    SubmitButton.AutoButtonColor = false
    SubmitButton.Text = ""
    SubmitButton.Parent = ContentArea
    
    local SubmitCorner = Instance.new("UICorner")
    SubmitCorner.CornerRadius = UDim.new(0, 12)
    SubmitCorner.Parent = SubmitButton
    
    local SubmitGradient = Instance.new("UIGradient")
    SubmitGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 127)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(138, 43, 226))
    }
    SubmitGradient.Rotation = 45
    SubmitGradient.Parent = SubmitButton
    
    local SubmitText = Instance.new("TextLabel")
    SubmitText.Size = UDim2.new(1, 0, 1, 0)
    SubmitText.BackgroundTransparency = 1
    SubmitText.Text = "VERIFY KEY"
    SubmitText.TextSize = 20
    SubmitText.Font = Enum.Font.GothamBold
    SubmitText.TextColor3 = Color3.fromRGB(255, 255, 255)
    SubmitText.Parent = SubmitButton
    
    -- Button hover effect
    SubmitButton.MouseEnter:Connect(function()
        CreateTween(SubmitButton, {Size = UDim2.new(0, 230, 0, 65)}, 0.2, Enum.EasingStyle.Back)
        CreateTween(SubmitText, {TextSize = 22}, 0.2)
    end)
    
    SubmitButton.MouseLeave:Connect(function()
        CreateTween(SubmitButton, {Size = UDim2.new(0, 220, 0, 60)}, 0.2, Enum.EasingStyle.Back)
        CreateTween(SubmitText, {TextSize = 20}, 0.2)
    end)
    
    -- Status Label
    local StatusLabel = Instance.new("TextLabel")
    StatusLabel.Size = UDim2.new(1, 0, 0, 40)
    StatusLabel.Position = UDim2.new(0, 0, 0, 250)
    StatusLabel.BackgroundTransparency = 1
    StatusLabel.Text = ""
    StatusLabel.TextSize = 16
    StatusLabel.Font = Enum.Font.GothamBold
    StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    StatusLabel.Parent = ContentArea
    
    -- Particles Effect
    local function CreateParticle(parent)
        local particle = Instance.new("Frame")
        particle.Size = UDim2.new(0, math.random(3, 8), 0, math.random(3, 8))
        particle.Position = UDim2.new(math.random(), 0, 1, 0)
        particle.BackgroundColor3 = Color3.fromHSV(math.random(), 1, 1)
        particle.BackgroundTransparency = 0.3
        particle.BorderSizePixel = 0
        particle.Parent = parent
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(1, 0)
        corner.Parent = particle
        
        CreateTween(particle, {
            Position = UDim2.new(math.random(), 0, -0.2, 0),
            BackgroundTransparency = 1
        }, math.random(2, 4), Enum.EasingStyle.Linear)
        
        task.delay(4, function()
            particle:Destroy()
        end)
    end
    
    -- Entrance Animation
    MainContainer.Size = UDim2.new(0, 0, 0, 0)
    MainContainer.Position = UDim2.new(0.5, 0, 0.5, 0)
    
    CreateTween(MainContainer, {
        Size = UDim2.new(0, 550, 0, 450),
        Position = UDim2.new(0.5, -275, 0.5, -225)
    }, 0.8, Enum.EasingStyle.Elastic)
    
    -- Make Draggable
    MakeDraggable(MainContainer, TopBar)
    
    -- Key Verification Function
    local function VerifyKey()
        local enteredKey = KeyBox.Text
        
        if enteredKey == "" then
            StatusLabel.Text = "âš ï¸ Please enter a key!"
            StatusLabel.TextColor3 = Color3.fromRGB(255, 200, 0)
            
            -- Shake
            for i = 1, 3 do
                CreateTween(InputContainer, {Position = UDim2.new(0, -10, 0, 80)}, 0.05)
                task.wait(0.05)
                CreateTween(InputContainer, {Position = UDim2.new(0, 10, 0, 80)}, 0.05)
                task.wait(0.05)
            end
            CreateTween(InputContainer, {Position = UDim2.new(0, 0, 0, 80)}, 0.05)
            return
        end
        
        if enteredKey == PREMIUM_KEY then
            -- Success!
            StatusLabel.Text = "âœ… KEY VERIFIED! Loading Premium Features..."
            StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 127)
            
            -- Success animation
            SubmitButton.Active = false
            CreateTween(MainStroke, {Color = Color3.fromRGB(0, 255, 127)}, 0.3)
            CreateTween(InputStroke, {Color = Color3.fromRGB(0, 255, 127)}, 0.3)
            CreateTween(TopBar, {BackgroundColor3 = Color3.fromRGB(0, 255, 127)}, 0.3)
            
            -- Spawn particles
            for i = 1, 20 do
                task.wait(0.05)
                CreateParticle(MainContainer)
            end
            
            task.wait(1.5)
            
            -- Exit animation
            CreateTween(MainContainer, {
                Size = UDim2.new(0, 0, 0, 0),
                Position = UDim2.new(0.5, 0, 0.5, 0)
            }, 0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In)
            
            CreateTween(Overlay, {BackgroundTransparency = 1}, 0.5)
            CreateTween(Blur, {Size = 0}, 0.5)
            
            task.wait(0.6)
            
            KeySystemUI:Destroy()
            Blur:Destroy()
            
            KEY_VERIFIED = true
            loadMainScript()
        else
            -- Wrong key!
            StatusLabel.Text = "âŒ INVALID KEY! Access Denied."
            StatusLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
            
            -- Error animation
            CreateTween(MainStroke, {Color = Color3.fromRGB(255, 0, 0)}, 0.2)
            CreateTween(InputStroke, {Color = Color3.fromRGB(255, 0, 0)}, 0.2)
            
            -- Shake
            for i = 1, 5 do
                CreateTween(MainContainer, {Position = UDim2.new(0.5, -285, 0.5, -225)}, 0.05)
                task.wait(0.05)
                CreateTween(MainContainer, {Position = UDim2.new(0.5, -265, 0.5, -225)}, 0.05)
                task.wait(0.05)
            end
            CreateTween(MainContainer, {Position = UDim2.new(0.5, -275, 0.5, -225)}, 0.1)
            
            KeyBox.Text = ""
            
            task.wait(0.5)
            
            CreateTween(MainStroke, {Color = Color3.fromRGB(255, 0, 127)}, 0.3)
            CreateTween(InputStroke, {Color = Color3.fromRGB(255, 0, 127)}, 0.3)
        end
    end
    
    SubmitButton.MouseButton1Click:Connect(VerifyKey)
    KeyBox.FocusLost:Connect(function(enterPressed)
        if enterPressed then
            VerifyKey()
        end
    end)
end

CreateKeySystem()

--[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    MAIN SCRIPT (LOADS AFTER KEY)
â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]

function loadMainScript()
    
    task.wait(0.5)
    
    print([[
â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
â•‘           LOADING ULTIMATE CUSTOM MENU...                     â•‘
â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
]])
    
    -- Get character
    local character = lplayer.Character or lplayer.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")
    local hrp = character:WaitForChild("HumanoidRootPart")
    
    -- Remotes
    local RemoteFunctions = ReplicatedStorage:WaitForChild("RemoteFunctions")
    local RemoteEvents = ReplicatedStorage:WaitForChild("RemoteEvents")
    
    local Rebirth_RF = RemoteFunctions:WaitForChild("Rebirth")
    local UpgradeSpeed_RF = RemoteFunctions:WaitForChild("UpgradeSpeed")
    local UpgradeBrainrot_RF = RemoteFunctions:WaitForChild("UpgradeBrainrot")
    local UpgradeBase_RF = RemoteFunctions:WaitForChild("UpgradeBase")
    local SellAll_RF = RemoteFunctions:WaitForChild("SellAll")
    local TakeFreeEpic_RF = RemoteFunctions:WaitForChild("TakeFreeEpic")
    local UpgradeCarry_RF = RemoteFunctions:WaitForChild("UpgradeCarry")
    
    local CollectMoney_RE = RemoteEvents:WaitForChild("CollectMoney")
    
    -- Global Settings
    _G.Ysl = _G.Ysl or {
        Enabled = true,
        Stats = {
            Brainrots = 0,
            LuckyBlocks = 0,
            WavesBlocked = 0,
            MoneyCollected = 0,
            SessionStart = os.time()
        },
        RebirthAt = 500,
        AutoFarm = {
            Brainrot = false,
            Money = false,
            LuckyBlocks = false,
            SellAll = false
        },
        AutoUpgrade = {
            Base = false,
            Brainrots = false,
            Carry = false
        },
        AutoRebirth = {
            Enabled = false,
            LastRebirth = 0
        },
        Movement = {
            Speed = false,
            SpeedValue = 100,
            Fly = false,
            FlySpeed = 50,
            NoClip = false,
            InfJump = false
        },
        Protection = {
            GodMode = false,
            AntiVoid = false,
            WaveImmunity = false
        },
        ESP = {
            Brainrots = false,
            LuckyBlocks = false,
            VIPWalls = false,
            Players = false
        }
    }
    
    -- Notification Function
    local function Notify(title, text, duration)
        duration = duration or 4
        game.StarterGui:SetCore("SendNotification", {
            Title = "YSL | " .. title,
            Text = text,
            Duration = duration
        })
    end
    
    -- Anti-AFK
    for i,v in pairs(getconnections(lplayer.Idled)) do
        v:Disable()
    end
    
    -- Character Respawn
    lplayer.CharacterAdded:Connect(function(char)
        task.wait(0.5)
        character = char
        humanoid = char:WaitForChild("Humanoid")
        hrp = char:WaitForChild("HumanoidRootPart")
        Notify("Respawn", "Features reactivated!", 2)
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    AUTO FARM FUNCTIONS (PERFECTED)
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    -- Auto Farm Brainrots
    local BrainrotFarmRunning = false
    task.spawn(function()
        while task.wait(0.2) do
            if _G.Ysl.AutoFarm.Brainrot and _G.Ysl.Enabled then
                BrainrotFarmRunning = true
                pcall(function()
                    local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
                    if not activeBrainrots then return end
                    
                    local nearestBrainrot = nil
                    local nearestDistance = 200
                    
                    for _, rarityFolder in pairs(activeBrainrots:GetChildren()) do
                        if not rarityFolder:IsA("Folder") then continue end
                        
                        for _, renderedModel in pairs(rarityFolder:GetChildren()) do
                            if renderedModel.Name ~= "RenderedBrainrot" then continue end
                            
                            for _, brainrotModel in pairs(renderedModel:GetChildren()) do
                                if not brainrotModel:IsA("Model") then continue end
                                
                                for _, desc in pairs(brainrotModel:GetDescendants()) do
                                    if desc.Name == "TakePrompt" and desc:IsA("ProximityPrompt") then
                                        local modelPivot = brainrotModel:GetPivot()
                                        if modelPivot then
                                            local distance = (modelPivot.Position - hrp.Position).Magnitude
                                            
                                            if distance < nearestDistance then
                                                nearestDistance = distance
                                                nearestBrainrot = {
                                                    model = brainrotModel,
                                                    prompt = desc,
                                                    position = modelPivot.Position
                                                }
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                    
                    if nearestBrainrot then
                        hrp.CFrame = CFrame.new(nearestBrainrot.position + Vector3.new(0, 5, 0))
                        task.wait(0.15)
                        for i = 1, 3 do
                            fireproximityprompt(nearestBrainrot.prompt)
                            task.wait(0.05)
                        end
                        _G.Ysl.Stats.Brainrots = _G.Ysl.Stats.Brainrots + 1
                        task.wait(0.3)
                    end
                end)
            else
                BrainrotFarmRunning = false
            end
        end
    end)
    
    -- Auto Collect Money
    task.spawn(function()
        while task.wait(0.4) do
            if _G.Ysl.AutoFarm.Money and _G.Ysl.Enabled then
                pcall(function()
                    CollectMoney_RE:FireServer()
                    local bases = Workspace:FindFirstChild("Bases")
                    if bases then
                        for _, base in pairs(bases:GetChildren()) do
                            local slots = base:FindFirstChild("Slots")
                            if slots then
                                for _, slot in pairs(slots:GetChildren()) do
                                    local collect = slot:FindFirstChild("Collect")
                                    if collect and collect:IsA("BasePart") then
                                        local detector = collect:FindFirstChildOfClass("ClickDetector")
                                        if detector then
                                            fireclickdetector(detector)
                                        end
                                    end
                                end
                            end
                        end
                    end
                    _G.Ysl.Stats.MoneyCollected = _G.Ysl.Stats.MoneyCollected + 1
                end)
            end
        end
    end)
    
    -- Auto Farm Lucky Blocks
    task.spawn(function()
        while task.wait(0.3) do
            if _G.Ysl.AutoFarm.LuckyBlocks and _G.Ysl.Enabled then
                pcall(function()
                    local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
                    if not luckyBlocks then return end
                    
                    local nearestBlock = nil
                    local nearestDistance = 200
                    
                    for _, block in pairs(luckyBlocks:GetChildren()) do
                        if not block:IsA("Model") then continue end
                        
                        local prompt = block:FindFirstChildWhichIsA("ProximityPrompt", true)
                        if prompt then
                            local blockPivot = block:GetPivot()
                            if blockPivot then
                                local distance = (blockPivot.Position - hrp.Position).Magnitude
                                
                                if distance < nearestDistance then
                                    nearestDistance = distance
                                    nearestBlock = {
                                        model = block,
                                        prompt = prompt,
                                        position = blockPivot.Position
                                    }
                                end
                            end
                        end
                    end
                    
                    if nearestBlock then
                        hrp.CFrame = CFrame.new(nearestBlock.position + Vector3.new(0, 5, 0))
                        task.wait(0.15)
                        for i = 1, 3 do
                            fireproximityprompt(nearestBlock.prompt)
                            task.wait(0.05)
                        end
                        _G.Ysl.Stats.LuckyBlocks = _G.Ysl.Stats.LuckyBlocks + 1
                        task.wait(0.3)
                    end
                end)
            end
        end
    end)
    
    -- Auto Sell
    task.spawn(function()
        while task.wait(60) do
            if _G.Ysl.AutoFarm.SellAll and _G.Ysl.Enabled then
                pcall(function()
                    SellAll_RF:InvokeServer()
                end)
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    MANUAL SPEED UPGRADES
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    local function UpgradeSpeedManual(times)
        task.spawn(function()
            local successCount = 0
            for i = 1, times do
                local success, result = pcall(function()
                    return UpgradeSpeed_RF:InvokeServer()
                end)
                
                if success and result then
                    successCount = successCount + 1
                    task.wait(0.15)
                else
                    Notify("Speed Upgrade", "Failed at " .. i .. "/" .. times, 2)
                    break
                end
            end
            
            if successCount > 0 then
                local currentSpeed = lplayer:GetAttribute("CurrentSpeed") or 16
                Notify("Speed Upgrade", string.format("Upgraded %d times! Speed: %d", successCount, currentSpeed), 3)
            end
        end)
    end
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    AUTO UPGRADES
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    task.spawn(function()
        while task.wait(12) do
            if _G.Ysl.AutoUpgrade.Base and _G.Ysl.Enabled then
                pcall(function() UpgradeBase_RF:InvokeServer() end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(12) do
            if _G.Ysl.AutoUpgrade.Carry and _G.Ysl.Enabled then
                pcall(function() UpgradeCarry_RF:InvokeServer() end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(10) do
            if _G.Ysl.AutoUpgrade.Brainrots and _G.Ysl.Enabled then
                pcall(function()
                    local bases = Workspace:FindFirstChild("Bases")
                    if bases then
                        for _, base in pairs(bases:GetChildren()) do
                            for _, desc in pairs(base:GetDescendants()) do
                                if desc.Name:match("^slot %d+") and desc:IsA("Model") then
                                    if desc:FindFirstChild("Root") then
                                        pcall(function() UpgradeBrainrot_RF:InvokeServer(desc) end)
                                        task.wait(0.2)
                                    end
                                end
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    AUTO REBIRTH
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    task.spawn(function()
        while task.wait(5) do
            if _G.Ysl.AutoRebirth.Enabled and _G.Ysl.Enabled then
                pcall(function()
                    local currentTime = os.time()
                    if currentTime - _G.Ysl.AutoRebirth.LastRebirth >= 10 then
                        local currentSpeed = lplayer:GetAttribute("CurrentSpeed") or 16
                        if currentSpeed >= _G.Ysl.RebirthAt then
                            local success = Rebirth_RF:InvokeServer()
                            if success then
                                _G.Ysl.AutoRebirth.LastRebirth = currentTime
                                Notify("Auto Rebirth", "Rebirthed at Speed: " .. currentSpeed, 2)
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    MOVEMENT SYSTEM (PERFECT)
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    -- Speed
    RunService.Heartbeat:Connect(function()
        if character and humanoid and humanoid.Parent then
            if _G.Ysl.Movement.Speed and _G.Ysl.Enabled then
                humanoid.WalkSpeed = _G.Ysl.Movement.SpeedValue
            else
                if humanoid.WalkSpeed ~= 16 then
                    humanoid.WalkSpeed = 16
                end
            end
        end
    end)
    
    -- Fly
    local flying = false
    local flyBodyVelocity, flyBodyGyro
    
    local function StartFly()
        if flying then return end
        flying = true
        
        if flyBodyVelocity then flyBodyVelocity:Destroy() end
        if flyBodyGyro then flyBodyGyro:Destroy() end
        
        flyBodyVelocity = Instance.new("BodyVelocity")
        flyBodyVelocity.Velocity = Vector3.zero
        flyBodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        flyBodyVelocity.Parent = hrp
        
        flyBodyGyro = Instance.new("BodyGyro")
        flyBodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        flyBodyGyro.P = 9e4
        flyBodyGyro.Parent = hrp
        
        RunService.Heartbeat:Connect(function()
            if not _G.Ysl.Movement.Fly or not flying then
                StopFly()
                return
            end
            
            if not hrp or not hrp.Parent then return end
            
            pcall(function()
                local camera = workspace.CurrentCamera
                local moveDirection = Vector3.zero
                
                if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                    moveDirection = moveDirection + camera.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                    moveDirection = moveDirection - camera.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                    moveDirection = moveDirection - camera.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                    moveDirection = moveDirection + camera.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                    moveDirection = moveDirection + Vector3.new(0, 1, 0)
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                    moveDirection = moveDirection - Vector3.new(0, 1, 0)
                end
                
                if moveDirection.Magnitude > 0 then
                    flyBodyVelocity.Velocity = moveDirection.Unit * _G.Ysl.Movement.FlySpeed
                else
                    flyBodyVelocity.Velocity = Vector3.zero
                end
                
                flyBodyGyro.CFrame = camera.CFrame
            end)
        end)
    end
    
    function StopFly()
        flying = false
        if flyBodyVelocity then flyBodyVelocity:Destroy() flyBodyVelocity = nil end
        if flyBodyGyro then flyBodyGyro:Destroy() flyBodyGyro = nil end
    end
    
    -- NoClip
    RunService.Stepped:Connect(function()
        if character and _G.Ysl.Movement.NoClip and _G.Ysl.Enabled then
            pcall(function()
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end)
        end
    end)
    
    -- Infinite Jump
    UserInputService.JumpRequest:Connect(function()
        if _G.Ysl.Movement.InfJump and _G.Ysl.Enabled and humanoid then
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    PROTECTION SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    -- God Mode
    RunService.Heartbeat:Connect(function()
        if character and humanoid and humanoid.Parent then
            if _G.Ysl.Protection.GodMode and _G.Ysl.Enabled then
                if humanoid.Health < humanoid.MaxHealth then
                    humanoid.Health = humanoid.MaxHealth
                end
            end
        end
    end)
    
    -- Anti-Void
    task.spawn(function()
        while task.wait(0.5) do
            if _G.Ysl.Protection.AntiVoid and _G.Ysl.Enabled and hrp and hrp.Parent then
                if hrp.Position.Y < -50 then
                    pcall(function()
                        local bases = Workspace:FindFirstChild("Bases")
                        if bases then
                            for _, base in pairs(bases:GetChildren()) do
                                local spawn = base:FindFirstChild("Spawn")
                                if spawn and spawn:IsA("BasePart") then
                                    hrp.CFrame = spawn.CFrame + Vector3.new(0, 10, 0)
                                    humanoid.Health = humanoid.MaxHealth
                                    break
                                end
                            end
                        end
                    end)
                end
            end
        end
    end)
    
    -- Wave Immunity
    RunService.Heartbeat:Connect(function()
        if character and humanoid and humanoid.Parent then
            if _G.Ysl.Protection.WaveImmunity and _G.Ysl.Enabled then
                pcall(function()
                    if humanoid.Health < humanoid.MaxHealth then
                        humanoid.Health = humanoid.MaxHealth
                        _G.Ysl.Stats.WavesBlocked = _G.Ysl.Stats.WavesBlocked + 1
                    end
                    
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming, false)
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
                    
                    local tsunamis = Workspace:FindFirstChild("ActiveTsunamis")
                    if tsunamis then
                        for _, tsunami in pairs(tsunamis:GetChildren()) do
                            for _, part in pairs(tsunami:GetDescendants()) do
                                if part:IsA("BasePart") and (part.Position - hrp.Position).Magnitude <= 100 then
                                    part.CanCollide = false
                                    part.CanTouch = false
                                end
                            end
                        end
                    end
                    
                    local lava = Workspace:FindFirstChild("Lava")
                    if lava then
                        for _, part in pairs(lava:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.CanCollide = false
                                part.CanTouch = false
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    MISC FUNCTIONS
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    local function RemoveVIPWalls()
        pcall(function()
            local vipWalls = Workspace:FindFirstChild("VIPWalls")
            if vipWalls then
                local count = 0
                for _, wall in pairs(vipWalls:GetChildren()) do
                    if wall:IsA("BasePart") then
                        wall:Destroy()
                        count = count + 1
                    end
                end
                if count > 0 then
                    Notify("VIP Bypass", count .. " walls removed!", 2)
                end
            end
        end)
    end
    
    local function TeleportTo(location)
        local locations = {
            Spawn = function()
                local bases = Workspace:FindFirstChild("Bases")
                if bases then
                    for _, base in pairs(bases:GetChildren()) do
                        local spawn = base:FindFirstChild("Spawn")
                        if spawn and spawn:IsA("BasePart") then
                            return spawn.CFrame + Vector3.new(0, 5, 0)
                        end
                    end
                end
            end,
            SellPoint = function()
                local sellPoint = Workspace:FindFirstChild("SellPoint")
                if sellPoint and sellPoint:FindFirstChild("Seller") then
                    return sellPoint.Seller:GetPivot() + Vector3.new(0, 5, 0)
                end
            end
        }
        
        pcall(function()
            local func = locations[location]
            if func then
                local cframe = func()
                if cframe then
                    hrp.CFrame = cframe
                    Notify("Teleport", "Teleported to " .. location, 2)
                end
            end
        end)
    end
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    CUSTOM UI CREATION
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    -- Main UI
    local YslUI = Instance.new("ScreenGui")
    YslUI.Name = "YslMenuV117"
    YslUI.Parent = CoreGui
    YslUI.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    YslUI.ResetOnSpawn = false
    
    -- Main Window
    local MainWindow = Instance.new("Frame")
    MainWindow.Name = "MainWindow"
    MainWindow.Size = UDim2.new(0, 650, 0, 500)
    MainWindow.Position = UDim2.new(0.5, -325, 0.5, -250)
    MainWindow.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
    MainWindow.BackgroundTransparency = 0.05
    MainWindow.BorderSizePixel = 0
    MainWindow.Active = true
    MainWindow.ClipsDescendants = true
    MainWindow.Parent = YslUI
    
    local WindowCorner = Instance.new("UICorner")
    WindowCorner.CornerRadius = UDim.new(0, 15)
    WindowCorner.Parent = MainWindow
    
    local WindowStroke = Instance.new("UIStroke")
    WindowStroke.Color = Color3.fromRGB(255, 0, 127)
    WindowStroke.Thickness = 2
    WindowStroke.Transparency = 0.3
    WindowStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    WindowStroke.Parent = MainWindow
    
    -- Animated Background Gradient
    local BGFrame = Instance.new("Frame")
    BGFrame.Size = UDim2.new(1, 0, 1, 0)
    BGFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    BGFrame.BackgroundTransparency = 0.96
    BGFrame.BorderSizePixel = 0
    BGFrame.Parent = MainWindow
    
    local BGGradient = Instance.new("UIGradient")
    BGGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 127)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(138, 43, 226)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 191, 255))
    }
    BGGradient.Rotation = 0
    BGGradient.Parent = BGFrame
    
    local BGCorner = Instance.new("UICorner")
    BGCorner.CornerRadius = UDim.new(0, 15)
    BGCorner.Parent = BGFrame
    
    -- Animate gradient rotation
    task.spawn(function()
        while YslUI.Parent do
            CreateTween(BGGradient, {Rotation = BGGradient.Rotation + 360}, 15, Enum.EasingStyle.Linear)
            task.wait(15)
        end
    end)
    
    -- Top Bar
    local TopBar = Instance.new("Frame")
    TopBar.Name = "TopBar"
    TopBar.Size = UDim2.new(1, 0, 0, 50)
    TopBar.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
    TopBar.BackgroundTransparency = 0.3
    TopBar.BorderSizePixel = 0
    TopBar.Parent = MainWindow
    
    local TopBarCorner = Instance.new("UICorner")
    TopBarCorner.CornerRadius = UDim.new(0, 15)
    TopBarCorner.Parent = TopBar
    
    -- Logo
    local LogoText = Instance.new("TextLabel")
    LogoText.Size = UDim2.new(0, 200, 1, 0)
    LogoText.Position = UDim2.new(0, 15, 0, 0)
    LogoText.BackgroundTransparency = 1
    LogoText.Text = "ğŸ’ YSL MENU V117"
    LogoText.TextSize = 20
    LogoText.Font = Enum.Font.GothamBold
    LogoText.TextColor3 = Color3.fromRGB(255, 255, 255)
    LogoText.TextXAlignment = Enum.TextXAlignment.Left
    LogoText.Parent = TopBar
    
    local LogoGradient = Instance.new("UIGradient")
    LogoGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 127)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(138, 43, 226))
    }
    LogoGradient.Parent = LogoText
    
    -- Version Label
    local VersionLabel = Instance.new("TextLabel")
    VersionLabel.Size = UDim2.new(0, 150, 1, 0)
    VersionLabel.Position = UDim2.new(1, -165, 0, 0)
    VersionLabel.BackgroundTransparency = 1
    VersionLabel.Text = "ULTIMATE EDITION"
    VersionLabel.TextSize = 12
    VersionLabel.Font = Enum.Font.Gotham
    VersionLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    VersionLabel.TextTransparency = 0.5
    VersionLabel.TextXAlignment = Enum.TextXAlignment.Right
    VersionLabel.Parent = TopBar
    
    -- Close Button
    local CloseButton = Instance.new("TextButton")
    CloseButton.Size = UDim2.new(0, 40, 0, 40)
    CloseButton.Position = UDim2.new(1, -45, 0, 5)
    CloseButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
    CloseButton.BackgroundTransparency = 0.3
    CloseButton.BorderSizePixel = 0
    CloseButton.Text = "Ã—"
    CloseButton.TextSize = 30
    CloseButton.Font = Enum.Font.GothamBold
    CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    CloseButton.AutoButtonColor = false
    CloseButton.Parent = TopBar
    
    local CloseCorner = Instance.new("UICorner")
    CloseCorner.CornerRadius = UDim.new(0, 10)
    CloseCorner.Parent = CloseButton
    
    CloseButton.MouseButton1Click:Connect(function()
        CreateTween(MainWindow, {Size = UDim2.new(0, 0, 0, 0)}, 0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In)
        task.wait(0.3)
        YslUI:Destroy()
    end)
    
    CloseButton.MouseEnter:Connect(function()
        CreateTween(CloseButton, {BackgroundTransparency = 0, Size = UDim2.new(0, 45, 0, 45)}, 0.2)
    end)
    
    CloseButton.MouseLeave:Connect(function()
        CreateTween(CloseButton, {BackgroundTransparency = 0.3, Size = UDim2.new(0, 40, 0, 40)}, 0.2)
    end)
    
    -- Make Draggable
    MakeDraggable(MainWindow, TopBar)
    
    -- Tabs Container
    local TabsContainer = Instance.new("Frame")
    TabsContainer.Name = "TabsContainer"
    TabsContainer.Size = UDim2.new(0, 150, 1, -70)
    TabsContainer.Position = UDim2.new(0, 10, 0, 60)
    TabsContainer.BackgroundColor3 = Color3.fromRGB(20, 20, 25)
    TabsContainer.BackgroundTransparency = 0.5
    TabsContainer.BorderSizePixel = 0
    TabsContainer.Parent = MainWindow
    
    local TabsCorner = Instance.new("UICorner")
    TabsCorner.CornerRadius = UDim.new(0, 10)
    TabsCorner.Parent = TabsContainer
    
    local TabsLayout = Instance.new("UIListLayout")
    TabsLayout.Padding = UDim.new(0, 5)
    TabsLayout.SortOrder = Enum.SortOrder.LayoutOrder
    TabsLayout.Parent = TabsContainer
    
    -- Content Container
    local ContentContainer = Instance.new("Frame")
    ContentContainer.Name = "ContentContainer"
    ContentContainer.Size = UDim2.new(1, -180, 1, -70)
    ContentContainer.Position = UDim2.new(0, 170, 0, 60)
    ContentContainer.BackgroundTransparency = 1
    ContentContainer.Parent = MainWindow
    
    -- Tab System
    local CurrentTab = nil
    local Tabs = {}
    
    local function CreateTab(name, icon)
        local TabButton = Instance.new("TextButton")
        TabButton.Size = UDim2.new(1, -10, 0, 40)
        TabButton.BackgroundColor3 = Color3.fromRGB(30, 30, 35)
        TabButton.BackgroundTransparency = 0.5
        TabButton.BorderSizePixel = 0
        TabButton.AutoButtonColor = false
        TabButton.Text = ""
        TabButton.Parent = TabsContainer
        
        local TabCorner = Instance.new("UICorner")
        TabCorner.CornerRadius = UDim.new(0, 8)
        TabCorner.Parent = TabButton
        
        local TabLabel = Instance.new("TextLabel")
        TabLabel.Size = UDim2.new(1, -10, 1, 0)
        TabLabel.Position = UDim2.new(0, 10, 0, 0)
        TabLabel.BackgroundTransparency = 1
        TabLabel.Text = icon .. " " .. name
        TabLabel.TextSize = 14
        TabLabel.Font = Enum.Font.GothamBold
        TabLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
        TabLabel.TextXAlignment = Enum.TextXAlignment.Left
        TabLabel.Parent = TabButton
        
        local TabContent = Instance.new("ScrollingFrame")
        TabContent.Name = name .. "Content"
        TabContent.Size = UDim2.new(1, 0, 1, 0)
        TabContent.BackgroundTransparency = 1
        TabContent.BorderSizePixel = 0
        TabContent.ScrollBarThickness = 6
        TabContent.ScrollBarImageColor3 = Color3.fromRGB(255, 0, 127)
        TabContent.Visible = false
        TabContent.CanvasSize = UDim2.new(0, 0, 0, 0)
        TabContent.AutomaticCanvasSize = Enum.AutomaticSize.Y
        TabContent.Parent = ContentContainer
        
        local ContentLayout = Instance.new("UIListLayout")
        ContentLayout.Padding = UDim.new(0, 10)
        ContentLayout.SortOrder = Enum.SortOrder.LayoutOrder
        ContentLayout.Parent = TabContent
        
        local ContentPadding = Instance.new("UIPadding")
        ContentPadding.PaddingTop = UDim.new(0, 10)
        ContentPadding.PaddingBottom = UDim.new(0, 10)
        ContentPadding.PaddingLeft = UDim.new(0, 10)
        ContentPadding.PaddingRight = UDim.new(0, 10)
        ContentPadding.Parent = TabContent
        
        TabButton.MouseButton1Click:Connect(function()
            -- Deselect all tabs
            for _, tab in pairs(Tabs) do
                CreateTween(tab.Button, {BackgroundTransparency = 0.5}, 0.2)
                CreateTween(tab.Label, {TextColor3 = Color3.fromRGB(200, 200, 200)}, 0.2)
                tab.Content.Visible = false
            end
            
            -- Select this tab
            CreateTween(TabButton, {BackgroundTransparency = 0}, 0.2)
            CreateTween(TabLabel, {TextColor3 = Color3.fromRGB(255, 255, 255)}, 0.2)
            TabContent.Visible = true
            CurrentTab = TabContent
        end)
        
        TabButton.MouseEnter:Connect(function()
            if CurrentTab ~= TabContent then
                CreateTween(TabButton, {BackgroundTransparency = 0.3}, 0.2)
            end
        end)
        
        TabButton.MouseLeave:Connect(function()
            if CurrentTab ~= TabContent then
                CreateTween(TabButton, {BackgroundTransparency = 0.5}, 0.2)
            end
        end)
        
        local tab = {
            Button = TabButton,
            Label = TabLabel,
            Content = TabContent
        }
        
        table.insert(Tabs, tab)
        
        return TabContent
    end
    
    -- UI Element Creation Functions
    local function CreateSection(parent, name)
        local Section = Instance.new("Frame")
        Section.Size = UDim2.new(1, 0, 0, 30)
        Section.BackgroundTransparency = 1
        Section.Parent = parent
        
        local SectionLabel = Instance.new("TextLabel")
        SectionLabel.Size = UDim2.new(1, 0, 1, 0)
        SectionLabel.BackgroundTransparency = 1
        SectionLabel.Text = name
        SectionLabel.TextSize = 16
        SectionLabel.Font = Enum.Font.GothamBold
        SectionLabel.TextColor3 = Color3.fromRGB(255, 0, 127)
        SectionLabel.TextXAlignment = Enum.TextXAlignment.Left
        SectionLabel.Parent = Section
        
        local SectionLine = Instance.new("Frame")
        SectionLine.Size = UDim2.new(1, 0, 0, 2)
        SectionLine.Position = UDim2.new(0, 0, 1, -2)
        SectionLine.BackgroundColor3 = Color3.fromRGB(255, 0, 127)
        SectionLine.BackgroundTransparency = 0.5
        SectionLine.BorderSizePixel = 0
        SectionLine.Parent = Section
        
        local LineCorner = Instance.new("UICorner")
        LineCorner.CornerRadius = UDim.new(1, 0)
        LineCorner.Parent = SectionLine
        
        return Section
    end
    
    local function CreateToggle(parent, name, callback)
        local Toggle = Instance.new("Frame")
        Toggle.Size = UDim2.new(1, 0, 0, 40)
        Toggle.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
        Toggle.BackgroundTransparency = 0.5
        Toggle.BorderSizePixel = 0
        Toggle.Parent = parent
        
        local ToggleCorner = Instance.new("UICorner")
        ToggleCorner.CornerRadius = UDim.new(0, 8)
        ToggleCorner.Parent = Toggle
        
        local ToggleLabel = Instance.new("TextLabel")
        ToggleLabel.Size = UDim2.new(1, -60, 1, 0)
        ToggleLabel.Position = UDim2.new(0, 10, 0, 0)
        ToggleLabel.BackgroundTransparency = 1
        ToggleLabel.Text = name
        ToggleLabel.TextSize = 14
        ToggleLabel.Font = Enum.Font.Gotham
        ToggleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        ToggleLabel.TextXAlignment = Enum.TextXAlignment.Left
        ToggleLabel.Parent = Toggle
        
        local ToggleButton = Instance.new("TextButton")
        ToggleButton.Size = UDim2.new(0, 45, 0, 25)
        ToggleButton.Position = UDim2.new(1, -55, 0.5, -12.5)
        ToggleButton.BackgroundColor3 = Color3.fromRGB(50, 50, 55)
        ToggleButton.BorderSizePixel = 0
        ToggleButton.AutoButtonColor = false
        ToggleButton.Text = ""
        ToggleButton.Parent = Toggle
        
        local ToggleButtonCorner = Instance.new("UICorner")
        ToggleButtonCorner.CornerRadius = UDim.new(1, 0)
        ToggleButtonCorner.Parent = ToggleButton
        
        local ToggleCircle = Instance.new("Frame")
        ToggleCircle.Size = UDim2.new(0, 21, 0, 21)
        ToggleCircle.Position = UDim2.new(0, 2, 0, 2)
        ToggleCircle.BackgroundColor3 = Color3.fromRGB(200, 200, 200)
        ToggleCircle.BorderSizePixel = 0
        ToggleCircle.Parent = ToggleButton
        
        local CircleCorner = Instance.new("UICorner")
        CircleCorner.CornerRadius = UDim.new(1, 0)
        CircleCorner.Parent = ToggleCircle
        
        local enabled = false
        
        ToggleButton.MouseButton1Click:Connect(function()
            enabled = not enabled
            
            if enabled then
                CreateTween(ToggleButton, {BackgroundColor3 = Color3.fromRGB(255, 0, 127)}, 0.2)
                CreateTween(ToggleCircle, {Position = UDim2.new(1, -23, 0, 2), BackgroundColor3 = Color3.fromRGB(255, 255, 255)}, 0.2)
            else
                CreateTween(ToggleButton, {BackgroundColor3 = Color3.fromRGB(50, 50, 55)}, 0.2)
                CreateTween(ToggleCircle, {Position = UDim2.new(0, 2, 0, 2), BackgroundColor3 = Color3.fromRGB(200, 200, 200)}, 0.2)
            end
            
            callback(enabled)
        end)
        
        Toggle.MouseEnter:Connect(function()
            CreateTween(Toggle, {BackgroundTransparency = 0.3}, 0.2)
        end)
        
        Toggle.MouseLeave:Connect(function()
            CreateTween(Toggle, {BackgroundTransparency = 0.5}, 0.2)
        end)
        
        return Toggle
    end
    
    local function CreateButton(parent, name, callback)
        local Button = Instance.new("TextButton")
        Button.Size = UDim2.new(1, 0, 0, 40)
        Button.BackgroundColor3 = Color3.fromRGB(255, 0, 127)
        Button.BackgroundTransparency = 0.3
        Button.BorderSizePixel = 0
        Button.AutoButtonColor = false
        Button.Text = name
        Button.TextSize = 14
        Button.Font = Enum.Font.GothamBold
        Button.TextColor3 = Color3.fromRGB(255, 255, 255)
        Button.Parent = parent
        
        local ButtonCorner = Instance.new("UICorner")
        ButtonCorner.CornerRadius = UDim.new(0, 8)
        ButtonCorner.Parent = Button
        
        local ButtonGradient = Instance.new("UIGradient")
        ButtonGradient.Color = ColorSequence.new{
            ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 127)),
            ColorSequenceKeypoint.new(1, Color3.fromRGB(138, 43, 226))
        }
        ButtonGradient.Rotation = 45
        ButtonGradient.Parent = Button
        
        Button.MouseButton1Click:Connect(function()
            CreateTween(Button, {Size = UDim2.new(1, 0, 0, 35)}, 0.1)
            task.wait(0.1)
            CreateTween(Button, {Size = UDim2.new(1, 0, 0, 40)}, 0.1)
            callback()
        end)
        
        Button.MouseEnter:Connect(function()
            CreateTween(Button, {BackgroundTransparency = 0}, 0.2)
        end)
        
        Button.MouseLeave:Connect(function()
            CreateTween(Button, {BackgroundTransparency = 0.3}, 0.2)
        end)
        
        return Button
    end
    
    local function CreateSlider(parent, name, min, max, default, callback)
        local Slider = Instance.new("Frame")
        Slider.Size = UDim2.new(1, 0, 0, 60)
        Slider.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
        Slider.BackgroundTransparency = 0.5
        Slider.BorderSizePixel = 0
        Slider.Parent = parent
        
        local SliderCorner = Instance.new("UICorner")
        SliderCorner.CornerRadius = UDim.new(0, 8)
        SliderCorner.Parent = Slider
        
        local SliderLabel = Instance.new("TextLabel")
        SliderLabel.Size = UDim2.new(1, -20, 0, 20)
        SliderLabel.Position = UDim2.new(0, 10, 0, 5)
        SliderLabel.BackgroundTransparency = 1
        SliderLabel.Text = name
        SliderLabel.TextSize = 14
        SliderLabel.Font = Enum.Font.Gotham
        SliderLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        SliderLabel.TextXAlignment = Enum.TextXAlignment.Left
        SliderLabel.Parent = Slider
        
        local SliderValue = Instance.new("TextLabel")
        SliderValue.Size = UDim2.new(0, 50, 0, 20)
        SliderValue.Position = UDim2.new(1, -60, 0, 5)
        SliderValue.BackgroundTransparency = 1
        SliderValue.Text = tostring(default)
        SliderValue.TextSize = 14
        SliderValue.Font = Enum.Font.GothamBold
        SliderValue.TextColor3 = Color3.fromRGB(255, 0, 127)
        SliderValue.TextXAlignment = Enum.TextXAlignment.Right
        SliderValue.Parent = Slider
        
        local SliderBar = Instance.new("Frame")
        SliderBar.Size = UDim2.new(1, -20, 0, 8)
        SliderBar.Position = UDim2.new(0, 10, 0, 35)
        SliderBar.BackgroundColor3 = Color3.fromRGB(40, 40, 45)
        SliderBar.BorderSizePixel = 0
        SliderBar.Parent = Slider
        
        local BarCorner = Instance.new("UICorner")
        BarCorner.CornerRadius = UDim.new(1, 0)
        BarCorner.Parent = SliderBar
        
        local SliderFill = Instance.new("Frame")
        SliderFill.Size = UDim2.new((default - min) / (max - min), 0, 1, 0)
        SliderFill.BackgroundColor3 = Color3.fromRGB(255, 0, 127)
        SliderFill.BorderSizePixel = 0
        SliderFill.Parent = SliderBar
        
        local FillCorner = Instance.new("UICorner")
        FillCorner.CornerRadius = UDim.new(1, 0)
        FillCorner.Parent = SliderFill
        
        local FillGradient = Instance.new("UIGradient")
        FillGradient.Color = ColorSequence.new{
            ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 127)),
            ColorSequenceKeypoint.new(1, Color3.fromRGB(138, 43, 226))
        }
        FillGradient.Parent = SliderFill
        
        local SliderDot = Instance.new("Frame")
        SliderDot.Size = UDim2.new(0, 18, 0, 18)
        SliderDot.Position = UDim2.new((default - min) / (max - min), -9, 0.5, -9)
        SliderDot.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderDot.BorderSizePixel = 0
        SliderDot.Parent = SliderBar
        
        local DotCorner = Instance.new("UICorner")
        DotCorner.CornerRadius = UDim.new(1, 0)
        DotCorner.Parent = SliderDot
        
        local dragging = false
        
        SliderBar.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                dragging = true
                local function update()
                    local mousePos = UserInputService:GetMouseLocation()
                    local barPos = SliderBar.AbsolutePosition
                    local barSize = SliderBar.AbsoluteSize
                    
                    local relative = math.clamp((mousePos.X - barPos.X) / barSize.X, 0, 1)
                    local value = math.floor(min + (max - min) * relative)
                    
                    SliderValue.Text = tostring(value)
                    CreateTween(SliderFill, {Size = UDim2.new(relative, 0, 1, 0)}, 0.1)
                    CreateTween(SliderDot, {Position = UDim2.new(relative, -9, 0.5, -9)}, 0.1)
                    
                    callback(value)
                end
                
                update()
                
                local connection
                connection = UserInputService.InputChanged:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
                        update()
                    end
                end)
                
                local releaseConnection
                releaseConnection = UserInputService.InputEnded:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 then
                        dragging = false
                        connection:Disconnect()
                        releaseConnection:Disconnect()
                    end
                end)
            end
        end)
        
        Slider.MouseEnter:Connect(function()
            CreateTween(Slider, {BackgroundTransparency = 0.3}, 0.2)
            CreateTween(SliderDot, {Size = UDim2.new(0, 22, 0, 22), Position = UDim2.new(SliderDot.Position.X.Scale, -11, 0.5, -11)}, 0.2)
        end)
        
        Slider.MouseLeave:Connect(function()
            if not dragging then
                CreateTween(Slider, {BackgroundTransparency = 0.5}, 0.2)
                CreateTween(SliderDot, {Size = UDim2.new(0, 18, 0, 18), Position = UDim2.new(SliderDot.Position.X.Scale, -9, 0.5, -9)}, 0.2)
            end
        end)
        
        return Slider
    end
    
