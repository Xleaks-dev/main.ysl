    CreateToggle(UpgradeTab, "Auto Upgrade Carry Capacity", function(v) _G.Ysl.AutoUpgrade.Carry = v end)
    
    -- TAB 3: REBIRTH
    local RebirthTab = CreateTab("Rebirth", "ğŸ”„")
    
    CreateSection(RebirthTab, "Auto Rebirth Settings")
    CreateToggle(RebirthTab, "Auto Rebirth", function(v)
        _G.Ysl.AutoRebirth.Enabled = v
        Notify("Auto Rebirth", v and "Enabled at speed " .. _G.Ysl.RebirthAt or "Disabled", 2)
    end)
    
    CreateSlider(RebirthTab, "Rebirth at Speed", 100, 10000, 500, function(v)
        _G.Ysl.RebirthAt = v
