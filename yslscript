        pcall(function()
            SellAll_RF:InvokeServer()
            Notify("üí∏ Sold Everything!")
        end)
    end, Color3.fromRGB(34, 197, 94))
    
    Button("üî• ENABLE ALL FARM", function()
        _G.YslPremium.AutoFarm.Brainrot = true
        _G.YslPremium.AutoFarm.Money = true
        _G.YslPremium.AutoFarm.LuckyBlocks = true
        _G.YslPremium.AutoFarm.SellAll = true
        _G.YslPremium.AutoUpgrade.Base = true
        _G.YslPremium.AutoUpgrade.Brainrots = true
        _G.YslPremium.AutoUpgrade.Carry = true
        Notify("üî• ALL FEATURES ACTIVATED!")
    end, Color3.fromRGB(239, 68, 68))
    
    Section("‚¨ÜÔ∏è UPGRADES", "üìà")
    
    Button("‚ö° Buy Speed +1", function()
        pcall(function()
            UpgradeSpeed_RF:InvokeServer()
            Notify("‚ö° Speed +1")
        end)
    end)
    
    Button("‚ö° Buy Speed +5", function()
        for i = 1, 5 do
            pcall(function() UpgradeSpeed_RF:InvokeServer() end)
            task.wait(0.1)
        end
        Notify("‚ö° Speed +5")
    end)
    
    Button("‚ö° Buy Speed +10", function()
        for i = 1, 10 do
            pcall(function() UpgradeSpeed_RF:InvokeServer() end)
            task.wait(0.1)
        end
        Notify("‚ö° Speed +10")
    end)
    
