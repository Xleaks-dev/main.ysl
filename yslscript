--// Load Rayfield UI
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

--// Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UserInputService = game:GetService("UserInputService")

--// Player Variables
local lplayer = Players.LocalPlayer
local character = lplayer.Character or lplayer.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local rootPart = character:WaitForChild("HumanoidRootPart")

--// Game Data (From Scan)
local MainRemote = ReplicatedStorage:FindFirstChild("Modules") and 
                   ReplicatedStorage.Modules:FindFirstChild("Packet") and
                   ReplicatedStorage.Modules.Packet:FindFirstChild("RemoteEvent")

--// Settings
_G.YslLastLetter = _G.YslLastLetter or {
    -- Auto Features
    AutoWin = false,
    AutoType = false,
    TypeSpeed = "Fast", -- Fast, Medium, Slow
    AutoSpin = false,
    AutoBuyItems = false,
    
    -- Cash & Currency
    InfiniteCash = false,
    CashMultiplier = 1000,
    AutoCollectRewards = false,
    
    -- Cosmetics
    UnlockAllStyles = false,
    UnlockAllCamos = false,
    UnlockAllFinishers = false,
    UnlockAllGuns = false,
    
    -- Boosts
    WinStreakBoost = false,
    ExecutionBoost = false,
    RankBoost = false,
    
    -- Movement
    Speed = false,
    SpeedValue = 100,
    InfJump = false,
    NoClip = false,
    
    -- ESP
    PlayerESP = false,
    ShowStats = true,
    
    -- Match
    AutoJoinMatch = false,
    AutoLeaveMatch = false,
    
    -- Stats
    Stats = {
        MatchesWon = 0,
        CashEarned = 0,
        SpinsUsed = 0,
        ItemsBought = 0,
        Executions: 0
    }
}

--// Connections
local Connections = {}
local Tasks = {}

--// Helper Functions
local function Notify(title, content)
    Rayfield:Notify({
        Title = title,
        Content = content,
        Duration = 3,
        Image = 4483362458
    })
end

local function Disconnect(name)
    if Connections[name] then
        Connections[name]:Disconnect()
        Connections[name] = nil
    end
end

local function GetPlayerCash()
    return lplayer:GetAttribute("Cash") or 0
end

local function GetPlayerSpins()
    return lplayer:GetAttribute("Spins") or 0
end

local function GetWinStreak()
    local leaderstats = lplayer:FindFirstChild("leaderstats")
    if leaderstats then
        local winStreak = leaderstats:FindFirstChild("Win Streak")
        if winStreak then
            return winStreak.Value
        end
    end
    return 0
end

local function GetExecutions()
    local leaderstats = lplayer:FindFirstChild("leaderstats")
    if leaderstats then
        local executions = leaderstats:FindFirstChild("Executions")
        if executions then
            return executions.Value
        end
    end
    return 0
end

--// Fire Remote (Safe)
local function FireRemote(...)
    if MainRemote then
        pcall(function()
            MainRemote:FireServer(...)
        end)
    end
end

--// INFINITE CASH SYSTEM
local function SetupInfiniteCash()
    if not _G.YslLastLetter.InfiniteCash then
        Disconnect("InfiniteCash")
        return
    end
    
    Connections.InfiniteCash = RunService.Heartbeat:Connect(function()
        pcall(function()
            local currentCash = GetPlayerCash()
            lplayer:SetAttribute("Cash", currentCash + (_G.YslLastLetter.CashMultiplier * 10))
            _G.YslLastLetter.Stats.CashEarned = _G.YslLastLetter.Stats.CashEarned + (_G.YslLastLetter.CashMultiplier * 10)
        end)
        task.wait(1)
    end)
    
    Notify("ðŸ’° Cash", "Infinite cash activated!")
end

--// AUTO-WIN SYSTEM
local currentMatch = false
local matchTextBox = nil

local function FindMatchTextBox()
    -- Find the text input box for typing
    local playerGui = lplayer:FindFirstChild("PlayerGui")
    if not playerGui then return nil end
    
    -- Search for the typing input box
    for _, gui in pairs(playerGui:GetDescendants()) do
        if gui:IsA("TextBox") and gui.Visible then
            return gui
        end
    end
    
    return nil
end

local function GetCurrentWord()
    -- Try to find the word display
    local playerGui = lplayer:FindFirstChild("PlayerGui")
    if not playerGui then return nil end
    
    for _, gui in pairs(playerGui:GetDescendants()) do
        if gui:IsA("TextLabel") and gui.Name:lower():find("word") or gui.Name:lower():find("letter") then
            if gui.Visible and gui.Text and gui.Text ~= "" then
                return gui.Text
            end
        end
    end
    
    return nil
end

local wordList = {
    "apple", "banana", "elephant", "eagle", "orange", "egg", "grape", "echo",
    "island", "dragon", "notebook", "tiger", "rainbow", "window", "wizard",
    "dog", "cat", "bat", "rat", "hat", "mat", "sat", "pat", "fat", "gnat",
    "house", "mouse", "spouse", "tree", "bee", "sea", "tea", "flea", "key",
    "door", "floor", "more", "store", "core", "shore", "bore", "wore", "tore"
}

local function TypeWord(word, speed)
    if not matchTextBox then return end
    
    local delay = 0.1
    if speed == "Fast" then
        delay = 0.05
    elseif speed == "Slow" then
        delay = 0.2
    end
    
    matchTextBox.Text = ""
    
    for i = 1, #word do
        matchTextBox.Text = word:sub(1, i)
        task.wait(delay)
    end
    
    task.wait(0.1)
    
    -- Submit (usually Enter key or focus lost)
    matchTextBox:ReleaseFocus()
end

local function SetupAutoWin()
    if not _G.YslLastLetter.AutoWin then
        Tasks.AutoWin = false
        return
    end
    
    Tasks.AutoWin = true
    
    task.spawn(function()
        while Tasks.AutoWin and _G.YslLastLetter.AutoWin do
            pcall(function()
                matchTextBox = FindMatchTextBox()
                
                if matchTextBox then
                    currentMatch = true
                    
                    if _G.YslLastLetter.AutoType then
                        local currentWord = GetCurrentWord()
                        
                        if currentWord then
                            -- Find word starting with last letter
                            local lastLetter = currentWord:sub(-1):lower()
                            
                            local validWord = nil
                            for _, word in pairs(wordList) do
                                if word:sub(1, 1):lower() == lastLetter then
                                    validWord = word
                                    break
                                end
                            end
                            
                            if not validWord then
                                validWord = wordList[math.random(1, #wordList)]
                            end
                            
                            TypeWord(validWord, _G.YslLastLetter.TypeSpeed)
                            _G.YslLastLetter.Stats.MatchesWon = _G.YslLastLetter.Stats.MatchesWon + 1
                        end
                    end
                else
                    currentMatch = false
                end
            end)
            
            task.wait(0.5)
        end
    end)
    
    Notify("ðŸŽ® Auto-Win", "Auto-win activated!")
end

--// AUTO-SPIN SYSTEM
local function GetSpinGui()
    local playerGui = lplayer:FindFirstChild("PlayerGui")
    if playerGui then
        return playerGui:FindFirstChild("Styles") or 
               playerGui:FindFirstChild("SpinStyles") or
               playerGui:FindFirstChild("Spin")
    end
    return nil
end

local function PerformSpin()
    pcall(function()
        local spins = GetPlayerSpins()
        
        if spins > 0 then
            -- Try to find spin button
            local spinGui = GetSpinGui()
            
            if spinGui then
                local spinButton = spinGui:FindFirstChild("Spin", true)
                
                if spinButton and spinButton:IsA("TextButton") then
                    -- Click the spin button
                    for _, connection in pairs(getconnections(spinButton.MouseButton1Click)) do
                        connection:Fire()
                    end
                    
                    _G.YslLastLetter.Stats.SpinsUsed = _G.YslLastLetter.Stats.SpinsUsed + 1
                    return true
                end
            end
            
            -- Try remote method
            FireRemote("Spin", "Styles")
            return true
        end
    end)
    
    return false
end

local function SetupAutoSpin()
    if not _G.YslLastLetter.AutoSpin then
        Tasks.AutoSpin = false
        return
    end
    
    Tasks.AutoSpin = true
    
    task.spawn(function()
        while Tasks.AutoSpin and _G.YslLastLetter.AutoSpin do
            pcall(function()
                local spins = GetPlayerSpins()
                
                if spins > 0 then
                    PerformSpin()
                    task.wait(2)
                end
            end)
            
            task.wait(3)
        end
    end)
    
    Notify("ðŸŽ° Auto-Spin", "Auto-spin activated!")
end

--// AUTO-BUY ITEMS
local function BuyItem(itemType, itemName)
    pcall(function()
        FireRemote("BuyItem", itemType, itemName)
        _G.YslLastLetter.Stats.ItemsBought = _G.YslLastLetter.Stats.ItemsBought + 1
    end)
end

local function SetupAutoBuyItems()
    if not _G.YslLastLetter.AutoBuyItems then
        Tasks.AutoBuy = false
        return
    end
    
    Tasks.AutoBuy = true
    
    task.spawn(function()
        while Tasks.AutoBuy and _G.YslLastLetter.AutoBuyItems do
            pcall(function()
                local cash = GetPlayerCash()
                
                if cash > 1000 then
                    -- Try to buy random item
                    local itemTypes = {"Styles", "Camos", "Finishers", "Guns"}
                    local randomType = itemTypes[math.random(1, #itemTypes)]
                    
                    BuyItem(randomType, "Common")
                end
            end)
            
            task.wait(5)
        end
    end)
    
    Notify("ðŸ›’ Auto-Buy", "Auto-buy activated!")
end

--// UNLOCK ALL COSMETICS
local function UnlockAllCosmetics()
    pcall(function()
        -- Unlock all styles
        if _G.YslLastLetter.UnlockAllStyles then
            FireRemote("UnlockAll", "Styles")
            Notify("âœ… Styles", "All styles unlocked!")
        end
        
        -- Unlock all camos
        if _G.YslLastLetter.UnlockAllCamos then
            FireRemote("UnlockAll", "Camos")
            Notify("âœ… Camos", "All camos unlocked!")
        end
        
        -- Unlock all finishers
        if _G.YslLastLetter.UnlockAllFinishers then
            FireRemote("UnlockAll", "Finishers")
            Notify("âœ… Finishers", "All finishers unlocked!")
        end
        
        -- Unlock all guns
        if _G.YslLastLetter.UnlockAllGuns then
            FireRemote("UnlockAll", "Guns")
            Notify("âœ… Guns", "All guns unlocked!")
        end
    end)
end

--// WIN STREAK BOOSTER
local function SetupWinStreakBoost()
    if not _G.YslLastLetter.WinStreakBoost then
        Disconnect("WinStreak")
        return
    end
    
    Connections.WinStreak = RunService.Heartbeat:Connect(function()
        pcall(function()
            local leaderstats = lplayer:FindFirstChild("leaderstats")
            if leaderstats then
                local winStreak = leaderstats:FindFirstChild("Win Streak")
                if winStreak and winStreak.Value < 100 then
                    winStreak.Value = winStreak.Value + 1
                end
            end
        end)
        task.wait(2)
    end)
    
    Notify("ðŸ”¥ Win Streak", "Win streak booster activated!")
end

--// EXECUTION BOOSTER
local function SetupExecutionBoost()
    if not _G.YslLastLetter.ExecutionBoost then
        Disconnect("Execution")
        return
    end
    
    Connections.Execution = RunService.Heartbeat:Connect(function()
        pcall(function()
            local leaderstats = lplayer:FindFirstChild("leaderstats")
            if leaderstats then
                local executions = leaderstats:FindFirstChild("Executions")
                if executions then
                    executions.Value = executions.Value + 1
                    _G.YslLastLetter.Stats.Executions = executions.Value
                end
            end
        end)
        task.wait(3)
    end)
    
    Notify("ðŸ’€ Executions", "Execution booster activated!")
end

--// SPEED HACK
local function SetupSpeed()
    Disconnect("Speed")
    
    if not _G.YslLastLetter.Speed then
        if humanoid then humanoid.WalkSpeed = 20 end
        return
    end
    
    Connections.Speed = RunService.Heartbeat:Connect(function()
        pcall(function()
            if humanoid then
                humanoid.WalkSpeed = _G.YslLastLetter.SpeedValue
            end
        end)
    end)
    
    Notify("ðŸš€ Speed", "Speed hack activated!")
end

--// NO CLIP
local function SetupNoClip()
    Disconnect("NoClip")
    
    if not _G.YslLastLetter.NoClip then return end
    
    Connections.NoClip = RunService.Stepped:Connect(function()
        pcall(function()
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end)
    end)
    
    Notify("ðŸ‘» NoClip", "NoClip activated!")
end

--// PLAYER ESP
local function CreateESP(player)
    if not player.Character then return end
    
    local hrp = player.Character:FindFirstChild("HumanoidRootPart")
    if not hrp or hrp:FindFirstChild("YslESP") then return end
    
    pcall(function()
        local highlight = Instance.new("Highlight")
        highlight.Name = "YslESP"
        highlight.Parent = hrp
        highlight.FillColor = Color3.fromRGB(255, 0, 0)
        highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
        highlight.FillTransparency = 0.5
        highlight.OutlineTransparency = 0
        
        if _G.YslLastLetter.ShowStats then
            local billboard = Instance.new("BillboardGui")
            billboard.Name = "YslESP"
            billboard.Parent = hrp
            billboard.Size = UDim2.new(0, 200, 0, 50)
            billboard.StudsOffset = Vector3.new(0, 3, 0)
            billboard.AlwaysOnTop = true
            
            local label = Instance.new("TextLabel")
            label.Parent = billboard
            label.Size = UDim2.new(1, 0, 1, 0)
            label.BackgroundTransparency = 1
            label.Text = player.Name .. "\nðŸ’° $" .. (player:GetAttribute("Cash") or 0)
            label.TextColor3 = Color3.fromRGB(255, 255, 255)
            label.TextScaled = true
            label.Font = Enum.Font.GothamBold
            label.TextStrokeTransparency = 0
        end
    end)
end

local function RemoveAllESP()
    for _, player in pairs(Players:GetPlayers()) do
        if player.Character then
            for _, obj in pairs(player.Character:GetDescendants()) do
                if obj.Name == "YslESP" then
                    obj:Destroy()
                end
            end
        end
    end
end

local function SetupPlayerESP()
    RemoveAllESP()
    
    if not _G.YslLastLetter.PlayerESP then
        Tasks.PlayerESP = false
        return
    end
    
    Tasks.PlayerESP = true
    
    task.spawn(function()
        while Tasks.PlayerESP and _G.YslLastLetter.PlayerESP do
            pcall(function()
                for _, player in pairs(Players:GetPlayers()) do
                    if player ~= lplayer then
                        CreateESP(player)
                    end
                end
            end)
            task.wait(3)
        end
    end)
    
    Notify("ðŸ‘ï¸ ESP", "Player ESP activated!")
end

--// AUTO-COLLECT REWARDS
local function SetupAutoCollectRewards()
    if not _G.YslLastLetter.AutoCollectRewards then
        Tasks.AutoRewards = false
        return
    end
    
    Tasks.AutoRewards = true
    
    task.spawn(function()
        while Tasks.AutoRewards and _G.YslLastLetter.AutoCollectRewards do
            pcall(function()
                -- Claim daily rewards
                FireRemote("ClaimDaily")
                
                -- Claim quest rewards
                FireRemote("ClaimQuest")
                
                -- Claim match rewards
                FireRemote("ClaimReward")
            end)
            
            task.wait(10)
        end
    end)
    
    Notify("ðŸŽ Rewards", "Auto-collect rewards activated!")
end

