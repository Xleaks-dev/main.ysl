-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local VirtualInputManager = game:GetService("VirtualInputManager")

-- Player
local lplayer = Players.LocalPlayer
local character = lplayer.Character or lplayer.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local hrp = character:WaitForChild("HumanoidRootPart")

-- Remotes
local RemoteFunctions = ReplicatedStorage:WaitForChild("RemoteFunctions")
local RemoteEvents = ReplicatedStorage:WaitForChild("RemoteEvents")

local Rebirth_RF = RemoteFunctions:WaitForChild("Rebirth")
local UpgradeSpeed_RF = RemoteFunctions:WaitForChild("UpgradeSpeed")
local UpgradeBrainrot_RF = RemoteFunctions:WaitForChild("UpgradeBrainrot")
local UpgradeBase_RF = RemoteFunctions:WaitForChild("UpgradeBase")
local SellAll_RF = RemoteFunctions:WaitForChild("SellAll")
local TakeFreeEpic_RF = RemoteFunctions:WaitForChild("TakeFreeEpic")
local BuyGear_RF = RemoteFunctions:WaitForChild("BuyGear")
local UpgradeCarry_RF = RemoteFunctions:WaitForChild("UpgradeCarry")

local CollectMoney_RE = RemoteEvents:WaitForChild("CollectMoney")
local DisplayNotification_RE = RemoteEvents:WaitForChild("DisplayNotification")

-- Global Table
_G.Ysl = _G.Ysl or {
    Enabled = true,
    
    -- Stats
    Stats = {
        Brainrots = 0,
        LuckyBlocks = 0,
        WavesBlocked = 0,
        SessionStart = os.time()
    },
    
    -- Settings
    RebirthAt = 500,
    UpgradeSpeedAt = 100,
    
    -- Toggles
    AutoFarm = {
        Brainrot = false,
        Money = false,
        LuckyBlocks = false,
        SellAll = false
    },
    
    AutoUpgrade = {
        Speed = false,
        Base = false,
        Brainrots = false,
        Carry = false
    },
    
    AutoRebirth = {
        Enabled = false,
        LastRebirth = 0
    },
    
    Movement = {
        Speed = false,
        SpeedValue = 100,
        Fly = false,
        FlySpeed = 50,
        NoClip = false,
        InfJump = false
    },
    
    Protection = {
        GodMode = false,
        AntiVoid = false,
        WaveImmunity = false,
        RemoveWaves = false
    },
    
    VIP = {
        RemoveWalls = false,
        TeleportPast = false
    },
    
    ESP = {
        Brainrots = false,
        LuckyBlocks = false,
        VIPWalls = false,
        Players = false
    },
    
    Teleport = {
        TargetLocation = nil
    }
}

-- Notification Function
local function Notify(title, text, duration)
    duration = duration or 5
    game.StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration,
        Icon = "rbxassetid://2541869220"
    })
end

-- Anti-AFK
for i,v in pairs(getconnections(lplayer.Idled)) do
    v:Disable()
end

-- Character Respawn Handler
lplayer.CharacterAdded:Connect(function(char)
    character = char
    humanoid = char:WaitForChild("Humanoid")
    hrp = char:WaitForChild("HumanoidRootPart")
    wait(1)
    Notify("YSL Menu", "Character Respawned - Features Reactivated!", 3)
end)

--[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    AUTO FARM FUNCTIONS
â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]

-- Auto Farm Brainrots (PERFECTED)
task.spawn(function()
    while task.wait(0.5) do
        if _G.Ysl.AutoFarm.Brainrot and _G.Ysl.Enabled then
            pcall(function()
                local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
                if activeBrainrots then
                    for _, rarityFolder in pairs(activeBrainrots:GetChildren()) do
                        if rarityFolder:IsA("Folder") then
                            for _, brainrotModel in pairs(rarityFolder:GetChildren()) do
                                if brainrotModel.Name == "RenderedBrainrot" and brainrotModel:IsA("Model") then
                                    for _, brainrot in pairs(brainrotModel:GetChildren()) do
                                        local prompt = brainrot:FindFirstChildWhichIsA("ProximityPrompt", true)
                                        if prompt and prompt.ActionText == "Take" then
                                            local dist = (brainrot:GetPivot().Position - hrp.Position).Magnitude
                                            if dist <= 200 then
                                                fireproximityprompt(prompt)
                                                _G.Ysl.Stats.Brainrots = _G.Ysl.Stats.Brainrots + 1
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
            end)
        end
    end
end)

-- Auto Collect Money (DUAL METHOD)
task.spawn(function()
    while task.wait(1) do
        if _G.Ysl.AutoFarm.Money and _G.Ysl.Enabled then
            pcall(function()
                -- Method 1: Click Collect Parts
                local bases = Workspace:FindFirstChild("Bases")
                if bases then
                    for _, base in pairs(bases:GetChildren()) do
                        local slots = base:FindFirstChild("Slots")
                        if slots then
                            for _, slot in pairs(slots:GetChildren()) do
                                local collect = slot:FindFirstChild("Collect")
                                if collect then
                                    local detector = collect:FindFirstChildOfClass("ClickDetector")
                                    if detector then
                                        fireclickdetector(detector)
                                    end
                                end
                            end
                        end
                    end
                end
                
                -- Method 2: Fire Remote
                CollectMoney_RE:FireServer()
            end)
        end
    end
end)

-- Auto Farm Lucky Blocks
task.spawn(function()
    while task.wait(0.3) do
        if _G.Ysl.AutoFarm.LuckyBlocks and _G.Ysl.Enabled then
            pcall(function()
                local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
                if luckyBlocks then
                    for _, block in pairs(luckyBlocks:GetChildren()) do
                        if block:IsA("Model") then
                            local prompt = block:FindFirstChildWhichIsA("ProximityPrompt", true)
                            if prompt then
                                local dist = (block:GetPivot().Position - hrp.Position).Magnitude
                                if dist <= 200 then
                                    fireproximityprompt(prompt)
                                    _G.Ysl.Stats.LuckyBlocks = _G.Ysl.Stats.LuckyBlocks + 1
                                end
                            end
                        end
                    end
                end
            end)
        end
    end
end)

-- Auto Sell All
task.spawn(function()
    while task.wait(60) do
        if _G.Ysl.AutoFarm.SellAll and _G.Ysl.Enabled then
            pcall(function()
                SellAll_RF:InvokeServer()
            end)
        end
    end
end)

--[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    AUTO UPGRADE FUNCTIONS
â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]

