local vipStatus = VIPTab:CreateLabel("Status: Inactive")

task.spawn(function()
    while task.wait(1) do
        if _G.Ysl.VIPBypass then
            local count = 0
            for _ in pairs(VIPBypass.ProcessedDoors) do count = count + 1 end
            local deathCount = 0
            for _ in pairs(VIPBypass.DeathWalls) do deathCount = deathCount + 1 end
            vipStatus:Set(string.format("âœ… Active | %d VIP + %d Death Walls Bypassed", count, deathCount))
        else
            vipStatus:Set("âŒ Inactive")
        end
    end
end)

