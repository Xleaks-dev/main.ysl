MovementTab:CreateButton({
    Name = "ğŸ  Teleport to Spawn",
    Callback = function()
        local spawn = Workspace:FindFirstChild("SpawnLocation")
        if spawn then
            rootPart.CFrame = spawn.CFrame + Vector3.new(0, 5, 0)
        else
            rootPart.CFrame = CFrame.new(0, 50, 0)
        end
    end,
})

MovementTab:CreateButton({
    Name = "ğŸ“ TP to Highest Point",
    Callback = function()
        local highest = nil
        local highestY = -math.huge
        
        for _, v in pairs(Workspace:GetDescendants()) do
            if v:IsA("BasePart") and v.Anchored and v.Position.Y > highestY then
                highestY = v.Position.Y
                highest = v
            end
        end
        
        if highest then
            rootPart.CFrame = CFrame.new(highest.Position + Vector3.new(0, 10, 0))
        end
    end,
})

