--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--//  ANTI-DETECTION & SECURITY LAYER
--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local Security = {}

function Security:Initialize()
    -- Anti-Kick Protection
    local mt = getrawmetatable(game)
    local oldNamecall = mt.__namecall
    local oldIndex = mt.__index
    
    setreadonly(mt, false)
    
    mt.__namecall = newcclosure(function(self, ...)
        local method = getnamecallmethod()
        local args = {...}
        
        -- Block kicks
        if method == "Kick" then
            return
        end
        
        -- Block teleports to lobby
        if method == "FireServer" or method == "InvokeServer" then
            local callingScript = getcallingscript()
            if callingScript then
                local scriptName = callingScript.Name:lower()
                if scriptName:find("anti") or scriptName:find("ban") or scriptName:find("kick") or scriptName:find("admin") then
                    return
                end
            end
        end
        
        return oldNamecall(self, ...)
    end)
    
    -- Spoof properties
    mt.__index = newcclosure(function(self, key)
        if key == "Parent" and self.ClassName == "Player" then
            return oldIndex(self, key)
        end
        return oldIndex(self, key)
    end)
    
    setreadonly(mt, true)
    
    -- Anti-AFK
    local VirtualUser = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:Connect(function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end)
    
    print("âœ… Security Layer Initialized")
end

pcall(function() Security:Initialize() end)

--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--//  PREMIUM KEY SYSTEM (SMALL & ADVANCED)
--// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local KeySystem = {}
KeySystem.ValidKeys = {
    ["ysl_free_ot"] = {tier = "Premium", color = Color3.fromRGB(0, 255, 127)},
    ["ysl_god_mode"] = {tier = "Ultimate", color = Color3.fromRGB(255, 0, 255)},
    ["ysl_admin_2026"] = {tier = "Developer", color = Color3.fromRGB(255, 215, 0)}
}

function KeySystem:CreateUI()
    local ScreenGui = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local UICorner = Instance.new("UICorner")
    local UIGradient = Instance.new("UIGradient")
    local Title = Instance.new("TextLabel")
    local KeyInput = Instance.new("TextBox")
    local InputCorner = Instance.new("UICorner")
    local SubmitBtn = Instance.new("TextButton")
    local BtnCorner = Instance.new("UICorner")
    local CopyBtn = Instance.new("TextButton")
    local CopyCorner = Instance.new("UICorner")
    local Status = Instance.new("TextLabel")
    
    -- Setup
    ScreenGui.Name = "YslKeyV110"
    ScreenGui.Parent = game.CoreGui
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    Frame.Parent = ScreenGui
    Frame.BackgroundColor3 = Color3.fromRGB(20, 20, 25)
    Frame.BorderSizePixel = 0
    Frame.Position = UDim2.new(0.5, -150, 0.5, -100)
    Frame.Size = UDim2.new(0, 300, 0, 200)
    Frame.Active = true
    Frame.Draggable = true
    
    UICorner.CornerRadius = UDim.new(0, 12)
    UICorner.Parent = Frame
    
    UIGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(30, 30, 40)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(15, 15, 20))
    }
    UIGradient.Rotation = 45
    UIGradient.Parent = Frame
    
    Title.Parent = Frame
    Title.BackgroundTransparency = 1
    Title.Size = UDim2.new(1, 0, 0, 50)
    Title.Font = Enum.Font.GothamBold
    Title.Text = "âš¡ YslMenu V110"
    Title.TextColor3 = Color3.fromRGB(0, 255, 127)
    Title.TextSize = 20
    Title.TextStrokeTransparency = 0.5
    
    KeyInput.Name = "KeyInput"
    KeyInput.Parent = Frame
    KeyInput.BackgroundColor3 = Color3.fromRGB(30, 30, 35)
    KeyInput.BorderSizePixel = 0
    KeyInput.Position = UDim2.new(0.1, 0, 0, 60)
    KeyInput.Size = UDim2.new(0.8, 0, 0, 35)
    KeyInput.Font = Enum.Font.Gotham
    KeyInput.PlaceholderText = "Enter Key..."
    KeyInput.Text = ""
    KeyInput.TextColor3 = Color3.fromRGB(255, 255, 255)
    KeyInput.TextSize = 14
    KeyInput.TextXAlignment = Enum.TextXAlignment.Center
    
    InputCorner.CornerRadius = UDim.new(0, 8)
    InputCorner.Parent = KeyInput
    
    SubmitBtn.Parent = Frame
    SubmitBtn.BackgroundColor3 = Color3.fromRGB(0, 255, 127)
    SubmitBtn.BorderSizePixel = 0
    SubmitBtn.Position = UDim2.new(0.1, 0, 0, 105)
    SubmitBtn.Size = UDim2.new(0.8, 0, 0, 30)
    SubmitBtn.Font = Enum.Font.GothamBold
    SubmitBtn.Text = "ğŸ”“ SUBMIT"
    SubmitBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
    SubmitBtn.TextSize = 14
    
    BtnCorner.CornerRadius = UDim.new(0, 8)
    BtnCorner.Parent = SubmitBtn
    
    CopyBtn.Parent = Frame
    CopyBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
    CopyBtn.BorderSizePixel = 0
    CopyBtn.Position = UDim2.new(0.1, 0, 0, 145)
    CopyBtn.Size = UDim2.new(0.8, 0, 0, 25)
    CopyBtn.Font = Enum.Font.Gotham
    CopyBtn.Text = "ğŸ“‹ Copy Key"
    CopyBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    CopyBtn.TextSize = 12
    
    CopyCorner.CornerRadius = UDim.new(0, 6)
    CopyCorner.Parent = CopyBtn
    
    Status.Parent = Frame
    Status.BackgroundTransparency = 1
    Status.Position = UDim2.new(0, 0, 0, 175)
    Status.Size = UDim2.new(1, 0, 0, 20)
    Status.Font = Enum.Font.Gotham
    Status.Text = "Waiting..."
    Status.TextColor3 = Color3.fromRGB(150, 150, 150)
    Status.TextSize = 11
    
    -- Animations
    local TweenService = game:GetService("TweenService")
    
    SubmitBtn.MouseEnter:Connect(function()
        TweenService:Create(SubmitBtn, TweenInfo.new(0.2), {Size = UDim2.new(0.8, 0, 0, 32)}):Play()
    end)
    
    SubmitBtn.MouseLeave:Connect(function()
        TweenService:Create(SubmitBtn, TweenInfo.new(0.2), {Size = UDim2.new(0.8, 0, 0, 30)}):Play()
    end)
    
    -- Copy button
    CopyBtn.MouseButton1Click:Connect(function()
        setclipboard("ysl_free_ot")
        Status.Text = "âœ… Key copied!"
        Status.TextColor3 = Color3.fromRGB(0, 255, 127)
        KeyInput.Text = "ysl_free_ot"
        wait(1.5)
        Status.Text = "Waiting..."
        Status.TextColor3 = Color3.fromRGB(150, 150, 150)
    end)
    
    -- Validate function
    local function Validate()
        local key = KeyInput.Text
        
        if KeySystem.ValidKeys[key] then
            local keyData = KeySystem.ValidKeys[key]
            Status.Text = "âœ… " .. keyData.tier .. " Validated!"
            Status.TextColor3 = keyData.color
            
            -- Success flash
            for i = 1, 2 do
                Frame.BackgroundColor3 = keyData.color
                wait(0.1)
                Frame.BackgroundColor3 = Color3.fromRGB(20, 20, 25)
                wait(0.1)
            end
            
            -- Fade out
            for i = 1, 10 do
                Frame.BackgroundTransparency = i / 10
                wait(0.02)
            end
            
            ScreenGui:Destroy()
            return true
        else
            Status.Text = "âŒ Invalid Key!"
            Status.TextColor3 = Color3.fromRGB(255, 0, 0)
            KeyInput.Text = ""
            
            -- Shake
            for i = 1, 3 do
                Frame.Position = Frame.Position + UDim2.new(0, 10, 0, 0)
                wait(0.05)
                Frame.Position = Frame.Position - UDim2.new(0, 20, 0, 0)
                wait(0.05)
                Frame.Position = Frame.Position + UDim2.new(0, 10, 0, 0)
            end
            
            wait(1)
            Status.Text = "Waiting..."
            Status.TextColor3 = Color3.fromRGB(150, 150, 150)
            return false
        end
    end
    
    SubmitBtn.MouseButton1Click:Connect(Validate)
    KeyInput.FocusLost:Connect(function(enter) if enter then Validate() end end)
    
    -- Wait for validation
    repeat wait() until not ScreenGui.Parent
end

-- Show key system
KeySystem:CreateUI()

