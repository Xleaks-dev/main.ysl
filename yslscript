    local function FormatTime(seconds)
        local hours = math.floor(seconds / 3600)
        local mins = math.floor((seconds % 3600) / 60)
        local secs = seconds % 60
        return string.format("%02d:%02d:%02d", hours, mins, secs)
    end
    
    local function GetSessionStats()
        local sessionTime = os.time() - _G.Ysl.Stats.SessionStart
        return {
            Time = FormatTime(sessionTime),
            Brainrots = _G.Ysl.Stats.Brainrots,
            LuckyBlocks = _G.Ysl.Stats.LuckyBlocks,
            WavesBlocked = _G.Ysl.Stats.WavesBlocked,
            MoneyCollected = _G.Ysl.Stats.MoneyCollected
        }
    end
    
