-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local CoreGui = game:GetService("CoreGui")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")

-- Player
local lplayer = Players.LocalPlayer
local mouse = lplayer:GetMouse()

-- Premium Key
local PREMIUM_KEY = "yslpm"
local KEY_VERIFIED = false

--[[ UTILITY FUNCTIONS ]]

local function CreateTween(object, properties, duration, style, direction)
    duration = duration or 0.3
    style = style or Enum.EasingStyle.Quad
    direction = direction or Enum.EasingDirection.Out
    
    local tween = TweenService:Create(object, TweenInfo.new(duration, style, direction), properties)
    tween:Play()
    return tween
end

local function MakeDraggable(frame, dragHandle)
    dragHandle = dragHandle or frame
    
    local dragging = false
    local dragInput
    local dragStart
    local startPos
    
    dragHandle.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = frame.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    dragHandle.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            local delta = input.Position - dragStart
            CreateTween(frame, {
                Position = UDim2.new(
                    startPos.X.Scale,
                    startPos.X.Offset + delta.X,
                    startPos.Y.Scale,
                    startPos.Y.Offset + delta.Y
                )
            }, 0.05, Enum.EasingStyle.Linear)
        end
    end)
end

local function AnimateGradient(object)
    local gradient = Instance.new("UIGradient")
    gradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 127)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(147, 51, 234)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(59, 130, 246))
    }
    gradient.Parent = object
    
    task.spawn(function()
        while gradient.Parent do
            CreateTween(gradient, {Rotation = 360}, 3, Enum.EasingStyle.Linear)
            task.wait(3)
            gradient.Rotation = 0
        end
    end)
    
    return gradient
end

--[[ KEY SYSTEM ]]

local function CreateKeySystem()
    local KeySystemUI = Instance.new("ScreenGui")
    KeySystemUI.Name = "YslKeySystem"
    KeySystemUI.Parent = CoreGui
    KeySystemUI.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    KeySystemUI.ResetOnSpawn = false
    
    local Blur = Instance.new("BlurEffect")
    Blur.Size = 0
    Blur.Parent = game.Lighting
    CreateTween(Blur, {Size = 24}, 0.8)
    
    local Overlay = Instance.new("Frame")
    Overlay.Size = UDim2.new(1, 0, 1, 0)
    Overlay.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    Overlay.BackgroundTransparency = 1
    Overlay.BorderSizePixel = 0
    Overlay.Parent = KeySystemUI
    CreateTween(Overlay, {BackgroundTransparency = 0.5}, 0.8)
    
    local MainContainer = Instance.new("Frame")
    MainContainer.Size = UDim2.new(0, 550, 0, 450)
    MainContainer.Position = UDim2.new(0.5, -275, 0.5, -225)
    MainContainer.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
    MainContainer.BackgroundTransparency = 0.05
    MainContainer.BorderSizePixel = 0
    MainContainer.ClipsDescendants = true
    MainContainer.Parent = KeySystemUI
    
    local MainCorner = Instance.new("UICorner")
    MainCorner.CornerRadius = UDim.new(0, 15)
    MainCorner.Parent = MainContainer
    
    local MainStroke = Instance.new("UIStroke")
    MainStroke.Color = Color3.fromRGB(255, 0, 127)
    MainStroke.Thickness = 2
    MainStroke.Parent = MainContainer
    
    task.spawn(function()
        local hue = 0
        while MainStroke.Parent do
            hue = (hue + 1) % 360
            MainStroke.Color = Color3.fromHSV(hue / 360, 1, 1)
            task.wait(0.03)
        end
    end)
    
    local TopBar = Instance.new("Frame")
    TopBar.Size = UDim2.new(1, 0, 0, 100)
    TopBar.BackgroundColor3 = Color3.fromRGB(20, 20, 28)
    TopBar.BorderSizePixel = 0
    TopBar.Parent = MainContainer
    
    local TopCorner = Instance.new("UICorner")
    TopCorner.CornerRadius = UDim.new(0, 15)
    TopCorner.Parent = TopBar
    
    AnimateGradient(TopBar)
    
    local Logo = Instance.new("TextLabel")
    Logo.Size = UDim2.new(1, 0, 1, 0)
    Logo.BackgroundTransparency = 1
    Logo.Text = "YSL SCRIPTS"
    Logo.TextSize = 36
    Logo.Font = Enum.Font.GothamBold
    Logo.TextColor3 = Color3.fromRGB(255, 255, 255)
    Logo.Parent = TopBar
    
    task.spawn(function()
        local hue = 0
        while Logo.Parent do
            hue = (hue + 2) % 360
            Logo.TextColor3 = Color3.fromHSV(hue / 360, 1, 1)
            task.wait(0.03)
        end
    end)
    
    local ContentArea = Instance.new("Frame")
    ContentArea.Size = UDim2.new(1, -40, 1, -120)
    ContentArea.Position = UDim2.new(0, 20, 0, 110)
    ContentArea.BackgroundTransparency = 1
    ContentArea.Parent = MainContainer
    
    local InfoText = Instance.new("TextLabel")
    InfoText.Size = UDim2.new(1, 0, 0, 40)
    InfoText.BackgroundTransparency = 1
    InfoText.Text = "üîê Enter Premium Key"
    InfoText.TextSize = 16
    InfoText.Font = Enum.Font.GothamBold
    InfoText.TextColor3 = Color3.fromRGB(200, 200, 200)
    InfoText.Parent = ContentArea
    
    local InputBox = Instance.new("Frame")
    InputBox.Size = UDim2.new(1, 0, 0, 60)
    InputBox.Position = UDim2.new(0, 0, 0, 60)
    InputBox.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
    InputBox.BorderSizePixel = 0
    InputBox.Parent = ContentArea
    
    local InputCorner = Instance.new("UICorner")
    InputCorner.CornerRadius = UDim.new(0, 10)
    InputCorner.Parent = InputBox
    
    local KeyInput = Instance.new("TextBox")
    KeyInput.Size = UDim2.new(1, -20, 1, -20)
    KeyInput.Position = UDim2.new(0, 10, 0, 10)
    KeyInput.BackgroundTransparency = 1
    KeyInput.PlaceholderText = "Enter key here..."
    KeyInput.PlaceholderColor3 = Color3.fromRGB(120, 120, 130)
    KeyInput.Text = ""
    KeyInput.TextSize = 18
    KeyInput.Font = Enum.Font.Gotham
    KeyInput.TextColor3 = Color3.fromRGB(255, 255, 255)
    KeyInput.Parent = InputBox
    
    local VerifyBtn = Instance.new("TextButton")
    VerifyBtn.Size = UDim2.new(1, 0, 0, 55)
    VerifyBtn.Position = UDim2.new(0, 0, 0, 140)
    VerifyBtn.BackgroundColor3 = Color3.fromRGB(147, 51, 234)
    VerifyBtn.BorderSizePixel = 0
    VerifyBtn.Text = "üöÄ VERIFY KEY"
    VerifyBtn.TextSize = 18
    VerifyBtn.Font = Enum.Font.GothamBold
    VerifyBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    VerifyBtn.Parent = ContentArea
    
    local BtnCorner = Instance.new("UICorner")
    BtnCorner.CornerRadius = UDim.new(0, 10)
    BtnCorner.Parent = VerifyBtn
    
    AnimateGradient(VerifyBtn)
    
    local Status = Instance.new("TextLabel")
    Status.Size = UDim2.new(1, 0, 0, 30)
    Status.Position = UDim2.new(0, 0, 0, 215)
    Status.BackgroundTransparency = 1
    Status.Text = ""
    Status.TextSize = 14
    Status.Font = Enum.Font.GothamBold
    Status.TextColor3 = Color3.fromRGB(255, 100, 100)
    Status.Parent = ContentArea
    
    local GetKeyBtn = Instance.new("TextButton")
    GetKeyBtn.Size = UDim2.new(1, 0, 0, 40)
    GetKeyBtn.Position = UDim2.new(0, 0, 0, 255)
    GetKeyBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    GetKeyBtn.BorderSizePixel = 0
    GetKeyBtn.Text = "üí° Get Key"
    GetKeyBtn.TextSize = 14
    GetKeyBtn.Font = Enum.Font.Gotham
    GetKeyBtn.TextColor3 = Color3.fromRGB(150, 150, 150)
    GetKeyBtn.Parent = ContentArea
    
    local GetKeyCorner = Instance.new("UICorner")
    GetKeyCorner.CornerRadius = UDim.new(0, 8)
    GetKeyCorner.Parent = GetKeyBtn
    
    GetKeyBtn.MouseButton1Click:Connect(function()
        Status.Text = "Key: ysl-ultimate-2026"
        Status.TextColor3 = Color3.fromRGB(100, 200, 255)
    end)
    
    MainContainer.Size = UDim2.new(0, 0, 0, 0)
    MainContainer.Position = UDim2.new(0.5, 0, 0.5, 0)
    CreateTween(MainContainer, {Size = UDim2.new(0, 550, 0, 450), Position = UDim2.new(0.5, -275, 0.5, -225)}, 1, Enum.EasingStyle.Elastic)
    
    MakeDraggable(MainContainer, TopBar)
    
    local function VerifyKey()
        if KeyInput.Text == PREMIUM_KEY then
            Status.Text = "‚úÖ VERIFIED!"
            Status.TextColor3 = Color3.fromRGB(0, 255, 127)
            task.wait(1)
            CreateTween(MainContainer, {Size = UDim2.new(0, 0, 0, 0), Position = UDim2.new(0.5, 0, 0.5, 0)}, 0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In)
            CreateTween(Overlay, {BackgroundTransparency = 1}, 0.5)
            CreateTween(Blur, {Size = 0}, 0.5)
            task.wait(0.6)
            KeySystemUI:Destroy()
            Blur:Destroy()
            KEY_VERIFIED = true
            loadMainScript()
        else
            Status.Text = "‚ùå INVALID KEY!"
            Status.TextColor3 = Color3.fromRGB(255, 50, 50)
        end
    end
    
    VerifyBtn.MouseButton1Click:Connect(VerifyKey)
    KeyInput.FocusLost:Connect(function(enterPressed)
        if enterPressed then VerifyKey() end
    end)
end

CreateKeySystem()

--[[ MAIN SCRIPT WITH TABBED UI ]]

function loadMainScript()
    
    local character = lplayer.Character or lplayer.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")
    local hrp = character:WaitForChild("HumanoidRootPart")
    
    local MyBase = nil
    task.spawn(function()
        task.wait(2)
        local bases = Workspace:FindFirstChild("Bases")
        if bases then
            local closestBase = nil
            local closestDist = math.huge
            for _, base in pairs(bases:GetChildren()) do
                local spawn = base:FindFirstChild("Spawn")
                if spawn then
                    local dist = (spawn.Position - hrp.Position).Magnitude
                    if dist < closestDist then
                        closestDist = dist
                        closestBase = base
                    end
                end
            end
            MyBase = closestBase
        end
    end)
    
    -- Remotes
    local RemoteFunctions = ReplicatedStorage:WaitForChild("RemoteFunctions")
    local RemoteEvents = ReplicatedStorage:WaitForChild("RemoteEvents")
    
    local Rebirth_RF = RemoteFunctions:WaitForChild("Rebirth")
    local UpgradeSpeed_RF = RemoteFunctions:WaitForChild("UpgradeSpeed")
    local UpgradeBrainrot_RF = RemoteFunctions:WaitForChild("UpgradeBrainrot")
    local UpgradeBase_RF = RemoteFunctions:WaitForChild("UpgradeBase")
    local SellAll_RF = RemoteFunctions:WaitForChild("SellAll")
    local UpgradeCarry_RF = RemoteFunctions:WaitForChild("UpgradeCarry")
    local CollectMoney_RE = RemoteEvents:WaitForChild("CollectMoney")
    
    -- Settings
    _G.Ysl = {
        Enabled = true,
        WalkSpeed = 16,
        WalkSpeedEnabled = false,
        RebirthAt = 500,
        AutoFarm = {
            Brainrot = false,
            Money = false,
            LuckyBlocks = false,
            SellAll = false
        },
        AutoUpgrade = {
            Base = false,
            Brainrots = false,
            Carry = false
        },
        AutoRebirth = false,
        Movement = {
            Fly = false,
            FlySpeed = 50,
            NoClip = false,
            InfJump = false
        },
        VIPBypass = false
    }
    
    local function Notify(text)
        game.StarterGui:SetCore("SendNotification", {
            Title = "YSL Scripts",
            Text = text,
            Duration = 3
        })
    end
    
    -- Anti-AFK
    for i,v in pairs(getconnections(lplayer.Idled)) do
        v:Disable()
    end
    
    -- Respawn handler
    lplayer.CharacterAdded:Connect(function(char)
        task.wait(0.5)
        character = char
        humanoid = char:WaitForChild("Humanoid")
        hrp = char:WaitForChild("HumanoidRootPart")
        
        task.wait(2)
        local bases = Workspace:FindFirstChild("Bases")
        if bases then
            local closestBase = nil
            local closestDist = math.huge
            for _, base in pairs(bases:GetChildren()) do
                local spawn = base:FindFirstChild("Spawn")
                if spawn then
                    local dist = (spawn.Position - hrp.Position).Magnitude
                    if dist < closestDist then
                        closestDist = dist
                        closestBase = base
                    end
                end
            end
            MyBase = closestBase
        end
        
        Notify("Respawned - Features Active")
    end)
    
    --[[ WALKSPEED CONTROL ]]
    
    local speedConnection = nil
    local function EnableWalkSpeed()
        if speedConnection then speedConnection:Disconnect() end
        
        speedConnection = RunService.Heartbeat:Connect(function()
            if _G.Ysl.WalkSpeedEnabled and humanoid and humanoid.Parent then
                humanoid.WalkSpeed = _G.Ysl.WalkSpeed
            end
        end)
    end
    
    local function DisableWalkSpeed()
        if speedConnection then
            speedConnection:Disconnect()
            speedConnection = nil
        end
        if humanoid then
            humanoid.WalkSpeed = 16
        end
    end
    
    --[[ AUTO FARM ]]
    
    task.spawn(function()
        while task.wait(0.15) do
            if _G.Ysl.AutoFarm.Brainrot and _G.Ysl.Enabled then
                pcall(function()
                    local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
                    if not activeBrainrots then return end
                    
                    local nearest = nil
                    local nearestDist = 500
                    
                    for _, folder in pairs(activeBrainrots:GetChildren()) do
                        for _, rendered in pairs(folder:GetChildren()) do
                            if rendered.Name == "RenderedBrainrot" then
                                for _, model in pairs(rendered:GetChildren()) do
                                    if model:IsA("Model") then
                                        for _, desc in pairs(model:GetDescendants()) do
                                            if desc.Name == "TakePrompt" and desc:IsA("ProximityPrompt") then
                                                local pos = model:GetPivot().Position
                                                local dist = (pos - hrp.Position).Magnitude
                                                if dist < nearestDist then
                                                    nearestDist = dist
                                                    nearest = {pos = pos, prompt = desc}
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                    
                    if nearest then
                        hrp.CFrame = CFrame.new(nearest.pos + Vector3.new(0, 2, 0))
                        task.wait(0.1)
                        fireproximityprompt(nearest.prompt)
                        task.wait(0.15)
                    end
                end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(0.2) do
            if _G.Ysl.AutoFarm.Money and _G.Ysl.Enabled and MyBase then
                pcall(function()
                    local slots = MyBase:FindFirstChild("Slots")
                    if not slots then return end
                    
                    for _, slot in pairs(slots:GetChildren()) do
                        if not _G.Ysl.AutoFarm.Money then break end
                        
                        local collect = slot:FindFirstChild("Collect")
                        if collect and collect:IsA("BasePart") then
                            hrp.CFrame = collect.CFrame * CFrame.new(0, 3, 0)
                            task.wait(0.1)
                            CollectMoney_RE:FireServer()
                            local detector = collect:FindFirstChildOfClass("ClickDetector")
                            if detector then fireclickdetector(detector) end
                            task.wait(0.15)
                        end
                    end
                end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(0.2) do
            if _G.Ysl.AutoFarm.LuckyBlocks and _G.Ysl.Enabled then
                pcall(function()
                    local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
                    if not luckyBlocks then return end
                    
                    local nearest = nil
                    local nearestDist = 500
                    
                    for _, block in pairs(luckyBlocks:GetChildren()) do
                        if block:IsA("Model") then
                            local prompt = block:FindFirstChildWhichIsA("ProximityPrompt", true)
                            if prompt then
                                local pos = block:GetPivot().Position
                                local dist = (pos - hrp.Position).Magnitude
                                if dist < nearestDist then
                                    nearestDist = dist
                                    nearest = {pos = pos, prompt = prompt}
                                end
                            end
                        end
                    end
                    
                    if nearest then
                        hrp.CFrame = CFrame.new(nearest.pos + Vector3.new(0, 2, 0))
                        task.wait(0.1)
                        for i = 1, 5 do
                            fireproximityprompt(nearest.prompt)
                            task.wait(0.02)
                        end
                        task.wait(0.15)
                    end
                end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(60) do
            if _G.Ysl.AutoFarm.SellAll and _G.Ysl.Enabled then
                pcall(function()
                    SellAll_RF:InvokeServer()
                    Notify("Auto Sold All!")
                end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(15) do
            if _G.Ysl.AutoUpgrade.Base then
                pcall(function() UpgradeBase_RF:InvokeServer() end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(15) do
            if _G.Ysl.AutoUpgrade.Carry then
                pcall(function() UpgradeCarry_RF:InvokeServer() end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(12) do
            if _G.Ysl.AutoUpgrade.Brainrots and MyBase then
                pcall(function()
                    for _, desc in pairs(MyBase:GetDescendants()) do
                        if desc.Name:match("slot %d+ brainrot") and desc:FindFirstChild("Root") then
                            pcall(function() UpgradeBrainrot_RF:InvokeServer(desc) end)
                            task.wait(0.1)
                        end
                    end
                end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(5) do
            if _G.Ysl.AutoRebirth and _G.Ysl.Enabled then
                pcall(function()
                    local speed = lplayer:GetAttribute("CurrentSpeed") or 16
                    if speed >= _G.Ysl.RebirthAt then
                        Rebirth_RF:InvokeServer()
                        Notify("Auto Rebirthed!")
                    end
                end)
            end
        end
    end)
    
    -- Fly
    local flying = false
    local flyBV, flyBG
    
    local function StartFly()
        flying = true
        
        flyBV = Instance.new("BodyVelocity")
        flyBV.Velocity = Vector3.zero
        flyBV.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        flyBV.Parent = hrp
        
        flyBG = Instance.new("BodyGyro")
        flyBG.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        flyBG.P = 9e4
        flyBG.Parent = hrp
        
        RunService.Heartbeat:Connect(function()
            if not _G.Ysl.Movement.Fly or not flying then
                if flyBV then flyBV:Destroy() end
                if flyBG then flyBG:Destroy() end
                flying = false
                return
            end
            
            if not hrp.Parent then return end
            
            local camera = workspace.CurrentCamera
            local dir = Vector3.zero
            
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then dir = dir + camera.CFrame.LookVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then dir = dir - camera.CFrame.LookVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then dir = dir - camera.CFrame.RightVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then dir = dir + camera.CFrame.RightVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then dir = dir + Vector3.new(0, 1, 0) end
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then dir = dir - Vector3.new(0, 1, 0) end
            
            flyBV.Velocity = dir.Magnitude > 0 and dir.Unit * _G.Ysl.Movement.FlySpeed or Vector3.zero
            flyBG.CFrame = camera.CFrame
        end)
    end
    
    -- NoClip
    RunService.Stepped:Connect(function()
        if _G.Ysl.Movement.NoClip and character then
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end)
    
    -- Inf Jump
    UserInputService.JumpRequest:Connect(function()
        if _G.Ysl.Movement.InfJump and humanoid then
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end)
    
    -- VIP Bypass
    task.spawn(function()
        while task.wait(1) do
            if _G.Ysl.VIPBypass then
                pcall(function()
                    local vipWalls = Workspace:FindFirstChild("VIPWalls")
                    if vipWalls then
                        for _, wall in pairs(vipWalls:GetChildren()) do
                            if wall:IsA("BasePart") then
                                wall.CanCollide = false
                                wall.Transparency = 0.8
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    --[[ ULTIMATE TABBED UI ]]
    
    local YslUI = Instance.new("ScreenGui")
    YslUI.Name = "YslUltimateUI"
    YslUI.Parent = CoreGui
    YslUI.ResetOnSpawn = false
    YslUI.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    -- Main Container
    local Main = Instance.new("Frame")
    Main.Name = "Main"
    Main.Size = UDim2.new(0, 700, 0, 500)
    Main.Position = UDim2.new(0.5, -350, 0.5, -250)
    Main.BackgroundColor3 = Color3.fromRGB(12, 12, 18)
    Main.BorderSizePixel = 0
    Main.ClipsDescendants = true
    Main.Parent = YslUI
    
    local MainCorner = Instance.new("UICorner")
    MainCorner.CornerRadius = UDim.new(0, 12)
    MainCorner.Parent = Main
    
    local MainStroke = Instance.new("UIStroke")
    MainStroke.Color = Color3.fromRGB(255, 0, 127)
    MainStroke.Thickness = 2
    MainStroke.Parent = Main
    
    task.spawn(function()
        local hue = 0
        while MainStroke.Parent do
            hue = (hue + 1) % 360
            MainStroke.Color = Color3.fromHSV(hue / 360, 1, 1)
            task.wait(0.03)
        end
    end)
    
