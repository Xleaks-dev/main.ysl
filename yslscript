    }, 1.2, Enum.EasingStyle.Elastic)
    
    -- Make Draggable
    MakeDraggable(MainContainer, TopBar)
    
    -- Verification Function
    local function VerifyKey()
        local key = KeyBox.Text
        
        if key == "" then
            StatusLabel.Text = "‚ö†Ô∏è Please enter a key!"
            StatusLabel.TextColor3 = Color3.fromRGB(255, 200, 0)
            
            -- Shake animation
            for i = 1, 3 do
                CreateTween(InputContainer, {Position = UDim2.new(0, 10, 0, 80)}, 0.05)
                task.wait(0.05)
                CreateTween(InputContainer, {Position = UDim2.new(0, -10, 0, 80)}, 0.05)
                task.wait(0.05)
            end
            CreateTween(InputContainer, {Position = UDim2.new(0, 0, 0, 80)}, 0.05)
            return
        end
        
        -- Verification animation
        SubmitText.Text = "‚è≥ VERIFYING..."
        SubmitButton.Active = false
        
        -- Spinner
        task.spawn(function()
            local rotation = 0
            for i = 1, 30 do
                rotation = rotation + 12
                SubmitButton.Rotation = rotation
                task.wait(0.03)
            end
            SubmitButton.Rotation = 0
        end)
        
        task.wait(1.5)
        
        if key == PREMIUM_KEY then
            -- Success!
            StatusLabel.Text = "‚úÖ KEY VERIFIED! Loading..."
            StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 127)
            SubmitText.Text = "‚úÖ SUCCESS!"
            
            -- Success particles
            for i = 1, 30 do
                local particle = Instance.new("Frame")
                particle.Size = UDim2.new(0, 10, 0, 10)
                particle.Position = UDim2.new(0.5, 0, 0.5, 0)
                particle.BackgroundColor3 = Color3.fromHSV(math.random() 0, 1, 1)
                particle.BorderSizePixel = 0
                particle.Parent = MainContainer
                
                local corner = Instance.new("UICorner")
                corner.CornerRadius = UDim.new(1, 0)
                corner.Parent = particle
                
                local angle = math.rad(math.random(0, 360))
                local distance = math.random(100, 300)
                
                CreateTween(particle, {
                    Position = UDim2.new(0.5, math.cos(angle) * distance, 0.5, math.sin(angle) * distance),
                    BackgroundTransparency = 1,
                    Rotation = math.random(-360, 360)
                }, 1, Enum.EasingStyle.Exponential)
                
                task.delay(1, function() particle:Destroy() end)
            end
            
            task.wait(1)
            
            -- Exit animation
            CreateTween(MainContainer, {
                Size = UDim2.new(0, 0, 0, 0),
                Position = UDim2.new(0.5, 0, 0.5, 0),
                Rotation = 180
            }, 0.8, Enum.EasingStyle.Back, Enum.EasingDirection.In)
            
            CreateTween(Overlay, {BackgroundTransparency = 1}, 0.8)
            CreateTween(Blur, {Size = 0}, 0.8)
            
            task.wait(0.9)
            KeySystemUI:Destroy()
            Blur:Destroy()
            
            KEY_VERIFIED = true
            loadMainScript()
        else
            -- Failed
            StatusLabel.Text = "‚ùå INVALID KEY!"
            StatusLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
            SubmitText.Text = "‚ùå TRY AGAIN"
            SubmitButton.Active = true
            
            -- Red flash
            local originalColor = InputStroke.Color
            InputStroke.Color = Color3.fromRGB(255, 0, 0)
            task.wait(0.5)
            InputStroke.Color = originalColor
        end
    end
    
    SubmitButton.MouseButton1Click:Connect(VerifyKey)
    KeyBox.FocusLost:Connect(function(enterPressed)
        if enterPressed then VerifyKey() end
    end)
end

-- Start Key System
CreatePremiumKeySystem()

--[[ MAIN ULTRA PREMIUM UI SCRIPT ]]

function loadMainScript()
    
    local character = lplayer.Character or lplayer.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")
    local hrp = character:WaitForChild("HumanoidRootPart")
    
    -- Store player's base
    local MyBase = nil
    task.spawn(function()
        task.wait(2)
        local bases = Workspace:FindFirstChild("Bases")
        if bases then
            local closestBase = nil
            local closestDist = math.huge
            for _, base in pairs(bases:GetChildren()) do
                local spawn = base:FindFirstChild("Spawn")
                if spawn then
                    local dist = (spawn.Position - hrp.Position).Magnitude
                    if dist < closestDist then
                        closestDist = dist
                        closestBase = base
                    end
                end
            end
            MyBase = closestBase
        end
    end)
    
    -- Remotes
    local RemoteFunctions = ReplicatedStorage:WaitForChild("RemoteFunctions")
    local RemoteEvents = ReplicatedStorage:WaitForChild("RemoteEvents")
    
    local Rebirth_RF = RemoteFunctions:WaitForChild("Rebirth")
    local UpgradeSpeed_RF = RemoteFunctions:WaitForChild("UpgradeSpeed")
    local UpgradeBrainrot_RF = RemoteFunctions:WaitForChild("UpgradeBrainrot")
    local UpgradeBase_RF = RemoteFunctions:WaitForChild("UpgradeBase")
    local SellAll_RF = RemoteFunctions:WaitForChild("SellAll")
    local UpgradeCarry_RF = RemoteFunctions:WaitForChild("UpgradeCarry")
    local CollectMoney_RE = RemoteEvents:WaitForChild("CollectMoney")
    
    -- Settings
    _G.YslPremium = {
        Enabled = true,
        WalkSpeed = 16,
        WalkSpeedEnabled = false,
        RebirthAt = 500,
        AutoFarm = {
            Brainrot = false,
            Money = false,
            LuckyBlocks = false,
            SellAll = false
        },
        AutoUpgrade = {
            Base = false,
            Brainrots = false,
            Carry = false
        },
        AutoRebirth = false,
        Movement = {
            Fly = false,
            FlySpeed = 50,
            NoClip = false,
            InfJump = false
        },
        VIPBypass = false
    }
    
    local function Notify(text, duration)
        game.StarterGui:SetCore("SendNotification", {
            Title = "YSL Premium",
            Text = text,
            Duration = duration or 4,
            Icon = "rbxassetid://7733992358"
        })
    end
    
    -- Anti-AFK
    for i,v in pairs(getconnections(lplayer.Idled)) do
        v:Disable()
    end
    
    -- Respawn handler
    lplayer.CharacterAdded:Connect(function(char)
        task.wait(0.5)
        character = char
        humanoid = char:WaitForChild("Humanoid")
        hrp = char:WaitForChild("HumanoidRootPart")
        
        task.wait(2)
        local bases = Workspace:FindFirstChild("Bases")
        if bases then
            local closestBase = nil
            local closestDist = math.huge
            for _, base in pairs(bases:GetChildren()) do
                local spawn = base:FindFirstChild("Spawn")
                if spawn then
                    local dist = (spawn.Position - hrp.Position).Magnitude
                    if dist < closestDist then
                        closestDist = dist
                        closestBase = base
                    end
                end
            end
            MyBase = closestBase
        end
        
        Notify("‚ú® Respawned - Premium Active")
    end)
    
    --[[ WALKSPEED CONTROL ]]
    
    local speedConnection = nil
    local function EnableWalkSpeed()
        if speedConnection then speedConnection:Disconnect() end
        
        speedConnection = RunService.Heartbeat:Connect(function()
            if _G.YslPremium.WalkSpeedEnabled and humanoid and humanoid.Parent then
                humanoid.WalkSpeed = _G.YslPremium.WalkSpeed
            end
        end)
    end
    
    local function DisableWalkSpeed()
        if speedConnection then
            speedConnection:Disconnect()
            speedConnection = nil
        end
        if humanoid then
            humanoid.WalkSpeed = 16
        end
    end
    
    --[[ AUTO FARM ]]
    
    -- Auto Farm Brainrots
    task.spawn(function()
        while task.wait(0.15) do
            if _G.YslPremium.AutoFarm.Brainrot and _G.YslPremium.Enabled then
                pcall(function()
                    local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
                    if not activeBrainrots then return end
                    
                    local nearest = nil
                    local nearestDist = 500
                    
                    for _, folder in pairs(activeBrainrots:GetChildren()) do
                        for _, rendered in pairs(folder:GetChildren()) do
                            if rendered.Name == "RenderedBrainrot" then
                                for _, model in pairs(rendered:GetChildren()) do
                                    if model:IsA("Model") then
                                        for _, desc in pairs(model:GetDescendants()) do
                                            if desc.Name == "TakePrompt" and desc:IsA("ProximityPrompt") then
                                                local pos = model:GetPivot().Position
                                                local dist = (pos - hrp.Position).Magnitude
                                                if dist < nearestDist then
                                                    nearestDist = dist
                                                    nearest = {pos = pos, prompt = desc}
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                    
                    if nearest then
                        hrp.CFrame = CFrame.new(nearest.pos + Vector3.new(0, 2, 0))
                        task.wait(0.1)
                        fireproximityprompt(nearest.prompt)
                        task.wait(0.15)
                    end
                end)
            end
        end
    end)
    
    -- Auto Collect Money
    task.spawn(function()
        while task.wait(0.2) do
            if _G.YslPremium.AutoFarm.Money and _G.YslPremium.Enabled and MyBase then
                pcall(function()
                    local slots = MyBase:FindFirstChild("Slots")
                    if not slots then return end
                    
                    for _, slot in pairs(slots:GetChildren()) do
                        if not _G.YslPremium.AutoFarm.Money then break end
                        
                        local collect = slot:FindFirstChild("Collect")
                        if collect and collect:IsA("BasePart") then
                            hrp.CFrame = collect.CFrame * CFrame.new(0, 3, 0)
                            task.wait(0.1)
                            CollectMoney_RE:FireServer()
                            local detector = collect:FindFirstChildOfClass("ClickDetector")
                            if detector then fireclickdetector(detector) end
                            task.wait(0.15)
                        end
                    end
                end)
            end
        end
    end)
    
    -- Auto Lucky Blocks
    task.spawn(function()
        while task.wait(0.2) do
            if _G.YslPremium.AutoFarm.LuckyBlocks and _G.YslPremium.Enabled then
                pcall(function()
                    local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
                    if not luckyBlocks then return end
                    
                    local nearest = nil
                    local nearestDist = 500
                    
                    for _, block in pairs(luckyBlocks:GetChildren()) do
                        if block:IsA("Model") then
                            local prompt = block:FindFirstChildWhichIsA("ProximityPrompt", true)
                            if prompt then
                                local pos = block:GetPivot().Position
                                local dist = (pos - hrp.Position).Magnitude
                                if dist < nearestDist then
                                    nearestDist = dist
                                    nearest = {pos = pos, prompt = prompt}
                                end
                            end
                        end
                    end
                    
                    if nearest then
                        hrp.CFrame = CFrame.new(nearest.pos + Vector3.new(0, 2, 0))
                        task.wait(0.1)
                        for i = 1, 5 do
                            fireproximityprompt(nearest.prompt)
                            task.wait(0.02)
                        end
                        task.wait(0.15)
                    end
                end)
            end
        end
    end)
    
    -- Auto Sell
    task.spawn(function()
        while task.wait(60) do
            if _G.YslPremium.AutoFarm.SellAll and _G.YslPremium.Enabled then
                pcall(function()
                    SellAll_RF:InvokeServer()
                    Notify("üí∞ Auto Sold All!")
                end)
            end
        end
    end)
    
    -- Auto Upgrades
    task.spawn(function()
        while task.wait(15) do
            if _G.YslPremium.AutoUpgrade.Base then
                pcall(function() UpgradeBase_RF:InvokeServer() end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(15) do
            if _G.YslPremium.AutoUpgrade.Carry then
                pcall(function() UpgradeCarry_RF:InvokeServer() end)
            end
        end
    end)
    
    task.spawn(function()
        while task.wait(12) do
            if _G.YslPremium.AutoUpgrade.Brainrots and MyBase then
                pcall(function()
                    for _, desc in pairs(MyBase:GetDescendants()) do
                        if desc.Name:match("slot %d+ brainrot") and desc:FindFirstChild("Root") then
                            pcall(function() UpgradeBrainrot_RF:InvokeServer(desc) end)
                            task.wait(0.1)
                        end
                    end
                end)
            end
        end
    end)
    
    -- Auto Rebirth
    task.spawn(function()
        while task.wait(5) do
            if _G.YslPremium.AutoRebirth and _G.YslPremium.Enabled then
                pcall(function()
                    local speed = lplayer:GetAttribute("CurrentSpeed") or 16
                    if speed >= _G.YslPremium.RebirthAt then
                        Rebirth_RF:InvokeServer()
                        Notify("üîÑ Auto Rebirthed!")
                    end
                end)
            end
        end
    end)
    
    -- Fly System
    local flying = false
    local flyBV, flyBG
    
    local function StartFly()
        flying = true
        
        flyBV = Instance.new("BodyVelocity")
        flyBV.Velocity = Vector3.zero
        flyBV.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        flyBV.Parent = hrp
        
        flyBG = Instance.new("BodyGyro")
        flyBG.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        flyBG.P = 9e4
        flyBG.Parent = hrp
        
        RunService.Heartbeat:Connect(function()
            if not _G.YslPremium.Movement.Fly or not flying then
                if flyBV then flyBV:Destroy() end
                if flyBG then flyBG:Destroy() end
                flying = false
                return
            end
            
            if not hrp.Parent then return end
            
            local camera = workspace.CurrentCamera
            local dir = Vector3.zero
            
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then dir = dir + camera.CFrame.LookVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then dir = dir - camera.CFrame.LookVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then dir = dir - camera.CFrame.RightVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then dir = dir + camera.CFrame.RightVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then dir = dir + Vector3.new(0, 1, 0) end
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then dir = dir - Vector3.new(0, 1, 0) end
            
            flyBV.Velocity = dir.Magnitude > 0 and dir.Unit * _G.YslPremium.Movement.FlySpeed or Vector3.zero
            flyBG.CFrame = camera.CFrame
        end)
    end
    
    -- NoClip
    RunService.Stepped:Connect(function()
        if _G.YslPremium.Movement.NoClip and character then
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end)
    
    -- Infinite Jump
    UserInputService.JumpRequest:Connect(function()
        if _G.YslPremium.Movement.InfJump and humanoid then
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end)
    
    -- VIP Bypass
    task.spawn(function()
        while task.wait(1) do
            if _G.YslPremium.VIPBypass then
                pcall(function()
                    local vipWalls = Workspace:FindFirstChild("VIPWalls")
                    if vipWalls then
                        for _, wall in pairs(vipWalls:GetChildren()) do
                            if wall:IsA("BasePart") then
                                wall.CanCollide = false
                                wall.Transparency = 0.8
                            end
                        end
                    end
                end)
            end
        end
    end)
    
