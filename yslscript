    return loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
end)

if not success then
    game.Players.LocalPlayer:Kick("\nâš¡ YslMenu V110\n\nFailed to load UI!\nRejoin to try again.")
    return
end

