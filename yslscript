-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local CoreGui = game:GetService("CoreGui")
local VirtualInputManager = game:GetService("VirtualInputManager")

-- Player
local lplayer = Players.LocalPlayer

--[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    PREMIUM KEY SYSTEM
â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]

local PREMIUM_KEY = "ysl-key-escapetsunami"
local KEY_VERIFIED = false

-- Create Premium Key UI
local function CreateKeyUI()
    local KeyUI = Instance.new("ScreenGui")
    KeyUI.Name = "YslKeySystem"
    KeyUI.Parent = CoreGui
    KeyUI.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    KeyUI.ResetOnSpawn = false
    
    -- Blur Effect
    local Blur = Instance.new("BlurEffect")
    Blur.Size = 20
    Blur.Parent = game.Lighting
    
    -- Background Frame
    local BG = Instance.new("Frame")
    BG.Size = UDim2.new(1, 0, 1, 0)
    BG.Position = UDim2.new(0, 0, 0, 0)
    BG.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    BG.BackgroundTransparency = 0.3
    BG.BorderSizePixel = 0
    BG.Parent = KeyUI
    
    -- Main Frame
    local MainFrame = Instance.new("Frame")
    MainFrame.Size = UDim2.new(0, 500, 0, 350)
    MainFrame.Position = UDim2.new(0.5, -250, 0.5, -175)
    MainFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
    MainFrame.BorderSizePixel = 0
    MainFrame.Parent = KeyUI
    
    local UICorner = Instance.new("UICorner")
    UICorner.CornerRadius = UDim.new(0, 20)
    UICorner.Parent = MainFrame
    
    local UIStroke = Instance.new("UIStroke")
    UIStroke.Color = Color3.fromRGB(255, 0, 127)
    UIStroke.Thickness = 3
    UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    UIStroke.Parent = MainFrame
    
    -- Gradient
    local Gradient = Instance.new("UIGradient")
    Gradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(25, 25, 35)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(10, 10, 15))
    }
    Gradient.Rotation = 45
    Gradient.Parent = MainFrame
    
    -- Title Frame
    local TitleFrame = Instance.new("Frame")
    TitleFrame.Size = UDim2.new(1, 0, 0, 80)
    TitleFrame.Position = UDim2.new(0, 0, 0, 0)
    TitleFrame.BackgroundColor3 = Color3.fromRGB(255, 0, 127)
    TitleFrame.BorderSizePixel = 0
    TitleFrame.Parent = MainFrame
    
    local TitleCorner = Instance.new("UICorner")
    TitleCorner.CornerRadius = UDim.new(0, 20)
    TitleCorner.Parent = TitleFrame
    
    -- Title
    local Title = Instance.new("TextLabel")
    Title.Size = UDim2.new(1, 0, 0, 50)
    Title.Position = UDim2.new(0, 0, 0, 10)
    Title.BackgroundTransparency = 1
    Title.Text = "ğŸ” YSL PREMIUM ACCESS"
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextSize = 28
    Title.Font = Enum.Font.GothamBold
    Title.Parent = TitleFrame
    
    -- Subtitle
    local Subtitle = Instance.new("TextLabel")
    Subtitle.Size = UDim2.new(1, 0, 0, 25)
    Subtitle.Position = UDim2.new(0, 0, 0, 55)
    Subtitle.BackgroundTransparency = 1
    Title.Text = "ğŸ” YSL PREMIUM ACCESS"
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextSize = 28
    Title.Font = Enum.Font.GothamBold
    Title.Parent = TitleFrame
    
    -- Subtitle
    local Subtitle = Instance.new("TextLabel")
    Subtitle.Size = UDim2.new(1, 0, 0, 25)
    Subtitle.Position = UDim2.new(0, 0, 0, 55)
    Subtitle.BackgroundTransparency = 1
    Subtitle.Text = "V116 ULTIMATE PERFECTION"
    Subtitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    Subtitle.TextSize = 14
    Subtitle.Font = Enum.Font.Gotham
    Subtitle.Parent = TitleFrame
    
    -- Info Text
    local InfoText = Instance.new("TextLabel")
    InfoText.Size = UDim2.new(1, -40, 0, 60)
    InfoText.Position = UDim2.new(0, 20, 0, 100)
    InfoText.BackgroundTransparency = 1
    InfoText.Text = "Enter your premium key to unlock\nthe most powerful Escape Tsunami script ever created"
    InfoText.TextColor3 = Color3.fromRGB(200, 200, 200)
    InfoText.TextSize = 15
    InfoText.Font = Enum.Font.Gotham
    InfoText.TextWrapped = true
    InfoText.Parent = MainFrame
    
    -- Key Input Box
    local KeyBox = Instance.new("TextBox")
    KeyBox.Size = UDim2.new(0, 400, 0, 50)
    KeyBox.Position = UDim2.new(0.5, -200, 0, 180)
    KeyBox.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
    KeyBox.BorderSizePixel = 0
    KeyBox.PlaceholderText = "Enter Premium Key Here..."
    KeyBox.PlaceholderColor3 = Color3.fromRGB(100, 100, 100)
    KeyBox.Text = ""
    KeyBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    KeyBox.TextSize = 18
    KeyBox.Font = Enum.Font.GothamBold
    KeyBox.ClearTextOnFocus = false
    KeyBox.Parent = MainFrame
    
    local KeyBoxCorner = Instance.new("UICorner")
    KeyBoxCorner.CornerRadius = UDim.new(0, 10)
    KeyBoxCorner.Parent = KeyBox
    
    local KeyBoxStroke = Instance.new("UIStroke")
    KeyBoxStroke.Color = Color3.fromRGB(255, 0, 127)
    KeyBoxStroke.Thickness = 2
    KeyBoxStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    KeyBoxStroke.Parent = KeyBox
    
    -- Submit Button
    local SubmitButton = Instance.new("TextButton")
    SubmitButton.Size = UDim2.new(0, 180, 0, 50)
    SubmitButton.Position = UDim2.new(0.5, -90, 0, 250)
    SubmitButton.BackgroundColor3 = Color3.fromRGB(255, 0, 127)
    SubmitButton.BorderSizePixel = 0
    SubmitButton.Text = "VERIFY KEY"
    SubmitButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    SubmitButton.TextSize = 18
    SubmitButton.Font = Enum.Font.GothamBold
    SubmitButton.Parent = MainFrame
    
    local SubmitCorner = Instance.new("UICorner")
    SubmitCorner.CornerRadius = UDim.new(0, 10)
    SubmitCorner.Parent = SubmitButton
    
    -- Status Label
    local StatusLabel = Instance.new("TextLabel")
    StatusLabel.Size = UDim2.new(1, -40, 0, 30)
    StatusLabel.Position = UDim2.new(0, 20, 0, 310)
    StatusLabel.BackgroundTransparency = 1
    StatusLabel.Text = ""
    StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    StatusLabel.TextSize = 14
    StatusLabel.Font = Enum.Font.GothamBold
    StatusLabel.Parent = MainFrame
    
    -- Animations
    MainFrame.Size = UDim2.new(0, 0, 0, 0)
    MainFrame:TweenSize(
        UDim2.new(0, 500, 0, 350),
        Enum.EasingDirection.Out,
        Enum.EasingStyle.Elastic,
        1,
        true
    )
    
    -- Button hover effects
    SubmitButton.MouseEnter:Connect(function()
        TweenService:Create(SubmitButton, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(255, 50, 150),
            Size = UDim2.new(0, 190, 0, 55)
        }):Play()
    end)
    
    SubmitButton.MouseLeave:Connect(function()
        TweenService:Create(SubmitButton, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(255, 0, 127),
            Size = UDim2.new(0, 180, 0, 50)
        }):Play()
    end)
    
    -- Key verification function
    local function VerifyKey()
        local enteredKey = KeyBox.Text
        
        if enteredKey == PREMIUM_KEY then
            StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
            StatusLabel.Text = "âœ… KEY VERIFIED! Loading Ultimate Features..."
            
            -- Success animation
            TweenService:Create(UIStroke, TweenInfo.new(0.3), {
                Color = Color3.fromRGB(0, 255, 127)
            }):Play()
            
            TweenService:Create(TitleFrame, TweenInfo.new(0.3), {
                BackgroundColor3 = Color3.fromRGB(0, 255, 127)
            }):Play()
            
            wait(1.5)
            
            -- Exit animation
            TweenService:Create(MainFrame, TweenInfo.new(0.5), {
                Size = UDim2.new(0, 0, 0, 0)
            }):Play()
            
            TweenService:Create(Blur, TweenInfo.new(0.5), {
                Size = 0
            }):Play()
            
            wait(0.6)
            KeyUI:Destroy()
            Blur:Destroy()
            
            KEY_VERIFIED = true
            loadMainScript()
        else
            StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
            StatusLabel.Text = "âŒ INVALID KEY! Access Denied."
            
            -- Shake animation
            for i = 1, 4 do
                MainFrame.Position = UDim2.new(0.5, -260, 0.5, -175)
                wait(0.05)
                MainFrame.Position = UDim2.new(0.5, -240, 0.5, -175)
                wait(0.05)
            end
            MainFrame.Position = UDim2.new(0.5, -250, 0.5, -175)
            
            KeyBox.Text = ""
            
            -- Red flash
            TweenService:Create(KeyBoxStroke, TweenInfo.new(0.3), {
                Color = Color3.fromRGB(255, 0, 0)
            }):Play()
            
            wait(0.5)
            
            TweenService:Create(KeyBoxStroke, TweenInfo.new(0.3), {
                Color = Color3.fromRGB(255, 0, 127)
            }):Play()
        end
    end
    
    SubmitButton.MouseButton1Click:Connect(VerifyKey)
    KeyBox.FocusLost:Connect(function(enterPressed)
        if enterPressed then
            VerifyKey()
        end
    end)
end

CreateKeyUI()

--[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    MAIN SCRIPT LOADER
â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]

function loadMainScript()
    
    wait(0.5)
    
    print([[
â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
â•‘               LOADING ULTIMATE FEATURES...                    â•‘
â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
]])
    
    -- Get character
    local character = lplayer.Character or lplayer.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")
    local hrp = character:WaitForChild("HumanoidRootPart")
    
    -- Remotes
    local RemoteFunctions = ReplicatedStorage:WaitForChild("RemoteFunctions")
    local RemoteEvents = ReplicatedStorage:WaitForChild("RemoteEvents")
    
    local Rebirth_RF = RemoteFunctions:WaitForChild("Rebirth")
    local UpgradeSpeed_RF = RemoteFunctions:WaitForChild("UpgradeSpeed")
    local UpgradeBrainrot_RF = RemoteFunctions:WaitForChild("UpgradeBrainrot")
    local UpgradeBase_RF = RemoteFunctions:WaitForChild("UpgradeBase")
    local SellAll_RF = RemoteFunctions:WaitForChild("SellAll")
    local TakeFreeEpic_RF = RemoteFunctions:WaitForChild("TakeFreeEpic")
    local UpgradeCarry_RF = RemoteFunctions:WaitForChild("UpgradeCarry")
    
    local CollectMoney_RE = RemoteEvents:WaitForChild("CollectMoney")
    
    -- Global Settings
    _G.Ysl = _G.Ysl or {
        Enabled = true,
        
        Stats = {
            Brainrots = 0,
            LuckyBlocks = 0,
            WavesBlocked = 0,
            MoneyCollected = 0,
            SessionStart = os.time()
        },
        
        RebirthAt = 500,
        
        AutoFarm = {
            Brainrot = false,
            Money = false,
            LuckyBlocks = false,
            SellAll = false
        },
        
        AutoUpgrade = {
            Base = false,
            Brainrots = false,
            Carry = false
        },
        
        AutoRebirth = {
            Enabled = false,
            LastRebirth = 0
        },
        
        Movement = {
            Speed = false,
            SpeedValue = 100,
            Fly = false,
            FlySpeed = 50,
            NoClip = false,
            InfJump = false
        },
        
        Protection = {
            GodMode = false,
            AntiVoid = false,
            WaveImmunity = false,
            AutoRemoveWaves = false
        },
        
        ESP = {
            Brainrots = false,
            LuckyBlocks = false,
            VIPWalls = false,
            Players = false
        },
        
        Misc = {
            AntiAFK = true
        }
    }
    
    -- Notification System
    local function Notify(title, text, duration)
        duration = duration or 4
        game.StarterGui:SetCore("SendNotification", {
            Title = "YSL | " .. title,
            Text = text,
            Duration = duration,
            Icon = "rbxassetid://7733964640"
        })
    end
    
    -- Anti-AFK (Enhanced)
    task.spawn(function()
        for i,v in pairs(getconnections(lplayer.Idled)) do
            v:Disable()
        end
        
        -- Virtual input every 5 minutes
        while wait(300) do
            if _G.Ysl.Misc.AntiAFK then
                VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.W, false, game)
                wait(0.1)
                VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.W, false, game)
            end
        end
    end)
    
    -- Character Respawn Handler
    lplayer.CharacterAdded:Connect(function(char)
        wait(0.5)
        character = char
        humanoid = char:WaitForChild("Humanoid")
        hrp = char:WaitForChild("HumanoidRootPart")
        Notify("Respawn", "Character respawned! Features reactivated.", 3)
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    ULTIMATE AUTO FARM SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    -- Auto Farm Brainrots (ULTIMATE VERSION - 100% WORKING)
    local BrainrotFarmConnection
    local function StartBrainrotFarm()
        if BrainrotFarmConnection then return end
        
        BrainrotFarmConnection = task.spawn(function()
            while task.wait(0.2) do
                if not _G.Ysl.AutoFarm.Brainrot or not _G.Ysl.Enabled then
                    task.wait(0.5)
                    continue
                end
                
                local success, err = pcall(function()
                    local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
                    if not activeBrainrots then return end
                    
                    local nearestBrainrot = nil
                    local nearestDistance = 200
                    
                    -- Find nearest brainrot
                    for _, rarityFolder in pairs(activeBrainrots:GetChildren()) do
                        if not rarityFolder:IsA("Folder") then continue end
                        
                        for _, renderedModel in pairs(rarityFolder:GetChildren()) do
                            if renderedModel.Name ~= "RenderedBrainrot" then continue end
                            if not renderedModel:IsA("Model") then continue end
                            
                            -- Find the actual brainrot model inside
                            for _, brainrotModel in pairs(renderedModel:GetChildren()) do
                                if not brainrotModel:IsA("Model") then continue end
                                
                                -- Find TakePrompt
                                for _, desc in pairs(brainrotModel:GetDescendants()) do
                                    if desc.Name == "TakePrompt" and desc:IsA("ProximityPrompt") then
                                        local modelPivot = brainrotModel:GetPivot()
                                        if modelPivot then
                                            local distance = (modelPivot.Position - hrp.Position).Magnitude
                                            
                                            if distance < nearestDistance then
                                                nearestDistance = distance
                                                nearestBrainrot = {
                                                    model = brainrotModel,
                                                    prompt = desc,
                                                    position = modelPivot.Position
                                                }
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                    
                    -- Collect nearest brainrot
                    if nearestBrainrot then
                        -- Teleport to brainrot
                        hrp.CFrame = CFrame.new(nearestBrainrot.position + Vector3.new(0, 5, 0))
                        task.wait(0.15)
                        
                        -- Fire prompt multiple times for reliability
                        for i = 1, 3 do
                            fireproximityprompt(nearestBrainrot.prompt)
                            task.wait(0.05)
                        end
                        
                        _G.Ysl.Stats.Brainrots = _G.Ysl.Stats.Brainrots + 1
                        task.wait(0.3)
                    end
                end)
                
                if not success then
                    warn("Brainrot farm error:", err)
                end
            end
        end)
    end
    
    -- Auto Collect Money (ULTIMATE VERSION - 100% WORKING)
    local MoneyFarmConnection
    local function StartMoneyFarm()
        if MoneyFarmConnection then return end
        
        MoneyFarmConnection = task.spawn(function()
            while task.wait(0.4) do
                if not _G.Ysl.AutoFarm.Money or not _G.Ysl.Enabled then
                    task.wait(0.5)
                    continue
                end
                
                local success, err = pcall(function()
                    -- Method 1: Fire CollectMoney RemoteEvent
                    CollectMoney_RE:FireServer()
                    
                    -- Method 2: Click all Collect parts with ClickDetectors
                    local bases = Workspace:FindFirstChild("Bases")
                    if not bases then return end
                    
                    local collectedCount = 0
                    for _, base in pairs(bases:GetChildren()) do
                        local slots = base:FindFirstChild("Slots")
                        if slots then
                            for _, slot in pairs(slots:GetChildren()) do
                                local collect = slot:FindFirstChild("Collect")
                                if collect and collect:IsA("BasePart") then
                                    local clickDetector = collect:FindFirstChildOfClass("ClickDetector")
                                    if clickDetector then
                                        fireclickdetector(clickDetector)
                                        collectedCount = collectedCount + 1
                                    end
                                end
                            end
                        end
                    end
                    
                    if collectedCount > 0 then
                        _G.Ysl.Stats.MoneyCollected = _G.Ysl.Stats.MoneyCollected + collectedCount
                    end
                end)
                
                if not success then
                    warn("Money farm error:", err)
                end
            end
        end)
    end
    
    -- Auto Farm Lucky Blocks (ULTIMATE VERSION - 100% WORKING)
    local LuckyBlockFarmConnection
    local function StartLuckyBlockFarm()
        if LuckyBlockFarmConnection then return end
        
        LuckyBlockFarmConnection = task.spawn(function()
            while task.wait(0.3) do
                if not _G.Ysl.AutoFarm.LuckyBlocks or not _G.Ysl.Enabled then
                    task.wait(0.5)
                    continue
                end
                
                local success, err = pcall(function()
                    local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
                    if not luckyBlocks then return end
                    
                    local nearestBlock = nil
                    local nearestDistance = 200
                    
                    -- Find nearest lucky block
                    for _, block in pairs(luckyBlocks:GetChildren()) do
                        if not block:IsA("Model") then continue end
                        
                        local prompt = block:FindFirstChildWhichIsA("ProximityPrompt", true)
                        if prompt then
                            local blockPivot = block:GetPivot()
                            if blockPivot then
                                local distance = (blockPivot.Position - hrp.Position).Magnitude
                                
                                if distance < nearestDistance then
                                    nearestDistance = distance
                                    nearestBlock = {
                                        model = block,
                                        prompt = prompt,
                                        position = blockPivot.Position
                                    }
                                end
                            end
                        end
                    end
                    
                    -- Collect nearest lucky block
                    if nearestBlock then
                        -- Teleport to lucky block
                        hrp.CFrame = CFrame.new(nearestBlock.position + Vector3.new(0, 5, 0))
                        task.wait(0.15)
                        
                        -- Fire prompt multiple times
                        for i = 1, 3 do
                            fireproximityprompt(nearestBlock.prompt)
                            task.wait(0.05)
                        end
                        
                        _G.Ysl.Stats.LuckyBlocks = _G.Ysl.Stats.LuckyBlocks + 1
                        task.wait(0.3)
                    end
                end)
                
                if not success then
                    warn("Lucky block farm error:", err)
                end
            end
        end)
    end
    
    -- Auto Sell All
    task.spawn(function()
        while task.wait(60) do
            if _G.Ysl.AutoFarm.SellAll and _G.Ysl.Enabled then
                pcall(function()
                    SellAll_RF:InvokeServer()
                end)
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    MANUAL SPEED UPGRADE SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    local function UpgradeSpeedManual(times)
        task.spawn(function()
            local successCount = 0
            for i = 1, times do
                local success, result = pcall(function()
                    return UpgradeSpeed_RF:InvokeServer()
                end)
                
                if success and result then
                    successCount = successCount + 1
                    task.wait(0.15)
                else
                    Notify("Speed Upgrade", "Failed at upgrade " .. i .. "/" .. times, 3)
                    break
                end
            end
            
            if successCount > 0 then
                local currentSpeed = lplayer:GetAttribute("CurrentSpeed") or 16
                Notify("Speed Upgrade", string.format("Upgraded %d/%d times! Speed: %d", successCount, times, currentSpeed), 3)
            end
        end)
    end
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    AUTO UPGRADE SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    -- Auto Upgrade Base
    task.spawn(function()
        while task.wait(12) do
            if _G.Ysl.AutoUpgrade.Base and _G.Ysl.Enabled then
                pcall(function()
                    UpgradeBase_RF:InvokeServer()
                end)
            end
        end
    end)
    
    -- Auto Upgrade Carry
    task.spawn(function()
        while task.wait(12) do
            if _G.Ysl.AutoUpgrade.Carry and _G.Ysl.Enabled then
                pcall(function()
                    UpgradeCarry_RF:InvokeServer()
                end)
            end
        end
    end)
    
    -- Auto Upgrade All Brainrots
    task.spawn(function()
        while task.wait(10) do
            if _G.Ysl.AutoUpgrade.Brainrots and _G.Ysl.Enabled then
                pcall(function()
                    local bases = Workspace:FindFirstChild("Bases")
                    if not bases then return end
                    
                    for _, base in pairs(bases:GetChildren()) do
                        -- Find all slot brainrots
                        for _, desc in pairs(base:GetDescendants()) do
                            if desc.Name:match("^slot %d+") and desc:IsA("Model") then
                                if desc:FindFirstChild("Root") then
                                    local success = pcall(function()
                                        UpgradeBrainrot_RF:InvokeServer(desc)
                                    end)
                                    if success then
                                        task.wait(0.2)
                                    end
                                end
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    SMART AUTO REBIRTH SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    task.spawn(function()
        while task.wait(5) do
            if _G.Ysl.AutoRebirth.Enabled and _G.Ysl.Enabled then
                pcall(function()
                    local currentTime = os.time()
                    local timeSinceLastRebirth = currentTime - _G.Ysl.AutoRebirth.LastRebirth
                    
                    -- 10 second cooldown
                    if timeSinceLastRebirth >= 10 then
                        local currentSpeed = lplayer:GetAttribute("CurrentSpeed") or 16
                        
                        if currentSpeed >= _G.Ysl.RebirthAt then
                            local success, result = pcall(function()
                                return Rebirth_RF:InvokeServer()
                            end)
                            
                            if success and result then
                                _G.Ysl.AutoRebirth.LastRebirth = currentTime
                                Notify("Auto Rebirth", "Rebirthed at Speed: " .. currentSpeed, 3)
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    ULTIMATE MOVEMENT SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    -- Speed System (Perfect Implementation)
    local speedConnection
    speedConnection = RunService.Heartbeat:Connect(function()
        if not character or not character.Parent then return end
        if not humanoid or not humanoid.Parent then return end
        
        if _G.Ysl.Movement.Speed and _G.Ysl.Enabled then
            humanoid.WalkSpeed = _G.Ysl.Movement.SpeedValue
        else
            if humanoid.WalkSpeed ~= 16 then
                humanoid.WalkSpeed = 16
            end
        end
    end)
    
    -- Fly System (Complete Rewrite - 100% Working)
    local flying = false
    local flyBodyVelocity = nil
    local flyBodyGyro = nil
    local flyConnection = nil
    
    local function StartFly()
        if flying then return end
        flying = true
        
        -- Clean up old instances
        if flyBodyVelocity then flyBodyVelocity:Destroy() end
        if flyBodyGyro then flyBodyGyro:Destroy() end
        
        -- Create new body movers
        flyBodyVelocity = Instance.new("BodyVelocity")
        flyBodyVelocity.Velocity = Vector3.zero
        flyBodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        flyBodyVelocity.P = 9e4
        flyBodyVelocity.Parent = hrp
        
        flyBodyGyro = Instance.new("BodyGyro")
        flyBodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        flyBodyGyro.P = 9e4
        flyBodyGyro.D = 500
        flyBodyGyro.Parent = hrp
        
        -- Fly control loop
        flyConnection = RunService.Heartbeat:Connect(function()
            if not _G.Ysl.Movement.Fly or not flying then
                StopFly()
                return
            end
            
            if not hrp or not hrp.Parent then
                StopFly()
                return
            end
            
            pcall(function()
                local camera = workspace.CurrentCamera
                if not camera then return end
                
                local moveDirection = Vector3.zero
                
                -- WASD Controls
                if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                    moveDirection = moveDirection + camera.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                    moveDirection = moveDirection - camera.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                    moveDirection = moveDirection - camera.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                    moveDirection = moveDirection + camera.CFrame.RightVector
                end
                
                -- Space/Shift for up/down
                if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                    moveDirection = moveDirection + Vector3.new(0, 1, 0)
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) or UserInputService:IsKeyDown(Enum.KeyCode.RightShift) then
                    moveDirection = moveDirection - Vector3.new(0, 1, 0)
                end
                
                -- Apply velocity
                if moveDirection.Magnitude > 0 then
                    flyBodyVelocity.Velocity = moveDirection.Unit * _G.Ysl.Movement.FlySpeed
                else
                    flyBodyVelocity.Velocity = Vector3.zero
                end
                
                -- Apply gyro
                flyBodyGyro.CFrame = camera.CFrame
            end)
        end)
        
        Notify("Fly", "Enabled! WASD + Space/Shift to control", 3)
    end
    
    function StopFly()
        flying = false
        
        if flyConnection then
            flyConnection:Disconnect()
            flyConnection = nil
        end
        
        if flyBodyVelocity then
            flyBodyVelocity:Destroy()
            flyBodyVelocity = nil
        end
        
        if flyBodyGyro then
            flyBodyGyro:Destroy()
            flyBodyGyro = nil
        end
    end
    
    -- NoClip System (Perfect Implementation)
    local noclipConnection
    noclipConnection = RunService.Stepped:Connect(function()
        if not character or not character.Parent then return end
        
        if _G.Ysl.Movement.NoClip and _G.Ysl.Enabled then
            pcall(function()
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end)
        end
    end)
    
    -- Infinite Jump System (Perfect Implementation)
    local infJumpConnection
    infJumpConnection = UserInputService.JumpRequest:Connect(function()
        if _G.Ysl.Movement.InfJump and _G.Ysl.Enabled then
            if humanoid and humanoid.Parent then
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    ULTIMATE PROTECTION SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    -- God Mode (Instant Heal)
    local godModeConnection
    godModeConnection = RunService.Heartbeat:Connect(function()
        if not character or not character.Parent then return end
        if not humanoid or not humanoid.Parent then return end
        
        if _G.Ysl.Protection.GodMode and _G.Ysl.Enabled then
            if humanoid.Health < humanoid.MaxHealth then
                humanoid.Health = humanoid.MaxHealth
            end
        end
    end)
    
    -- Anti-Void (Teleport to Safety)
    task.spawn(function()
        while task.wait(0.5) do
            if _G.Ysl.Protection.AntiVoid and _G.Ysl.Enabled then
                if hrp and hrp.Parent then
                    if hrp.Position.Y < -50 then
                        pcall(function()
                            -- Find spawn
                            local bases = Workspace:FindFirstChild("Bases")
                            if bases then
                                for _, base in pairs(bases:GetChildren()) do
                                    local spawn = base:FindFirstChild("Spawn")
                                    if spawn and spawn:IsA("BasePart") then
                                        hrp.CFrame = spawn.CFrame + Vector3.new(0, 10, 0)
                                        humanoid.Health = humanoid.MaxHealth
                                        Notify("Anti-Void", "Saved from the void!", 2)
                                        break
                                    end
                                end
                            end
                        end)
                    end
                end
            end
        end
    end)
    
    -- Wave Immunity (Ultimate Protection)
    local waveImmunityConnection
    waveImmunityConnection = RunService.Heartbeat:Connect(function()
        if not character or not character.Parent then return end
        if not humanoid or not humanoid.Parent then return end
        
        if _G.Ysl.Protection.WaveImmunity and _G.Ysl.Enabled then
            pcall(function()
                -- Instant heal
                if humanoid.Health < humanoid.MaxHealth then
                    humanoid.Health = humanoid.MaxHealth
                    _G.Ysl.Stats.WavesBlocked = _G.Ysl.Stats.WavesBlocked + 1
                end
                
                -- Disable harmful states
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming, false)
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
                humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
                
                -- Make waves non-collidable
                local activeTsunamis = Workspace:FindFirstChild("ActiveTsunamis")
                if activeTsunamis then
                    for _, tsunami in pairs(activeTsunamis:GetChildren()) do
                        for _, part in pairs(tsunami:GetDescendants()) do
                            if part:IsA("BasePart") and (part.Position - hrp.Position).Magnitude <= 100 then
                                part.CanCollide = false
                                part.CanTouch = false
                                part.CanQuery = false
                            end
                        end
                    end
                end
                
                -- Make lava non-collidable
                local lava = Workspace:FindFirstChild("Lava")
                if lava then
                    for _, part in pairs(lava:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                            part.CanTouch = false
                            part.CanQuery = false
                        end
                    end
                end
            end)
        end
    end)
    
    -- Auto Remove Waves
    task.spawn(function()
        while task.wait(5) do
            if _G.Ysl.Protection.AutoRemoveWaves and _G.Ysl.Enabled then
                pcall(function()
                    local tsunamis = Workspace:FindFirstChild("ActiveTsunamis")
                    if tsunamis then
                        tsunamis:ClearAllChildren()
                    end
                end)
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    VIP BYPASS SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    local function RemoveVIPWalls()
        pcall(function()
            local vipWalls = Workspace:FindFirstChild("VIPWalls")
            if vipWalls then
                local count = 0
                for _, wall in pairs(vipWalls:GetChildren()) do
                    if wall:IsA("BasePart") then
                        wall:Destroy()
                        count = count + 1
                    end
                end
                if count > 0 then
                    Notify("VIP Bypass", count .. " VIP walls removed!", 2)
                end
            end
        end)
    end
    
    local function TeleportPastVIP()
        pcall(function()
            local vipWalls = Workspace:FindFirstChild("VIPWalls")
            if vipWalls then
                local furthest = nil
                local furthestDist = 0
                
                for _, wall in pairs(vipWalls:GetChildren()) do
                    if wall:IsA("BasePart") then
                        local dist = (wall.Position - Vector3.zero).Magnitude
                        if dist > furthestDist then
                            furthestDist = dist
                            furthest = wall
                        end
                    end
                end
                
                if furthest then
                    hrp.CFrame = furthest.CFrame + furthest.CFrame.LookVector * 20 + Vector3.new(0, 10, 0)
                    Notify("VIP Bypass", "Teleported past VIP area!", 2)
                end
            end
        end)
    end
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    ULTIMATE ESP SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    local ESPObjects = {}
    local ESPConnections = {}
    
    local function CreateESP(obj, color, text, offset)
        if not obj or not obj.Parent then return end
        if ESPObjects[obj] then return end
        
        offset = offset or Vector3.new(0, 3, 0)
        
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "YslESP"
        billboard.AlwaysOnTop = true
        billboard.Size = UDim2.new(0, 120, 0, 60)
        billboard.StudsOffset = offset
        billboard.Parent = obj
        
        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(1, 0, 1, 0)
        frame.BackgroundTransparency = 0.4
        frame.BackgroundColor3 = color
        frame.BorderSizePixel = 0
        frame.Parent = billboard
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 10)
        corner.Parent = frame
        
        local stroke = Instance.new("UIStroke")
        stroke.Color = color
        stroke.Thickness = 2
        stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        stroke.Parent = frame
        
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.Text = text
        label.TextColor3 = Color3.new(1, 1, 1)
        label.TextScaled = true
        label.Font = Enum.Font.GothamBold
        label.TextStrokeTransparency = 0.3
        label.Parent = frame
        
        ESPObjects[obj] = billboard
    end
    
    local function RemoveESP(obj)
        if ESPObjects[obj] then
            ESPObjects[obj]:Destroy()
            ESPObjects[obj] = nil
        end
    end
    
    local function ClearAllESP()
        for obj, billboard in pairs(ESPObjects) do
            billboard:Destroy()
        end
        ESPObjects = {}
    end
    
    -- Brainrot ESP
    local function UpdateBrainrotESP()
        if _G.Ysl.ESP.Brainrots then
            pcall(function()
                local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
                if not activeBrainrots then return end
                
                for _, rarityFolder in pairs(activeBrainrots:GetChildren()) do
                    if not rarityFolder:IsA("Folder") then continue end
                    
                    local color = Color3.fromRGB(255, 255, 255)
                    local rarity = rarityFolder.Name
                    
                    if rarity == "Legendary" then
                        color = Color3.fromRGB(255, 170, 0)
                    elseif rarity == "Epic" then
                        color = Color3.fromRGB(163, 53, 238)
                    elseif rarity == "Rare" then
                        color = Color3.fromRGB(0, 170, 255)
                    elseif rarity == "Cosmic" then
                        color = Color3.fromRGB(255, 0, 255)
                    elseif rarity == "Secret" then
                        color = Color3.fromRGB(255, 255, 0)
                    elseif rarity == "Mythical" then
                        color = Color3.fromRGB(255, 0, 0)
                    elseif rarity == "Divine" then
                        color = Color3.fromRGB(0, 255, 255)
                    end
                    
                    for _, model in pairs(rarityFolder:GetChildren()) do
                        if model.Name == "RenderedBrainrot" and model:IsA("Model") then
                            CreateESP(model, color, rarity, Vector3.new(0, 5, 0))
                        end
                    end
                end
            end)
        else
            -- Remove all brainrot ESP
            local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
            if activeBrainrots then
                for _, rarityFolder in pairs(activeBrainrots:GetDescendants()) do
                    if rarityFolder.Name == "RenderedBrainrot" then
                        RemoveESP(rarityFolder)
                    end
                end
            end
        end
    end
    
    -- Lucky Block ESP
    local function UpdateLuckyBlockESP()
        if _G.Ysl.ESP.LuckyBlocks then
            pcall(function()
                local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
                if not luckyBlocks then return end
                
                for _, block in pairs(luckyBlocks:GetChildren()) do
                    if block:IsA("Model") then
                        CreateESP(block, Color3.fromRGB(255, 215, 0), "LUCKY", Vector3.new(0, 4, 0))
                    end
                end
            end)
        else
            local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
            if luckyBlocks then
                for _, block in pairs(luckyBlocks:GetChildren()) do
                    RemoveESP(block)
                end
            end
        end
    end
    
    -- VIP Wall ESP
    local function UpdateVIPWallESP()
        if _G.Ysl.ESP.VIPWalls then
            pcall(function()
                local vipWalls = Workspace:FindFirstChild("VIPWalls")
                if not vipWalls then return end
                
                for _, wall in pairs(vipWalls:GetChildren()) do
                    if wall:IsA("BasePart") then
                        CreateESP(wall, Color3.fromRGB(255, 0, 0), "VIP", Vector3.new(0, 2, 0))
                    end
                end
            end)
        else
            local vipWalls = Workspace:FindFirstChild("VIPWalls")
            if vipWalls then
                for _, wall in pairs(vipWalls:GetChildren()) do
                    RemoveESP(wall)
                end
            end
        end
    end
    
    -- Player ESP
    local function UpdatePlayerESP()
        if _G.Ysl.ESP.Players then
            pcall(function()
                for _, player in pairs(Players:GetPlayers()) do
                    if player == lplayer then continue end
                    if not player.Character then continue end
                    
                    local head = player.Character:FindFirstChild("Head")
                    if head then
                        CreateESP(head, Color3.fromRGB(0, 255, 0), player.Name, Vector3.new(0, 2, 0))
                    end
                end
            end)
        else
            for _, player in pairs(Players:GetPlayers()) do
                if player.Character then
                    local head = player.Character:FindFirstChild("Head")
                    if head then
                        RemoveESP(head)
                    end
                end
            end
        end
    end
    
    -- ESP Update Loop
    task.spawn(function()
        while task.wait(2) do
            UpdateBrainrotESP()
            UpdateLuckyBlockESP()
            UpdateVIPWallESP()
            UpdatePlayerESP()
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    TELEPORT SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    local teleportLocations = {
        Spawn = function()
            local bases = Workspace:FindFirstChild("Bases")
            if bases then
                for _, base in pairs(bases:GetChildren()) do
                    local spawn = base:FindFirstChild("Spawn")
                    if spawn and spawn:IsA("BasePart") then
                        return spawn.CFrame + Vector3.new(0, 5, 0)
                    end
                end
            end
        end,
        
        SellPoint = function()
            local sellPoint = Workspace:FindFirstChild("SellPoint")
            if sellPoint and sellPoint:FindFirstChild("Seller") then
                return sellPoint.Seller:GetPivot() + Vector3.new(0, 5, 0)
            end
        end,
        
        UpgradeShop = function()
            local shop = Workspace:FindFirstChild("UpgradeShop")
            if shop then
                return shop:GetPivot() + Vector3.new(0, 5, 0)
            end
        end,
        
        MoneyWheel = function()
            local wheel = Workspace:FindFirstChild("MoneySpinWheel")
            if wheel then
                return wheel:GetPivot() + Vector3.new(0, 5, 0)
            end
        end,
        
        NearestBrainrot = function()
            local nearest = nil
            local nearestDist = math.huge
            
            local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
            if activeBrainrots then
                for _, rarityFolder in pairs(activeBrainrots:GetChildren()) do
                    for _, model in pairs(rarityFolder:GetDescendants()) do
                        if model.Name == "RenderedBrainrot" and model:IsA("Model") then
                            local pivot = model:GetPivot()
                            if pivot then
                                local dist = (pivot.Position - hrp.Position).Magnitude
                                if dist < nearestDist then
                                    nearestDist = dist
                                    nearest = pivot.Position
                                end
                            end
                        end
                    end
                end
            end
            
            if nearest then
                return CFrame.new(nearest + Vector3.new(0, 5, 0))
            end
        end,
        
        NearestLuckyBlock = function()
            local nearest = nil
            local nearestDist = math.huge
            
            local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
            if luckyBlocks then
                for _, block in pairs(luckyBlocks:GetChildren()) do
                    if block:IsA("Model") then
                        local pivot = block:GetPivot()
                        if pivot then
                            local dist = (pivot.Position - hrp.Position).Magnitude
                            if dist < nearestDist then
                                nearestDist = dist
                                nearest = pivot.Position
                            end
                        end
                    end
                end
            end
            
            if nearest then
                return CFrame.new(nearest + Vector3.new(0, 5, 0))
            end
        end
    }
    
    local function TeleportTo(location)
        pcall(function()
            local func = teleportLocations[location]
            if func then
                local cframe = func()
                if cframe then
                    hrp.CFrame = cframe
                    Notify("Teleport", "Teleported to " .. location, 2)
                else
                    Notify("Teleport", "Location not found!", 2)
                end
            end
        end)
    end
    
