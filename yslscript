StatusLabel.TextSize = 13
StatusLabel.Font = Enum.Font.Gotham
StatusLabel.Parent = MainFrame

-- Animation
local function AnimateFrame()
    MainFrame.Size = UDim2.new(0, 0, 0, 0)
    MainFrame:TweenSize(
        UDim2.new(0, 450, 0, 300),
        Enum.EasingDirection.Out,
        Enum.EasingStyle.Elastic,
        0.8,
        true
    )
end

AnimateFrame()

-- Button Hover Effect
SubmitButton.MouseEnter:Connect(function()
    TweenService:Create(SubmitButton, TweenInfo.new(0.3), {
        BackgroundColor3 = Color3.fromRGB(255, 20, 147)
    }):Play()
end)

SubmitButton.MouseLeave:Connect(function()
    TweenService:Create(SubmitButton, TweenInfo.new(0.3), {
        BackgroundColor3 = Color3.fromRGB(255, 0, 127)
    }):Play()
end)

-- Key Verification
local function VerifyKey()
    local enteredKey = KeyBox.Text
    
    if enteredKey == PREMIUM_KEY then
        StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        StatusLabel.Text = "âœ… KEY VERIFIED! Loading Premium Features..."
        
        wait(1.5)
        
        -- Smooth exit animation
        TweenService:Create(MainFrame, TweenInfo.new(0.5), {
            Size = UDim2.new(0, 0, 0, 0)
        }):Play()
        
        TweenService:Create(Blur, TweenInfo.new(0.5), {
            Size = 0
        }):Play()
        
        wait(0.6)
        KeyUI:Destroy()
        Blur:Destroy()
        
        KEY_VERIFIED = true
        
        -- Load main script
        loadMainScript()
    else
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        StatusLabel.Text = "âŒ Invalid Key! Access Denied."
        
        -- Shake animation
        for i = 1, 3 do
            MainFrame.Position = UDim2.new(0.5, -230, 0.5, -150)
            wait(0.05)
            MainFrame.Position = UDim2.new(0.5, -220, 0.5, -150)
            wait(0.05)
        end
        MainFrame.Position = UDim2.new(0.5, -225, 0.5, -150)
        
        KeyBox.Text = ""
    end
end

SubmitButton.MouseButton1Click:Connect(VerifyKey)
KeyBox.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        VerifyKey()
    end
end)

--[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                    MAIN SCRIPT (LOADS AFTER KEY VERIFICATION)
â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]

function loadMainScript()
    
    -- Get character
    local character = lplayer.Character or lplayer.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")
    local hrp = character:WaitForChild("HumanoidRootPart")
    
    -- Remotes
    local RemoteFunctions = ReplicatedStorage:WaitForChild("RemoteFunctions")
    local RemoteEvents = ReplicatedStorage:WaitForChild("RemoteEvents")
    
    local Rebirth_RF = RemoteFunctions:WaitForChild("Rebirth")
    local UpgradeSpeed_RF = RemoteFunctions:WaitForChild("UpgradeSpeed")
    local UpgradeBrainrot_RF = RemoteFunctions:WaitForChild("UpgradeBrainrot")
    local UpgradeBase_RF = RemoteFunctions:WaitForChild("UpgradeBase")
    local SellAll_RF = RemoteFunctions:WaitForChild("SellAll")
    local TakeFreeEpic_RF = RemoteFunctions:WaitForChild("TakeFreeEpic")
    local UpgradeCarry_RF = RemoteFunctions:WaitForChild("UpgradeCarry")
    
    local CollectMoney_RE = RemoteEvents:WaitForChild("CollectMoney")
    
    -- Global Settings
    _G.Ysl = _G.Ysl or {
        Enabled = true,
        Stats = {
            Brainrots = 0,
            LuckyBlocks = 0,
            WavesBlocked = 0,
            SessionStart = os.time()
        },
        RebirthAt = 500,
        AutoFarm = {
            Brainrot = false,
            Money = false,
            LuckyBlocks = false,
            SellAll = false
        },
        AutoUpgrade = {
            Base = false,
            Brainrots = false,
            Carry = false
        },
        AutoRebirth = {
            Enabled = false,
            LastRebirth = 0
        },
        Movement = {
            Speed = false,
            SpeedValue = 100,
            Fly = false,
            FlySpeed = 50,
            NoClip = false,
            InfJump = false
        },
        Protection = {
            GodMode = false,
            AntiVoid = false,
            WaveImmunity = false
        },
        VIP = {
            RemoveWalls = false
        },
        ESP = {
            Brainrots = false,
            LuckyBlocks = false,
            VIPWalls = false,
            Players = false
        }
    }
    
    -- Notification Function
    local function Notify(title, text, duration)
        duration = duration or 5
        game.StarterGui:SetCore("SendNotification", {
            Title = title,
            Text = text,
            Duration = duration,
            Icon = "rbxassetid://7733964640"
        })
    end
    
    -- Anti-AFK
    for i,v in pairs(getconnections(lplayer.Idled)) do
        v:Disable()
    end
    
    -- Character Respawn Handler
    lplayer.CharacterAdded:Connect(function(char)
        character = char
        humanoid = char:WaitForChild("Humanoid")
        hrp = char:WaitForChild("HumanoidRootPart")
        wait(0.5)
        Notify("YSL Premium", "Character respawned! Features reactivated.", 3)
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                        FIXED AUTO FARM SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    -- Auto Farm Brainrots (COMPLETELY FIXED)
    task.spawn(function()
        while true do
            task.wait(0.3)
            if _G.Ysl.AutoFarm.Brainrot and _G.Ysl.Enabled then
                pcall(function()
                    local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
                    if not activeBrainrots then return end
                    
                    for _, rarityFolder in pairs(activeBrainrots:GetChildren()) do
                        if rarityFolder:IsA("Folder") then
                            for _, model in pairs(rarityFolder:GetChildren()) do
                                if model:IsA("Model") and model.Name == "RenderedBrainrot" then
                                    -- Find the actual brainrot model inside
                                    for _, brainrot in pairs(model:GetChildren()) do
                                        if brainrot:IsA("Model") then
                                            -- Find TakePrompt
                                            local prompt = brainrot:FindFirstChild("TakePrompt", true)
                                            if prompt and prompt:IsA("ProximityPrompt") then
                                                local modelPos = brainrot:GetPivot().Position
                                                local dist = (modelPos - hrp.Position).Magnitude
                                                
                                                if dist <= 150 then
                                                    -- Teleport to brainrot
                                                    hrp.CFrame = CFrame.new(modelPos + Vector3.new(0, 3, 0))
                                                    task.wait(0.1)
                                                    
                                                    -- Fire prompt
                                                    fireproximityprompt(prompt)
                                                    _G.Ysl.Stats.Brainrots = _G.Ysl.Stats.Brainrots + 1
                                                    task.wait(0.2)
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    -- Auto Collect Money (COMPLETELY FIXED)
    task.spawn(function()
        while true do
            task.wait(0.5)
            if _G.Ysl.AutoFarm.Money and _G.Ysl.Enabled then
                pcall(function()
                    -- Method 1: Fire CollectMoney event
                    CollectMoney_RE:FireServer()
                    
                    -- Method 2: Click all Collect parts
                    local bases = Workspace:FindFirstChild("Bases")
                    if bases then
                        for _, base in pairs(bases:GetChildren()) do
                            local slots = base:FindFirstChild("Slots")
                            if slots then
                                for _, slot in pairs(slots:GetChildren()) do
                                    local collect = slot:FindFirstChild("Collect")
                                    if collect and collect:IsA("BasePart") then
                                        local clickDetector = collect:FindFirstChildOfClass("ClickDetector")
                                        if clickDetector then
                                            fireclickdetector(clickDetector)
                                        end
                                    end
                                end
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    -- Auto Farm Lucky Blocks (COMPLETELY FIXED)
    task.spawn(function()
        while true do
            task.wait(0.4)
            if _G.Ysl.AutoFarm.LuckyBlocks and _G.Ysl.Enabled then
                pcall(function()
                    local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
                    if not luckyBlocks then return end
                    
                    for _, block in pairs(luckyBlocks:GetChildren()) do
                        if block:IsA("Model") then
                            local prompt = block:FindFirstChildWhichIsA("ProximityPrompt", true)
                            if prompt then
                                local blockPos = block:GetPivot().Position
                                local dist = (blockPos - hrp.Position).Magnitude
                                
                                if dist <= 150 then
                                    hrp.CFrame = CFrame.new(blockPos + Vector3.new(0, 3, 0))
                                    task.wait(0.1)
                                    fireproximityprompt(prompt)
                                    _G.Ysl.Stats.LuckyBlocks = _G.Ysl.Stats.LuckyBlocks + 1
                                    task.wait(0.2)
                                end
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    -- Auto Sell All
    task.spawn(function()
        while true do
            task.wait(60)
            if _G.Ysl.AutoFarm.SellAll and _G.Ysl.Enabled then
                pcall(function()
                    SellAll_RF:InvokeServer()
                end)
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                        MANUAL SPEED UPGRADE FUNCTIONS
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    local function UpgradeSpeedManual(times)
        pcall(function()
            for i = 1, times do
                local success = UpgradeSpeed_RF:InvokeServer()
                if not success then
                    Notify("Speed Upgrade", "Failed at upgrade " .. i .. "/" .. times, 3)
                    break
                end
                task.wait(0.1)
            end
            local currentSpeed = lplayer:GetAttribute("CurrentSpeed") or 16
            Notify("Speed Upgrade", "Upgraded " .. times .. " times! Current: " .. currentSpeed, 3)
        end)
    end
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                        AUTO UPGRADE FUNCTIONS
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    -- Auto Upgrade Base
    task.spawn(function()
        while true do
            task.wait(10)
            if _G.Ysl.AutoUpgrade.Base and _G.Ysl.Enabled then
                pcall(function()
                    UpgradeBase_RF:InvokeServer()
                end)
            end
        end
    end)
    
    -- Auto Upgrade Carry
    task.spawn(function()
        while true do
            task.wait(10)
            if _G.Ysl.AutoUpgrade.Carry and _G.Ysl.Enabled then
                pcall(function()
                    UpgradeCarry_RF:InvokeServer()
                end)
            end
        end
    end)
    
    -- Auto Upgrade Brainrots
    task.spawn(function()
        while true do
            task.wait(8)
            if _G.Ysl.AutoUpgrade.Brainrots and _G.Ysl.Enabled then
                pcall(function()
                    local bases = Workspace:FindFirstChild("Bases")
                    if bases then
                        for _, base in pairs(bases:GetChildren()) do
                            for _, obj in pairs(base:GetDescendants()) do
                                if obj.Name:match("slot") and obj:FindFirstChild("Root") then
                                    UpgradeBrainrot_RF:InvokeServer(obj)
                                    task.wait(0.1)
                                end
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                        AUTO REBIRTH (SMART MODE)
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    task.spawn(function()
        while true do
            task.wait(5)
            if _G.Ysl.AutoRebirth.Enabled and _G.Ysl.Enabled then
                pcall(function()
                    local currentTime = os.time()
                    local timeSinceLastRebirth = currentTime - _G.Ysl.AutoRebirth.LastRebirth
                    
                    if timeSinceLastRebirth >= 10 then
                        local currentSpeed = lplayer:GetAttribute("CurrentSpeed") or 16
                        
                        if currentSpeed >= _G.Ysl.RebirthAt then
                            local success = Rebirth_RF:InvokeServer()
                            if success then
                                _G.Ysl.AutoRebirth.LastRebirth = currentTime
                                Notify("Auto Rebirth", "Rebirthed at Speed: " .. currentSpeed, 3)
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                        FIXED MOVEMENT SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    -- Speed (FIXED)
    RunService.Heartbeat:Connect(function()
        if _G.Ysl.Movement.Speed and _G.Ysl.Enabled and humanoid then
            humanoid.WalkSpeed = _G.Ysl.Movement.SpeedValue
        elseif humanoid and not _G.Ysl.Movement.Speed then
            humanoid.WalkSpeed = 16
        end
    end)
    
    -- Fly (COMPLETELY FIXED)
    local flying = false
    local flyBodyVelocity, flyBodyGyro
    
    local function StartFly()
        if flying then return end
        flying = true
        
        -- Create body movers
        flyBodyVelocity = Instance.new("BodyVelocity")
        flyBodyVelocity.Velocity = Vector3.zero
        flyBodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        flyBodyVelocity.Parent = hrp
        
        flyBodyGyro = Instance.new("BodyGyro")
        flyBodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        flyBodyGyro.P = 9e4
        flyBodyGyro.Parent = hrp
        
        -- Fly loop
        task.spawn(function()
            while flying and _G.Ysl.Movement.Fly do
                pcall(function()
                    local camera = workspace.CurrentCamera
                    local moveDirection = Vector3.zero
                    
                    if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                        moveDirection += camera.CFrame.LookVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                        moveDirection -= camera.CFrame.LookVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                        moveDirection -= camera.CFrame.RightVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                        moveDirection += camera.CFrame.RightVector
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                        moveDirection += Vector3.new(0, 1, 0)
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                        moveDirection -= Vector3.new(0, 1, 0)
                    end
                    
                    if moveDirection.Magnitude > 0 then
                        flyBodyVelocity.Velocity = moveDirection.Unit * _G.Ysl.Movement.FlySpeed
                    else
                        flyBodyVelocity.Velocity = Vector3.zero
                    end
                    
                    flyBodyGyro.CFrame = camera.CFrame
                end)
                task.wait()
            end
        end)
    end
    
    local function StopFly()
        flying = false
        if flyBodyVelocity then flyBodyVelocity:Destroy() end
        if flyBodyGyro then flyBodyGyro:Destroy() end
        flyBodyVelocity = nil
        flyBodyGyro = nil
    end
    
    -- NoClip (FIXED)
    RunService.Stepped:Connect(function()
        if _G.Ysl.Movement.NoClip and _G.Ysl.Enabled then
            pcall(function()
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end)
        end
    end)
    
    -- Infinite Jump (FIXED)
    UserInputService.JumpRequest:Connect(function()
        if _G.Ysl.Movement.InfJump and _G.Ysl.Enabled and humanoid then
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                        PROTECTION SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    -- God Mode
    RunService.Heartbeat:Connect(function()
        if _G.Ysl.Protection.GodMode and _G.Ysl.Enabled and humanoid then
            if humanoid.Health < humanoid.MaxHealth then
                humanoid.Health = humanoid.MaxHealth
            end
        end
    end)
    
    -- Anti-Void
    task.spawn(function()
        while true do
            task.wait(0.5)
            if _G.Ysl.Protection.AntiVoid and _G.Ysl.Enabled and hrp then
                if hrp.Position.Y < -50 then
                    pcall(function()
                        local bases = Workspace:FindFirstChild("Bases")
                        if bases then
                            for _, base in pairs(bases:GetChildren()) do
                                local spawn = base:FindFirstChild("Spawn")
                                if spawn and spawn:IsA("BasePart") then
                                    hrp.CFrame = spawn.CFrame + Vector3.new(0, 5, 0)
                                    Notify("Anti-Void", "Teleported to safety!", 2)
                                    break
                                end
                            end
                        end
                    end)
                end
            end
        end
    end)
    
    -- Wave Immunity
    RunService.Heartbeat:Connect(function()
        if _G.Ysl.Protection.WaveImmunity and _G.Ysl.Enabled then
            pcall(function()
                -- Heal
                if humanoid and humanoid.Health < humanoid.MaxHealth then
                    humanoid.Health = humanoid.MaxHealth
                    _G.Ysl.Stats.WavesBlocked += 1
                end
                
                -- Disable harmful states
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming, false)
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
                
                -- Make waves non-collidable
                local activeTsunamis = Workspace:FindFirstChild("ActiveTsunamis")
                if activeTsunamis then
                    for _, tsunami in pairs(activeTsunamis:GetChildren()) do
                        for _, part in pairs(tsunami:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.CanCollide = false
                                part.CanTouch = false
                            end
                        end
                    end
                end
                
                -- Handle Lava
                local lava = Workspace:FindFirstChild("Lava")
                if lava then
                    for _, part in pairs(lava:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                            part.CanTouch = false
                        end
                    end
                end
            end)
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                        VIP BYPASS FUNCTIONS
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    local function RemoveVIPWalls()
        pcall(function()
            local vipWalls = Workspace:FindFirstChild("VIPWalls")
            if vipWalls then
                for _, wall in pairs(vipWalls:GetChildren()) do
                    if wall:IsA("BasePart") then
                        wall:Destroy()
                    end
                end
                Notify("VIP Bypass", "All VIP walls removed!", 2)
            end
        end)
    end
    
    local function TeleportPastVIP()
        pcall(function()
            local vipWalls = Workspace:FindFirstChild("VIPWalls")
            if vipWalls then
                local furthest = nil
                local furthestDist = 0
                
                for _, wall in pairs(vipWalls:GetChildren()) do
                    if wall:IsA("BasePart") then
                        local dist = (wall.Position - Vector3.zero).Magnitude
                        if dist > furthestDist then
                            furthestDist = dist
                            furthest = wall
                        end
                    end
                end
                
                if furthest then
                    hrp.CFrame = furthest.CFrame + furthest.CFrame.LookVector * 15 + Vector3.new(0, 5, 0)
                    Notify("VIP Bypass", "Teleported past VIP area!", 2)
                end
            end
        end)
    end
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                        ESP SYSTEM
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    local ESPObjects = {}
    
    local function CreateESP(obj, color, text)
        if ESPObjects[obj] then return end
        
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "ESP"
        billboard.AlwaysOnTop = true
        billboard.Size = UDim2.new(0, 100, 0, 50)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.Parent = obj
        
        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(1, 0, 1, 0)
        frame.BackgroundTransparency = 0.5
        frame.BackgroundColor3 = color
        frame.BorderSizePixel = 0
        frame.Parent = billboard
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 8)
        corner.Parent = frame
        
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.Text = text
        label.TextColor3 = Color3.new(1, 1, 1)
        label.TextScaled = true
        label.Font = Enum.Font.GothamBold
        label.TextStrokeTransparency = 0.5
        label.Parent = frame
        
        ESPObjects[obj] = billboard
    end
    
    local function RemoveESP(obj)
        if ESPObjects[obj] then
            ESPObjects[obj]:Destroy()
            ESPObjects[obj] = nil
        end
    end
    
    -- Brainrot ESP
    task.spawn(function()
        while true do
            task.wait(2)
            if _G.Ysl.ESP.Brainrots then
                pcall(function()
                    local activeBrainrots = Workspace:FindFirstChild("ActiveBrainrots")
                    if activeBrainrots then
                        for _, rarityFolder in pairs(activeBrainrots:GetChildren()) do
                            local color = Color3.fromRGB(255, 255, 255)
                            if rarityFolder.Name == "Legendary" then
                                color = Color3.fromRGB(255, 170, 0)
                            elseif rarityFolder.Name == "Epic" then
                                color = Color3.fromRGB(163, 53, 238)
                            elseif rarityFolder.Name == "Rare" then
                                color = Color3.fromRGB(0, 170, 255)
                            elseif rarityFolder.Name == "Cosmic" then
                                color = Color3.fromRGB(255, 0, 255)
                            elseif rarityFolder.Name == "Secret" then
                                color = Color3.fromRGB(255, 255, 0)
                            end
                            
                            for _, model in pairs(rarityFolder:GetChildren()) do
                                if model.Name == "RenderedBrainrot" then
                                    CreateESP(model, color, rarityFolder.Name)
                                end
                            end
                        end
                    end
                end)
            else
                for obj, _ in pairs(ESPObjects) do
                    if obj:IsDescendantOf(Workspace:FindFirstChild("ActiveBrainrots") or game) then
                        RemoveESP(obj)
                    end
                end
            end
        end
    end)
    
    -- Lucky Block ESP
    task.spawn(function()
        while true do
            task.wait(2)
            if _G.Ysl.ESP.LuckyBlocks then
                pcall(function()
                    local luckyBlocks = Workspace:FindFirstChild("ActiveLuckyBlocks")
                    if luckyBlocks then
                        for _, block in pairs(luckyBlocks:GetChildren()) do
                            CreateESP(block, Color3.fromRGB(255, 215, 0), "Lucky")
                        end
                    end
                end)
            else
                for obj, _ in pairs(ESPObjects) do
                    if obj:IsDescendantOf(Workspace:FindFirstChild("ActiveLuckyBlocks") or game) then
                        RemoveESP(obj)
                    end
                end
            end
        end
    end)
    
    -- VIP Wall ESP
    task.spawn(function()
        while true do
            task.wait(2)
            if _G.Ysl.ESP.VIPWalls then
                pcall(function()
                    local vipWalls = Workspace:FindFirstChild("VIPWalls")
                    if vipWalls then
                        for _, wall in pairs(vipWalls:GetChildren()) do
                            if wall:IsA("BasePart") then
                                CreateESP(wall, Color3.fromRGB(255, 0, 0), "VIP")
                            end
                        end
                    end
                end)
            else
                for obj, _ in pairs(ESPObjects) do
                    if obj:IsDescendantOf(Workspace:FindFirstChild("VIPWalls") or game) then
                        RemoveESP(obj)
                    end
                end
            end
        end
    end)
    
    -- Player ESP
    task.spawn(function()
        while true do
            task.wait(1)
            if _G.Ysl.ESP.Players then
                pcall(function()
                    for _, player in pairs(Players:GetPlayers()) do
                        if player ~= lplayer and player.Character then
                            local head = player.Character:FindFirstChild("Head")
                            if head then
                                CreateESP(head, Color3.fromRGB(0, 255, 0), player.Name)
                            end
                        end
                    end
                end)
            else
                for obj, _ in pairs(ESPObjects) do
                    for _, player in pairs(Players:GetPlayers()) do
                        if player.Character and obj:IsDescendantOf(player.Character) then
                            RemoveESP(obj)
                        end
                    end
                end
            end
        end
    end)
    
    --[[ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
                        TELEPORT FUNCTIONS
    â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ]]
    
    local teleportLocations = {
        Spawn = function()
            local bases = Workspace:FindFirstChild("Bases")
            if bases then
                for _, base in pairs(bases:GetChildren()) do
                    local spawn = base:FindFirstChild("Spawn")
                    if spawn then
                        return spawn.CFrame + Vector3.new(0, 5, 0)
                    end
                end
            end
        end,
        
        SellPoint = function()
            local sellPoint = Workspace:FindFirstChild("SellPoint")
            if sellPoint and sellPoint:FindFirstChild("Seller") then
                return sellPoint.Seller:GetPivot() + Vector3.new(0, 5, 0)
            end
        end,
        
        UpgradeShop = function()
            local shop = Workspace:FindFirstChild("UpgradeShop")
            if shop then
                return shop:GetPivot() + Vector3.new(0, 5, 0)
            end
        end,
        
        MoneyWheel = function()
            local wheel = Workspace:FindFirstChild("MoneySpinWheel")
            if wheel then
                return wheel:GetPivot() + Vector3.new(0, 5, 0)
            end
        end
    }
    
    local function TeleportTo(location)
        pcall(function()
            local func = teleportLocations[location]
            if func then
                local cframe = func()
                if cframe then
                    hrp.CFrame = cframe
                    Notify("Teleport", "Teleported to " .. location, 2)
                end
            end
        end)
    end
    
